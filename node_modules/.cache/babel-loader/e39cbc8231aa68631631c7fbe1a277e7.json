{"ast":null,"code":"import axios from 'axios';\nimport Header from '@/views/userLayout/Header/header.vue';\nimport { ElButton } from 'element-plus';\nexport default {\n  components: {\n    Header,\n    ElButton\n  },\n\n  data() {\n    return {\n      medicines: []\n    };\n  },\n\n  mounted() {\n    this.fetchMedicines();\n  },\n\n  methods: {\n    fetchMedicines() {\n      axios.get('http://localhost:8081/medicine/all').then(response => {\n        this.medicines = response.data.data;\n      }).catch(error => {\n        console.error('Error fetching medicines:', error);\n      });\n    },\n\n    async addToCart(medicine) {\n      try {\n        const params = new URLSearchParams();\n        params.append('introduction', this.updateForm.introduction);\n        params.append('telephone', this.updateForm.telephone);\n        params.append('doctorId', 13);\n        await axios.post('http://localhost:8081/cart/changeInfo', params);\n        alert('用户信息已更新');\n      } catch (error) {\n        console.error('Error updating doctor:', error);\n        alert('更新用户信息时出错');\n      }\n\n      console.log('Added to cart:', medicine);\n    },\n\n    // eslint-disable-next-line no-unused-vars\n    detail(medicine) {// Implement the detail method\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,sCAAnB;AACA,SAASC,QAAT,QAAyB,cAAzB;AAEA,eAAe;AACbC,YAAU,EAAE;AAAEF,UAAF;AAAUC;AAAV,GADC;;AAEbE,MAAI,GAAG;AACL,WAAO;AACLC,eAAS,EAAE;AADN,KAAP;AAGD,GANY;;AAObC,SAAO,GAAG;AACR,SAAKC,cAAL;AACD,GATY;;AAUbC,SAAO,EAAE;AACPD,kBAAc,GAAG;AACfP,WAAK,CAACS,GAAN,CAAU,oCAAV,EACGC,IADH,CACQC,QAAO,IAAK;AAChB,aAAKN,SAAL,GAAiBM,QAAQ,CAACP,IAAT,CAAcA,IAA/B;AACD,OAHH,EAIGQ,KAJH,CAISC,KAAI,IAAK;AACdC,eAAO,CAACD,KAAR,CAAc,2BAAd,EAA2CA,KAA3C;AACD,OANH;AAOD,KATM;;AAUP,UAAME,SAAN,CAAgBC,QAAhB,EAA0B;AACxB,UAAI;AACF,cAAMC,MAAK,GAAI,IAAIC,eAAJ,EAAf;AACAD,cAAM,CAACE,MAAP,CAAc,cAAd,EAA8B,KAAKC,UAAL,CAAgBC,YAA9C;AACAJ,cAAM,CAACE,MAAP,CAAc,WAAd,EAA2B,KAAKC,UAAL,CAAgBE,SAA