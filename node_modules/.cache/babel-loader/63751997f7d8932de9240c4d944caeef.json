120px\">\r\n      <el-form-item label=\"患者编号\">\r\n        <el-input v-model=\"info1.parentId\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"上任日期\">\r\n        <el-input v-model=\"info1.createTime\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"edit()\" v-model=\"dialogVisible\"\r\n          >确认</el-button\r\n        >\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n  \r\n</template>\r\n<script>\r\nimport { get } from '@/utils/request'\r\nimport { ElMessage } from 'element-plus'\r\nexport default {\r\n  data() {\r\n    return {\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      info: {}, //修改数据\r\n      info1: {}, //新增数据\r\n      tbData: []\r\n      // {\r\n      //   departmentName:'兰恒宇',\r\n      //   id:'209',\r\n      //   name2:'中医科',\r\n      //   id2:'1',\r\n      //   time:'2016-05-02',\r\n      //   level:'5'\r\n      // },\r\n      // {\r\n      //   name:'周青',\r\n      //   id:'307',\r\n      //   name2:'儿科',\r\n      //   id2:'2',\r\n      //   time:'2016-05-02',\r\n      //   level:'6'\r\n      // },\r\n      // {\r\n      //   name:'李小明',\r\n      //   id:'323',\r\n      //   name2:'外科',\r\n      //   id2:'3',\r\n      //   time:'2016-05-02',\r\n      //   level:'4'\r\n      // },\r\n      // {\r\n      //   name:'胡迭',\r\n      //   id:'406',\r\n      //   name2:'内科',\r\n      //   id2:'4',\r\n      //   time:'2016-05-02',\r\n      //   level:'5'\r\n      // },\r\n      // {\r\n      //   name:'杨琉',\r\n      //   id:'104',\r\n      //   name2:'妇科',\r\n      //   id2:'5',\r\n      //   time:'2016-05-02',\r\n      //   level:'9'\r\n      // },\r\n      // {\r\n      //   name:'叶言',\r\n      //   id:'808',\r\n      //   name2:'药剂科',\r\n      //   id2:'6',\r\n      //   time:'2016-05-02',\r\n      //   level:'3'\r\n      // },\r\n    }\r\n  },\r\n  //初始化工作\r\n  mounted() {\r\n    get('/Departments').then((res) => {\r\n      console.log(res)\r\n      this.tbData = res.data\r\n    })\r\n  },\r\n  methods: {\r\n    handleClick(row) {\r\n      this.info = row\r\n      this.dialogVisible = !this.dialogVisible\r\n    },\r\n    del(row) {\r\n      coms\r\n      get('/deleDepartment',{departmentId: row.row.departmentId}).then((res) => {\r\n        ElMessage({\r\n          message: '删除成功！',\r\n          type: 'success'\r\n        })\r\n      })\r\n      window.location.reload()//刷新\r\n    },\r\n    edit() {\r\n      ElMessage({\r\n        message: '修改成功！',\r\n        type: 'success'\r\n      })\r\n      this.dialogVisible = !this.dialogVisible\r\n    },\r\n    add() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n    },\r\n    insert() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n      get('/addDepartment').then((res) => {\r\n        console.log(res)\r\n        if (res.data == 1) {\r\n          ElMessage({\r\n            message: '增加成功',\r\n            type: 'success'\r\n          })\r\n          window.location.reload()\r\n        } else {\r\n          ElMessage({\r\n            message: '添加失败',\r\n            type: 'danger'\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}