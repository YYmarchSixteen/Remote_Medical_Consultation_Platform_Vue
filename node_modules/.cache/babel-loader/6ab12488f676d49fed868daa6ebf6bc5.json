{"ast":null,"code":"import { ref } from '@vue/reactivity';\nimport axios from 'axios'; // do not use same name with ref\n\nexport default {\n  name: 'LoginRegister',\n\n  data() {\n    return {\n      BASE_URL: 'http://localhost:8081/',\n      from: {\n        name: '',\n        password: ''\n      },\n      types: [],\n      value: '',\n      login: []\n    };\n  },\n\n  setup() {\n    const rules = ref({\n      name: [{\n        required: true,\n        message: '请填写账户',\n        trigger: 'blur'\n      }],\n      password: [{\n        required: true,\n        message: '请填写密码',\n        trigger: 'blur'\n      }]\n    });\n    return {\n      rules\n    };\n  },\n\n  mounted() {},\n\n  methods: {\n    reg() {\n      // 跳转到 /reg 页面\n      this.$router.push('/reg');\n    },\n\n    checkLogin(from) {\n      const params = new URLSearchParams();\n      params.append('username', from.name);\n      params.append('password', from.password);\n      axios.post(this.BASE_URL + 'doctor/login', params).then(res => {\n        if (res.data.status === '200') {\n          this.$message.success('登录成功,欢迎回来');\n        } else {\n          this.$message.error('用户名或密码错误，登录失败');\n        }\n      }).catch(err => {\n        console.error(err);\n        this.$message.error('登录失败，请稍后再试');\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4BA,SAASA,GAAT,QAAoB,iBAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB,C,CACA;;AAEA,eAAe;AACbC,MAAI,EAAE,eADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLC,cAAQ,EAAE,wBADL;AAELC,UAAI,EAAE;AACJH,YAAI,EAAE,EADF;AAEJI,gBAAQ,EAAE;AAFN,OAFD;AAMLC,WAAK,EAAE,EANF;AAOLC,WAAK,EAAE,EAPF;AAQLC,WAAK,EAAE;AARF,KAAP;AAUD,GAbY;;AAcbC,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIX,GAAG,CAAC;AAChBE,UAAI,EAAE,CACJ;AACEU,gBAAQ,EAAE,IADZ;AAEEC,eAAO,EAAE,OAFX;AAGEC,eAAO,EAAE;AAHX,OADI,CADU;AAQhBR,cAAQ,EAAE,CACR;AACEM,gBAAQ,EAAE,IADZ;AAEEC,eAAO,EAAE,OAFX;AAGEC,eAAO,EAAE;AAHX,OADQ;AARM,KAAD,CAAjB;AAiBA,WAAO;AAAEH;AAAF,KAAP;AACD,GAjCY;;AAkCbI,SAAO,GAAG,CACT,CAnCY;;AAoCbC,SAAO,EAAE;AACPC,OAAG,GAAG;AACJ;AACA,WAA