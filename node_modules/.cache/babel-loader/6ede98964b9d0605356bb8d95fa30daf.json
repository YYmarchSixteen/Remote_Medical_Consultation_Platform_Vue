{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      userList: [],\n      currentUserId: null,\n      currentUsername: '',\n      currentChat: [],\n      newMessage: ''\n    };\n  },\n\n  mounted() {\n    this.fetchUserList();\n  },\n\n  methods: {\n    fetchUserList() {\n      axios.get(`http://localhost:8081/chat/doctorChatList?doctorId=13`).then(response => {\n        this.userList = response.data.data;\n\n        if (this.userList.length > 0) {\n          // 如果用户列表不为空，则将第一个用户的用户名设置为当前用户名\n          this.currentUsername = this.userList[0].username;\n          this.currentUserId = this.userList[0].userId;\n          this.fetchChatContent(this.currentUserId);\n        }\n      }).catch(error => {\n        console.error('Error fetching user list:', error);\n      });\n    },\n\n    handleUserSelect(userId) {\n      this.currentUserId = userId;\n      this.fetchChatContent(userId);\n    },\n\n    fetchChatContent(userId) {\n      axios.get(`http://localhost:8081/chat/all?doctorId=13&userId=${userId}`).then(response => {\n        this.currentChat = response.data.data.chat;\n      }).catch(error => {\n        console.error('Error fetching chat content:', error);\n      });\n    },\n\n    sendMessage() {\n      if (this.newMessage.trim() !== '') {\n        // 发送消息逻辑\n        this.newMessage = '';\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6BE,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACfC,MAAI,GAAG;AACP,WAAO;AACPC,cAAQ,EAAE,EADH;AAEPC,mBAAa,EAAE,IAFR;AAGPC,qBAAe,EAAE,EAHV;AAIPC,iBAAW,EAAE,EAJN;AAKPC,gBAAU,EAAE;AALL,KAAP;AAOD,GATgB;;AAUfC,SAAO,GAAG;AACV,SAAKC,aAAL;AACD,GAZgB;;AAafC,SAAO,EAAE;AACTD,iBAAa,GAAG;AAChBR,WAAK,CAACU,GAAN,CAAW,uDAAX,EACCC,IADD,CACMC,QAAO,IAAK;AAClB,aAAKV,QAAL,GAAgBU,QAAQ,CAACX,IAAT,CAAcA,IAA9B;;AACA,YAAI,KAAKC,QAAL,CAAcW,MAAd,GAAuB,CAA3B,EAA8B;AAC9B;AACA,eAAKT,eAAL,GAAuB,KAAKF,QAAL,CAAc,CAAd,EAAiBY,QAAxC;AACA,eAAKX,aAAL,GAAqB,KAAKD,QAAL,CAAc,CAAd,EAAiBa,MAAtC;AACA,eAAKC,gBAAL,CAAsB,KAAKb,aAA