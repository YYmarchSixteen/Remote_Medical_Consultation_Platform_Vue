"content","addForm","mounted","fetcharticles","methods","loading","length","total","response","get","params","page","Math","ceil","pageSize","concat","error","console","disablearticle","article","put","articleId","$message","success","activatearticle","showDialog1","showDialog2","updatearticle","URLSearchParams","append","window","location","reload","addarticle","receiver","post"],"sourceRoot":"","sources":["D:\\Dev\\Front\\Remote_Medical_Consultation_Platform_Vue\\src\\views\\Doctor\\article.vue"],"sourcesContent":["<template>\r\n  <div class=\"header\">\r\n    <doctorHeader></doctorHeader>\r\n  </div>\r\n  <div class=\"container\" ref=\"scrollContainer\"  style=\"max-height: 800px; overflow-y: auto;\">\r\n    <el-button type=\"primary\" @click=\"showDialog1();\" size=\"small\">添加文章</el-button>\r\n    <br><br>\r\n    <el-table :data=\"articles\" border stripe>\r\n      <el-table-column prop=\"articleId\" label=\"文章ID\"></el-table-column>\r\n      <el-table-column prop=\"title\" label=\"标题\"></el-table-column>\r\n      <el-table-column prop=\"content\" label=\"内容\" width=\"200\"></el-table-column>\r\n      <el-table-column prop=\"sendTime\" label=\"发送时间\"></el-table-column>\r\n      <el-table-column label=\"操作\" width=\"200\">\r\n        <template #default=\"scope\">\r\n          <el-button v-if=\"scope.row.status === 1\" type=\"danger\" size=\"small\" @click=\"disablearticle(scope.row)\">\r\n            点击禁用\r\n          </el-button>\r\n          <el-button v-else-if=\"scope.row.status === 0\" type=\"success\" size=\"small\" @click=\"activatearticle(scope.row)\">\r\n            激活文章\r\n          </el-button>\r\n          <el-button type=\"primary\" @click=\"showDialog2();updateForm=scope.row\" size=\"small\">编辑文章</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-dialog title=\"编辑文章\" v-model=\"dialogVisible2\">\r\n      <el-form ref=\"updateForm\" :model=\"updateForm\" label-width=\"100px\">\r\n        <el-form-item label=\"标题\" prop=\"title\">\r\n          <el-input v-model=\"updateForm.title\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"文章内容\" prop=\"content\">\r\n          <el-input v-model=\"updateForm.content\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span id=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible2 = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"updatearticle\">添加</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <el-dialog title=\"新增文章\" v-model=\"dialogVisible1\">\r\n      <el-form ref=\"addForm\" :model=\"addForm\" label-width=\"100px\">\r\n        <el-form-item label=\"标题\" prop=\"title\">\r\n          <el-input v-model=\"addForm.title\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"文章内容\" prop=\"content\">\r\n          <el-input v-model=\"addForm.content\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"接收者\" prop=\"receiver\">\r\n          <el-select v-model=\"addForm.receiver\" placeholder=\"请选择接收者\">\r\n            <el-option label=\"用户\" value=\"1\"></el-option>\r\n            <el-option label=\"医生\" value=\"2\"></el-option>\r\n            <el-option label=\"医生和用户\" value=\"3\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span id=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible1 = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"addarticle\">添加</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport { ElDialog } from 'element-plus'\r\nimport doctorHeader from '@/views/doctorLayout/doctorHeader.vue'\r\n\r\nexport default {\r\n  components: { doctorHeader, ElDialog },\r\n  data() {\r\n    return {\r\n      articles: [],\r\n      dialogVisible1: false,\r\n      dialogVisible2: false,\r\n      updateForm: {\r\n        title: '',\r\n        content: ''\r\n      },\r\n      addForm: {\r\n        title: '',\r\n        content: ''\r\n      },\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetcharticles()\r\n  },\r\n  methods: {\r\n    async fetcharticles() {\r\n      try {\r\n        // 如果正在加载数据或已加载完所有数据，则不再请求\r\n        if (this.loading || this.articles.length >= this.total) {\r\n          return\r\n        }\r\n        this.loading = true\r\n        const response = await axios.get('http://localhost:8081/article/all', {\r\n          params: {\r\n            page: Math.ceil(this.articles.length / this.pageSize) + 1,\r\n            pageSize: this.pageSize\r\n          }\r\n        })\r\n        this.articles = this.articles.concat(response.data.data)\r\n        this.loading = false\r\n      } catch (error) {\r\n        console.error('Error fetching articles:', error)\r\n      }\r\n    },\r\n    async disablearticle(article) {\r\n      try {\r\n        await axios.put(`http://localhost:8081/article/disabled?articleId=${article.articleId}`)\r\n        this.$message.success('当前文章已禁用')\r\n        this.fetcharticles()\r\n      } catch (error) {\r\n        console.error('Error disabling article:', error)\r\n      }\r\n    },\r\n    async activatearticle(article) {\r\n      try {\r\n        await axios.put(`http://localhost:8081/article/activate?articleId=${article.articleId}`)\r\n        this.$message.success('当前文章已激活')\r\n        this.fetcharticles()\r\n      } catch (error) {\r\n        console.error('Error activating article:', error)\r\n      }\r\n    },\r\n    showDialog1() {\r\n      this.dialogVisible1 = true\r\n    },\r\n    showDialog2() {\r\n      this.dialogVisible2 = true\r\n    },\r\n    async updatearticle() {\r\n      try {\r\n        const params = new URLSearchParams()\r\n        params.append('title', this.updateForm.title)\r\n        params.append('content', this.updateForm.content)\r\n        params.append('articleId', this.updateForm.articleId)\r\n        axios.put('http://localhost:8081/article/update', params)\r\n        this.$message.success('编辑成功')\r\n        this.dialogVisible2 = false\r\n        await this.fetcharticles()\r\n        window.location.reload()\r\n      } catch (error) {\r\n        console.error('Error adding article:', error)\r\n      }\r\n    },\r\n    async addarticle() {\r\n      try {\r\n        const params = new URLSearchParams()\r\n        params.append('title', this.addForm.title)\r\n        params.append('content', this.addForm.content)\r\n        params.append('receiver', this.addForm.receiver)\r\n        axios.post('http://localhost:8081/article/add', params)\r\n        this.$message.success('新增成功')\r\n        this.dialogVisible1 = false\r\n        await this.fetcharticles()\r\n      } catch (error) {\r\n        console.error('Error adding article:', error)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  margin: 120px auto;\r\n  max-width: 1000px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}