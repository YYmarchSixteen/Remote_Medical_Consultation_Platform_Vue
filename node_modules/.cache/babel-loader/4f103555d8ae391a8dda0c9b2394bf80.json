{"ast":null,"code":"import { ref, reactive, onMounted } from 'vue';\nimport axios from 'axios';\nexport default {\n  name: 'Header',\n\n  setup() {\n    const searchTerm = ref('');\n    const equipmentTypes = reactive([]);\n\n    const handleSearch = () => {\n      const searchUrl = `http://localhost:8080/search?equipmentname=${searchTerm.value}`;\n      window.location.href = searchUrl;\n    };\n\n    const fetchEquipmentTypes = async () => {\n      try {\n        const res = await axios.get('http://localhost:8080/equipmenttypeList');\n        equipmentTypes.push(...res.data);\n      } catch (error) {\n        console.log(error);\n      }\n    };\n\n    onMounted(() => {\n      fetchEquipmentTypes();\n    });\n    return {\n      searchTerm,\n      equipmentTypes,\n      handleSearch\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAmCA,SAASA,GAAT,EAAcC,QAAd,EAAwBC,SAAxB,QAAyC,KAAzC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,MAAI,EAAE,QADK;;AAEXC,OAAK,GAAG;AACJ,UAAMC,UAAS,GAAIN,GAAG,CAAC,EAAD,CAAtB;AACA,UAAMO,cAAa,GAAIN,QAAQ,CAAC,EAAD,CAA/B;;AACA,UAAMO,YAAW,GAAI,MAAM;AACvB,YAAMC,SAAQ,GAAK,8CAA6CH,UAAU,CAACI,KAAM,EAAjF;AACAC,YAAM,CAACC,QAAP,CAAgBC,IAAhB,GAAuBJ,SAAvB;AACH,KAHD;;AAKA,UAAMK,mBAAkB,GAAI,YAAY;AACpC,UAAI;AACA,cAAMC,GAAE,GAAI,MAAMZ,KAAK,CAACa,GAAN,CAAU,yCAAV,CAAlB;AACAT,sBAAc,CAACU,IAAf,CAAoB,GAAGF,GAAG,CAACG,IAA3B;AACJ,OAHA,CAGE,OAAOC,KAAP,EAAc;AACZC,eAAO,CAACC,GAAR,CAAYF,KAAZ;AACJ;AACJ,KAPA;;AASAjB,aAAS,CAAC,MAAM;AACZY,yBAAmB;AACtB,KAFQ,CAAT;AAIA,WAAO;AACHR,gBADG;AAEHC,oBAFG;AAGHC;AAHG,KAAP;AAKH;;AA5BU,CAAf","names":["ref","reactive","onMounted","axios","name","setup","searchTerm","equipmentTypes","handleSearch","searchUrl","value","window","location","href","fetchEquipmentTypes","res","get","push","data","error","console","log"],"sourceRoot":"","sources":["D:\\Dev\\Front\\demo01\\src\\views\\userLayout\\Search\\search.vue"],"sourcesContent":["<template>\r\n    <div class=\"header\">\r\n        <el-header class=\"header-wrapper\" height=\"70px\">\r\n            <div class=\"header-logo\">\r\n                <img src=\"../../../assets/OIP-C.jpg\" alt=\"logo\" class=\"logo-img\" />\r\n                <span class=\"logo-text\">健康守护者</span>\r\n            </div>\r\n            <div class=\"header-nav\">\r\n                <el-menu :default-active=\"$route.path\" mode=\"horizontal\" router>\r\n                    <el-menu-item index=\"/\">首页</el-menu-item>\r\n                    <el-submenu index=\"/products\">\r\n                        <template #title>产品中心</template>\r\n                        <el-menu-item index=\"/\">我的收藏</el-menu-item>\r\n                        <el-menu-item index=\"/\">个人中心</el-menu-item>\r\n                    </el-submenu>\r\n                    <!-- <el-menu-item index=\"/\">关于我们</el-menu-item>\r\n                    <el-menu-item index=\"/\">联系我们</el-menu-item> -->\r\n                    <!-- 循环渲染设备类型列表 -->\r\n                    <el-menu-item-group v-if=\"equipmentTypes.length\">\r\n                        <template #title>设备类型</template>\r\n                        <div v-for=\"(type, index) in equipmentTypes\" :key=\"index\">\r\n                            <el-menu-item :key=\"index\" :index=\"`/products/${type}`\">{{ type.typename }}</el-menu-item>\r\n                        </div>\r\n                    </el-menu-item-group>\r\n                </el-menu>\r\n            </div>\r\n            <div class=\"header-search\">\r\n                <input type=\"text\" v-model=\"searchTerm\" class=\"search-input\" placeholder=\"请输入搜索内容\" />\r\n                <button class=\"search-btn\" @click=\"handleSearch\">搜索</button>\r\n            </div>\r\n        </el-header>\r\n    </div>\r\n</template>\r\n  \r\n<script>\r\nimport { ref, reactive, onMounted } from 'vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    name: 'Header',\r\n    setup() {\r\n        const searchTerm = ref('');\r\n        const equipmentTypes = reactive([]);\r\n        const handleSearch = () => {\r\n            const searchUrl = `http://localhost:8080/search?equipmentname=${searchTerm.value}`;\r\n            window.location.href = searchUrl;\r\n        };\r\n\r\n        const fetchEquipmentTypes = async () => {\r\n            try {\r\n                const res = await axios.get('http://localhost:8080/equipmenttypeList');\r\n                equipmentTypes.push(...res.data);\r\n            } catch (error) {\r\n                console.log(error);\r\n            }\r\n        }\r\n\r\n        onMounted(() => {\r\n            fetchEquipmentTypes();\r\n        });\r\n\r\n        return {\r\n            searchTerm,\r\n            equipmentTypes,\r\n            handleSearch,\r\n        };\r\n    },\r\n};\r\n</script>\r\n  \r\n  \r\n<style scoped>\r\n.header {\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    right: 0;\r\n    z-index: 999;\r\n}\r\n\r\n.header-wrapper {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    background-color: #fff;\r\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.header-logo {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-left: 20px;\r\n}\r\n\r\n.logo-img {\r\n    height: 40px;\r\n}\r\n\r\n.logo-text {\r\n    font-size: 20px;\r\n    font-weight: bold;\r\n    margin-left: 10px;\r\n}\r\n\r\n.header-nav {\r\n    margin-right: 120px;\r\n}\r\n\r\n.header-search {\r\n    margin-right: 20px;\r\n}\r\n\r\n.search-input {\r\n    border: 2px solid #7ec8e3;\r\n    border-radius: 20px;\r\n    padding: 6px 12px;\r\n    font-size: 14px;\r\n    color: #333;\r\n}\r\n\r\n.search-btn {\r\n    border: none;\r\n    border-radius: 20px;\r\n    padding: 6px 16px;\r\n    font-size: 14px;\r\n    color: #fff;\r\n    background-color: #b9d76b;\r\n    cursor: pointer;\r\n}\r\n\r\nbody {\r\n    font-family: 'Microsoft YaHei';\r\n    color: #666;\r\n    font-size: 12px;\r\n}\r\n</style>\r\n  "]},"metadata":{},"sourceType":"module"}