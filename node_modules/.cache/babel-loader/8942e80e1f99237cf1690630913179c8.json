{"ast":null,"code":"import { get } from '@/utils/request';\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 15,\n      //默认一页显示 条\n      dialogVisible: false,\n      dialogVisible1: false,\n      info2: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      list: [],\n      adg: [],\n      editForm: {\n        chairImg: '' // 其他表单字段\n\n      }\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    get('http://localhost:8080/equipmentList').then(res => {\n      console.log(res);\n      this.list = res.data;\n    });\n  },\n\n  methods: {\n    handleAvatarSuccess(res) {\n      // 将图片名赋值给表单字段\n      this.editForm.chairImg = res;\n    },\n\n    // 限制上传图片格式和大小\n    beforeAvatarUpload(file) {\n      const img_mimetype = ['image/jpeg', 'image/jpg', 'image/png'];\n      const isJPG = img_mimetype.includes(file.type);\n      const isLt2M = file.size / 1024 / 1024 < 2;\n\n      if (!isJPG) {\n        this.$message.error('上传头像只能是图片格式!');\n        return false;\n      }\n\n      if (!isLt2M) {\n        this.$message.error('上传头像图片大小不能超过 2MB!');\n        return false;\n      }\n\n      return isJPG && isLt2M;\n    },\n\n    // 提交修改的表单数据\n    async editChairmans() {}\n\n  },\n\n  del(data) {\n    console.log(data.row.id);\n    get('http://localhost:8080/deleteEquipment', {\n      id: data.row.id\n    }).then(() => {\n      ElMessage({\n        message: '删除成功！',\n        type: 'success'\n      });\n    });\n    window.location.reload(); //刷新\n  },\n\n  add() {\n    this.dialogVisible1 = !this.dialogVisible1;\n  },\n\n  insert() {\n    this.dialogVisible1 = !this.dialogVisible1;\n    get('http://localhost:8080/insertEquipment', this.info1).then(res => {\n      console.log(res);\n\n      if (res.data == '添加成功') {\n        ElMessage({\n          message: '添加成功',\n          type: 'success'\n        });\n        window.location.reload();\n      } else {\n        ElMessage({\n          message: '添加失败',\n          type: 'danger'\n        });\n      }\n    });\n  },\n\n  handleClick(data) {\n    this.info2 = data.row;\n    this.dialogVisible = !this.dialogVisible;\n  },\n\n  edit() {\n    get('http://localhost:8080/updateEquipment', this.info2).then(res => {\n      console.log(res);\n\n      if (res.data == '修改成功') {\n        ElMessage({\n          message: '修改成功',\n          type: 'success'\n        });\n        window.location.reload();\n      } else {\n        ElMessage({\n          message: '修改失败',\n          type: 'danger'\n        });\n      }\n    });\n  },\n\n  handleSizeChange: function (size) {\n    //一页显示多少条\n    this.pagesize = size;\n  },\n  handleCurrentChange: function (currentPage) {\n    //页码更改方法\n    this.currentPage = currentPage;\n  }\n};","map":{"version":3,"mappings":"AA8IA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,cAA1B;AAEA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,CADR;AACW;AAChBC,cAAQ,EAAE,EAFL;AAES;AACdC,mBAAa,EAAE,KAHV;AAILC,oBAAc,EAAE,KAJX;AAKLC,WAAK,EAAE,EALF;AAKM;AACXC,WAAK,EAAE,EANF;AAMM;AACXC,UAAI,EAAE,EAPD;AAQLC,SAAG,EAAE,EARA;AASLC,cAAQ,EAAE;AACRC,gBAAQ,EAAE,EADF,CAER;;AAFQ;AATL,KAAP;AAcD,GAhBY;;AAiBb;AACAC,SAAO,GAAG;AACRb,OAAG,CAAC,qCAAD,CAAH,CAA2Cc,IAA3C,CAAiDC,GAAD,IAAS;AACvDC,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,WAAKN,IAAL,GAAYM,GAAG,CAACb,IAAhB;AACD,KAHD;AAID,GAvBY;;AAwBbgB,SAAO,EAAE;AACPC,uBAAmB,CAACJ,GAAD,EAAM;AACvB;AACA,WAAKJ,QAAL,CAAcC,QAAd,GAAyBG,GAAzB;AACD,KAJM;;AAKP;AACAK,sBAAkB,CAACC,IAAD,EAAO;AACvB,YAAMC,YAAW,GAAI,CAAC,YAAD,EAAe,WAAf,EAA4B,WAA5B,CAArB;AACA,YAAMC,KAAI,GAAID,YAAY,CAACE,QAAb,CAAsBH,IAAI,CAACI,IAA3B,CAAd;AACA,YAAMC,MAAK,GAAIL,IAAI,CAACM,IAAL,GAAY,IAAZ,GAAmB,IAAnB,GAA0B,CAAzC;;AACA,UAAI,CAACJ,KAAL,EAAY;AACV,aAAKK,QAAL,CAAcC,KAAd,CAAoB,cAApB;AACA,eAAO,KAAP;AACF;;AACA,UAAI,CAACH,MAAL,EAAa;AACX,aAAKE,QAAL,CAAcC,KAAd,CAAoB,mBAApB;AACA,eAAO,KAAP;AACF;;AACA,aAAON,KAAI,IAAKG,MAAhB;AACD,KAnBM;;AAoBP;AACA,UAAMI,aAAN,GAAsB,CACtB;;AAtBO,GAxBI;;AAgDXC,KAAG,CAAC7B,IAAD,EAAO;AACRc,WAAO,CAACC,GAAR,CAAYf,IAAI,CAAC8