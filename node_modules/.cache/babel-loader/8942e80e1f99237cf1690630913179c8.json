\\equipment.vue"],"sourcesContent":["<template>\r\n  <el-card class=\"box-card\">\r\n    \r\n    <el-button type=\"primary\" @click=\"add(props)\" class=\"box-card1\">添加器械</el-button>\r\n    <!-- <img src=\"../../assets/img证件照.png\" alt=\"\"> -->\r\n    <el-table :data=\"list.slice((currentPage - 1) * pagesize, currentPage * pagesize)\" style=\"width: 100%\">\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"200\" />\r\n      <el-table-column prop=\"equipmentname\" label=\"器械名\" width=\"150\" />\r\n      <el-table-column prop=\"feature\" label=\"特点\" width=\"150\" />\r\n      <el-table-column prop=\"material\" label=\"材质成分\" width=\"150\" />\r\n      <el-table-column prop=\"price\" label=\"价格\" width=\"150\" />\r\n      <el-table-column prop=\"type\" label=\"所属类型\" width=\"150\" />\r\n      <el-table-column prop=\"picture\" label=\"图片\" width=\"300\" >\r\n        <template #default=\"scope\">\r\n          <el-image style=\"width: 70px; height: 70px\" :src=\"scope.row.picture || imageUrl\" alt=\"\" :fit=\"fill\" ></el-image>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column fixed=\"right\" label=\"操作\" width=\"240\">\r\n        <template #default=\"row\">\r\n          <el-button type=\"success\" @click=\"handleClick(row)\">编辑</el-button>\r\n          <el-button type=\"danger\" @click=\"del(row)\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 添加框 -->\r\n    <el-dialog v-model=\"dialogVisible1\" title=\"增加\" width=\"30%\">\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"器械名\">\r\n          <el-input v-model=\"info1.equipmentname\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"特点\">\r\n          <el-input v-model=\"info1.feature\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"材质成分\">\r\n          <el-input v-model=\"info1.material\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"价格\">\r\n          <el-input v-model=\"info1.price\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"所属类型\">\r\n          <el-input v-model=\"info1.type\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"图片\">\r\n          <el-upload\r\n            class=\"avatar-uploader\"\r\n            action=\"http://localhost:8080/uploadEquipmentImg\"\r\n            :show-file-list=\"false\"\r\n            :on-success=\"handleAvatarSuccess('add')\"\r\n            :before-upload=\"beforeAvatarUpload\">\r\n            <img v-if=\"info1.picture || imageUrl\" :src=\"info1.picture || imageUrl\" class=\"avatar\">\r\n            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n          </el-upload>\r\n        </el-form-item>\r\n      </el-form>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible1 = false\">取消</el-button>\r\n          <el-button type=\"primary\" @click=\"insert()\" v-model=\"dialogVisible1\">确认</el-button>\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n\r\n    <!-- 修改框 -->\r\n    <el-dialog v-model=\"dialogVisible\" title=\"编辑\" width=\"30%\">\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"器械名\">\r\n          <el-input v-model=\"info2.equipmentname\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"特点\">\r\n          <el-input v-model=\"info2.feature\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"材质成分\">\r\n          <el-input v-model=\"info2.material\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"价格\">\r\n          <el-input v-model=\"info2.price\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"所属类型\">\r\n          <el-input v-model=\"info2.type\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"图片\">\r\n          <el-form :model=\"editForm\" :rules=\"editFormRules\" ref=\"editFormRef\" label-width=\"80px\">\r\n\t\t</el-form>\r\n        <el-form-item label=\"图片\" prop=\"chairImg\">\r\n          <el-upload\r\n            class=\"avatar-uploader\"\r\n            action=\"http://localhost:8080/uploadimg\"\r\n            :show-file-list=\"false\"\r\n            multiple\r\n            :limit=\"1\"\r\n            :on-success=\"handleAvatarSuccess\"\r\n            :before-upload=\"beforeAvatarUpload\">\r\n            <!-- 构建虚拟路径 -->\r\n            <img v-if=\"editForm.chairImg\" :src=\" 'http://localhost:8080/api/file/' + editForm.chairImg\" class=\"avatar\">\r\n            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n          </el-upload>\r\n        </el-form-item>\r\n  </el-form-item>\r\n      </el-form>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n          <el-button type=\"primary\" @click=\"edit()\" v-model=\"dialogVisible\">确认</el-button>\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n\r\n    <el-pagination\r\n      currentPage=\"currentPage\"\r\n      :page-sizes=\"[10]\"\r\n      page-size=\"10\"\r\n      :background=\"background\"\r\n      layout=\"total, sizes, prev, pager, next, jumper\"\r\n      :total=\"list.length\"\r\n      @size-change=\"handleSizeChange\"\r\n      @current-change=\"handleCurrentChange\"\r\n    />\r\n  </el-card>\r\n</template>\r\n\r\n<script>\r\nimport { get } from '@/utils/request'\r\nimport { ElMessage } from 'element-plus'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentPage: 1, //默认页码为1\r\n      pagesize: 15, //默认一页显示 条\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      info2: {}, //修改数据\r\n      info1: {}, //新增数据\r\n      list: [],\r\n      adg: [],\r\n      editForm: {\r\n        chairImg: '',\r\n        // 其他表单字段\r\n      }\r\n    }\r\n  },\r\n  //初始化工作\r\n  mounted() {\r\n    get('http://localhost:8080/equipmentList').then((res) => {\r\n      console.log(res)\r\n      this.list = res.data\r\n    })\r\n  },\r\n  methods: {\r\n    handleAvatarSuccess(res) {\r\n      // 将图片名赋值给表单字段\r\n      this.editForm.chairImg = res;\r\n    },\r\n    // 限制上传图片格式和大小\r\n    beforeAvatarUpload(file) {\r\n      const img_mimetype = ['image/jpeg', 'image/jpg', 'image/png'];\r\n      const isJPG = img_mimetype.includes(file.type);\r\n      const isLt2M = file.size / 1024 / 1024 < 2;\r\n      if (!isJPG) {\r\n        this.$message.error('上传头像只能是图片格式!');\r\n        return false;\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error('上传头像图片大小不能超过 2MB!');\r\n        return false;\r\n      }\r\n      return isJPG && isLt2M;\r\n    },\r\n    // 提交修改的表单数据\r\n    async editChairmans() {\r\n    }\r\n  },\r\n    del(data) {\r\n      console.log(data.row.id)\r\n      get('http://localhost:8080/deleteEquipment', { id: data.row.id }).then(() => {\r\n        ElMessage({\r\n          message: '删除成功！',\r\n          type: 'success'\r\n        })\r\n      })\r\n\r\n      window.location.reload() //刷新\r\n    },\r\n    add() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n    },\r\n    insert() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n      get('http://localhost:8080/insertEquipment', this.info1).then((res) => {\r\n        console.log(res)\r\n        if (res.data == '添加成功') {\r\n          ElMessage({\r\n            message: '添加成功',\r\n            type: 'success'\r\n          })\r\n          window.location.reload()\r\n        } else {\r\n          ElMessage({\r\n            message: '添加失败',\r\n            type: 'danger'\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleClick(data) {\r\n      this.info2 = data.row\r\n      this.dialogVisible = !this.dialogVisible\r\n    },\r\n    edit() {\r\n      get('http://localhost:8080/updateEquipment', this.info2).then((res) => {\r\n        console.log(res)\r\n        if (res.data == '修改成功') {\r\n          ElMessage({\r\n            message: '修改成功',\r\n            type: 'success'\r\n          })\r\n          window.location.reload()\r\n        } else {\r\n          ElMessage({\r\n            message: '修改失败',\r\n            type: 'danger'\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleSizeChange: function (size) {\r\n      //一页显示多少条\r\n      this.pagesize = size\r\n    },\r\n    handleCurrentChange: function (currentPage) {\r\n      //页码更改方法\r\n      this.currentPage = currentPage\r\n    },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.box-card {\r\npadding: 10px;\r\nmargin-bottom: 20px;\r\nbox-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n}\r\n.box-card1 {\r\nfloat: right;\r\nmargin-top: 15px;\r\nmargin-right: 20px;\r\n}\r\n.el-pagination {\r\nmargin-top: 20px;\r\nfloat: right;\r\n}\r\n.avatar-uploader {\r\ndisplay: inline-block;\r\nwidth: 70px;\r\nheight: 70px;\r\nborder-radius: 50%;\r\nborder: 1px dashed #d9d9d9;\r\noverflow: hidden;\r\nbackground: #fff;\r\n}\r\n.avatar {\r\nwidth: 100%;\r\nheight: 100%;\r\nobject-fit: cover;\r\n}\r\n.avatar-uploader-icon {\r\nfont-size: 28px;\r\ncolor: #8c939d;\r\nwidth: 70px;\r\nheight: 70px;\r\nline-height: 70px;\r\ntext-align: center;\r\n}\r\n.dialog-footer {\r\npadding: 10px 20px;\r\ntext-align: right;\r\nbackground-color: #fff;\r\nborder-top: 1px solid #ebeef5;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}