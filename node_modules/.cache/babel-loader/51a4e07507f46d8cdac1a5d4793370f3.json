{"ast":null,"code":"import { ref, reactive, onMounted } from 'vue';\nimport axios from 'axios';\nimport { useRouter } from 'vue-router';\nexport default {\n  name: 'Header',\n\n  setup() {\n    const searchTerm = ref('');\n    const equipmentTypes = reactive([]);\n    const isShowText = ref(false);\n    const router = useRouter();\n\n    const handleSearch = () => {\n      const searchUrl = `http://localhost:8080/search?equipmentname=${searchTerm.value}`;\n      window.location.href = searchUrl;\n    };\n\n    const fetchEquipmentTypes = async () => {\n      try {\n        const res = await axios.get('http://localhost:8080/equipmenttypeList');\n        equipmentTypes.push(...res.data);\n      } catch (error) {\n        console.log(error);\n      }\n    };\n\n    const handleLogoClick = () => {\n      router.push('/login');\n    };\n\n    onMounted(() => {\n      fetchEquipmentTypes();\n    });\n    return {\n      searchTerm,\n      equipmentTypes,\n      isShowText,\n      handleSearch,\n      handleLogoClick\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAmDA,SAASA,GAAT,EAAcC,QAAd,EAAwBC,SAAxB,QAAyC,KAAzC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAASC,SAAT,QAA0B,YAA1B;AAEA,eAAe;AACbC,MAAI,EAAE,QADO;;AAEbC,OAAK,GAAG;AACN,UAAMC,UAAS,GAAIP,GAAG,CAAC,EAAD,CAAtB;AACA,UAAMQ,cAAa,GAAIP,QAAQ,CAAC,EAAD,CAA/B;AACA,UAAMQ,UAAS,GAAIT,GAAG,CAAC,KAAD,CAAtB;AACA,UAAMU,MAAK,GAAIN,SAAS,EAAxB;;AAEA,UAAMO,YAAW,GAAI,MAAM;AACzB,YAAMC,SAAQ,GAAK,8CAA6CL,UAAU,CAACM,KAAM,EAAjF;AACAC,YAAM,CAACC,QAAP,CAAgBC,IAAhB,GAAuBJ,SAAvB;AACD,KAHD;;AAKA,UAAMK,mBAAkB,GAAI,YAAY;AACtC,UAAI;AACF,cAAMC,GAAE,GAAI,MAAMf,KAAK,CAACgB,GAAN,CAAU,yCAAV,CAAlB;AACAX,sBAAc,CAACY,IAAf,CAAoB,GAAGF,GAAG,CAACG,IAA3B;AACF,OAHA,CAGE,OAAOC,KAAP,EAAc;AACdC,eAAO,CAACC,GAAR,CAAYF,KAAZ;AACF;AACD,KAPD;;AASA,UAAMG,eAAc,GAAI,MAAM;AAC5Bf,YAAM,CAACU,IAAP,CAAY,QAAZ;AACD,KAFD;;AAIAlB,aAAS,CAAC,MAAM;AACde,yBAAmB;AACpB,KAFQ,CAAT;AAIA,WAAO;AACLV,gBADK;AAELC,oBAFK;AAGLC,gBAHK;AAILE,kBAJK;AAKLc;AALK,KAAP;AAOD;;AArCY,CAAf","names":["ref","reactive","onMounted","axios","useRouter","name","setup","searchTerm","equipmentTypes","isShowText","router","handleSearch","searchUrl","value","window","location","href","fetchEquipmentTypes","res","get","push","data","error","console","log","handleLogoClick"],"sourceRoot":"","sources":["D:\\Dev\\Front\\demo01\\src\\views\\userLayout\\Header\\header.vue"],"sourcesContent":["<template>\r\n  <div class=\"header\">\r\n    <el-header class=\"header-wrapper\" height=\"70px\">\r\n      <div\r\n        class=\"header-logo\"\r\n        @mouseenter=\"isShowText=true\"\r\n        @mouseleave=\"isShowText=false\"\r\n      >\r\n        <img\r\n          src=\"../../../assets/OIP-C.jpg\"\r\n          alt=\"logo\"\r\n          class=\"logo-img\"\r\n          @click=\"handleLogoClick\"\r\n        />\r\n        <span class=\"logo-text\" v-show=\"isShowText\">返回登录</span>\r\n      </div>\r\n      <div class=\"header-nav\">\r\n        <el-menu :default-active=\"$route.path\" mode=\"horizontal\" router>\r\n          <el-menu-item index=\"/userIndex\">首页</el-menu-item>\r\n          <el-submenu index=\"/products\">\r\n            <template #title>产品中心</template>\r\n            <el-menu-item index=\"/myCollect\">我的收藏</el-menu-item>\r\n            <el-menu-item index=\"/personal\">个人中心</el-menu-item>\r\n          </el-submenu>\r\n          <!-- 将设备类型列表的标题改为“全部设备” -->\r\n          <el-menu-item index=\"/products\">全部设备</el-menu-item>\r\n          <!-- 循环渲染设备类型列表 -->\r\n          <el-menu-item-group>\r\n            <div v-for=\"(type, index) in equipmentTypes\" :key=\"index\">\r\n              <el-menu-item\r\n                :key=\"index\"\r\n                :index=\"`/products/${type}`\"\r\n              >{{ type.typename }}</el-menu-item>\r\n            </div>\r\n          </el-menu-item-group>\r\n        </el-menu>\r\n      </div>\r\n      <div class=\"header-search\">\r\n        <input\r\n          type=\"text\"\r\n          v-model=\"searchTerm\"\r\n          class=\"search-input\"\r\n          placeholder=\"请输入搜索内容\"\r\n        />\r\n        <button class=\"search-btn\" @click=\"handleSearch\">搜索</button>\r\n      </div>\r\n    </el-header>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, reactive, onMounted } from 'vue';\r\nimport axios from 'axios';\r\nimport { useRouter } from 'vue-router';\r\n\r\nexport default {\r\n  name: 'Header',\r\n  setup() {\r\n    const searchTerm = ref('');\r\n    const equipmentTypes = reactive([]);\r\n    const isShowText = ref(false);\r\n    const router = useRouter();\r\n\r\n    const handleSearch = () => {\r\n      const searchUrl = `http://localhost:8080/search?equipmentname=${searchTerm.value}`;\r\n      window.location.href = searchUrl;\r\n    };\r\n\r\n    const fetchEquipmentTypes = async () => {\r\n      try {\r\n        const res = await axios.get('http://localhost:8080/equipmenttypeList');\r\n        equipmentTypes.push(...res.data);\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    };\r\n\r\n    const handleLogoClick = () => {\r\n      router.push('/login');\r\n    };\r\n\r\n    onMounted(() => {\r\n      fetchEquipmentTypes();\r\n    });\r\n\r\n    return {\r\n      searchTerm,\r\n      equipmentTypes,\r\n      isShowText,\r\n      handleSearch,\r\n      handleLogoClick,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n  \r\n  \r\n  <style scoped>\r\n  .header {\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    right: 0;\r\n    z-index: 999;\r\n  }\r\n  \r\n  .header-wrapper {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    background-color: #fff;\r\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n  }\r\n  \r\n  .header-logo {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-left: 20px;\r\n  }\r\n  \r\n  .logo-img {\r\n    height: 40px;\r\n  }\r\n  \r\n  .logo-text {\r\n    font-size: 20px;\r\n    font-weight: bold;\r\n    margin-left: 10px;\r\n  }\r\n  \r\n  .header-nav {\r\n    margin-right: 120px;\r\n  }\r\n  \r\n  .header-search {\r\n    margin-right: 20px;\r\n  }\r\n  \r\n  .search-input {\r\n    border: 2px solid #7ec8e3;\r\n    border-radius: 20px;\r\n    padding: 6px 12px;\r\n    font-size: 14px;\r\n    color: #333;\r\n  }\r\n  \r\n  .search-btn {\r\n    border: none;\r\n    border-radius: 20px;\r\n    padding: 6px 16px;\r\n    font-size: 14px;\r\n    color: #fff;\r\n    background-color: #3d7f00;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  body {\r\n    font-family: 'Microsoft YaHei';\r\n    color: #666;\r\n    font-size: 12px;\r\n  }\r\n  \r\n  </style>\r\n  "]},"metadata":{},"sourceType":"module"}