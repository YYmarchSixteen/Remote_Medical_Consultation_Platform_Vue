data","doctor","mounted","fetchDoctor","methods","response","get","error","console","updateDoctor","params","URLSearchParams","append","updateForm","introduction","telephone","put","alert","updateProduct","price","medicineId","$message","success","dialogVisible1","fetchProducts","window","location","reload","re","toAdmin","$router","push","toDoctor","toNotice","toPersonal","toStore","toUser"],"sourceRoot":"","sources":["D:\\Dev\\Front\\Remote_Medical_Consultation_Platform_Vue\\src\\views\\User\\personal.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"header\">\r\n      <Header></Header>\r\n    </div>\r\n    <div v-if=\"doctor\">\r\n      <el-form :model=\"doctor\" label-width=\"80px\">\r\n        <el-form-item label=\"联系方式\">\r\n          <el-input v-model=\"doctor.telephone\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"密码\">\r\n          <el-input v-model=\"doctor.password\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"地址\">\r\n          <el-input v-model=\"doctor.address\"></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-column label=\"头像\">\r\n        </el-form-column>\r\n        <el-form-item label=\"照片\">\r\n          <el-upload\r\n            class=\"avatar-uploader\"\r\n            :action=\"uploadUrl\"\r\n            :show-file-list=\"false\"\r\n            :data={doctorId:1}\r\n            :on-success=\"re\"\r\n            name=\"picture\"\r\n          >\r\n            <img v-if=\"doctor.picture\" :src=\"doctor.picture\" class=\"avatar\" width=\"500px\" height=\"500px\">\r\n            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n          </el-upload>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"updateDoctor\">更新用户信息</el-button>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"uploadPic\">更新用户头像</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n    <div v-else>\r\n      <el-alert\r\n        title=\"正在加载用户信息...\"\r\n        type=\"info\"\r\n        show-icon\r\n      ></el-alert>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport Header from '@/views/userLayout/Header/header.vue'\r\n\r\nexport default {\r\n  components: { Header },\r\n  data() {\r\n    return {\r\n      doctor: null\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchDoctor()\r\n  },\r\n  methods: {\r\n    async fetchDoctor() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8081/user/findById?userId=1')\r\n        this.doctor = response.data.data\r\n      } catch (error) {\r\n        console.error('Error fetching doctor:', error)\r\n      }\r\n    },\r\n    async updateDoctor() {\r\n      try {\r\n        const params = new URLSearchParams()\r\n        params.append('introduction', this.updateForm.introduction)\r\n        params.append('telephone', this.updateForm.telephone)\r\n        params.append('doctorId', 13)\r\n        await axios.put('http://localhost:8081/doctor/changeInfo', params)\r\n        alert('用户信息已更新')\r\n      } catch (error) {\r\n        console.error('Error updating doctor:', error)\r\n        alert('更新用户信息时出错')\r\n      }\r\n    },\r\n    async updateProduct() {\r\n      try {\r\n        const params = new URLSearchParams()\r\n        params.append('price', this.updateForm.price)\r\n        params.append('medicineId', this.updateForm.medicineId)\r\n        axios.put('http://localhost:8081/medicine/changePrice', params)\r\n        this.$message.success('调价成功')\r\n        this.dialogVisible1 = false\r\n        await this.fetchProducts()\r\n        window.location.reload()\r\n      } catch (error) {\r\n        console.error('Error adding Product:', error)\r\n      }\r\n    },\r\n    re() {\r\n      window.location.reload()\r\n    },\r\n    toAdmin() {\r\n      this.$router.push('/admin/admin')\r\n    },\r\n    toDoctor() {\r\n      this.$router.push('/admin/doctor')\r\n    },\r\n    toNotice() {\r\n      this.$router.push('/admin/notice')\r\n    },\r\n    toPersonal() {\r\n      this.$router.push('/admin/personal')\r\n    },\r\n    toStore() {\r\n      this.$router.push('/admin/store')\r\n    },\r\n    toUser() {\r\n      this.$router.push('/admin/user')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  max-width: 800px;\r\n  margin-top: 100px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}