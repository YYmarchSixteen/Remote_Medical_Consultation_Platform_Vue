"\r\n      style=\"width: 100%\"\r\n    >\r\n      <el-table-column prop=\"id\" label=\"轮播图Id\" width=\"200\" />\r\n      <el-table-column prop=\"title\" label=\"标题\" width=\"300\" />\r\n      <!-- <el-table-column prop=\"content\" label=\"具体内容\" width=\"300\" /> -->\r\n      <el-table-column prop=\"img\" label=\"照片\" sortable width=\"300\">\r\n        <template #default=\"row\">\r\n          <el-image :src=\"row.row.img\" style=\"width: 70px; height: 70px\" />\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"是否推荐\" width=\"150\">\r\n        <template #default=\"row\">\r\n          <el-switch\r\n            v-model=\"row.row.recommend\"\r\n            class=\"mt-2\"\r\n            @click=\"tuijina(row)\"\r\n          />\r\n        </template>\r\n      </el-table-column>\r\n      <!-- <el-table-column prop=\"noticeId\" label=\"公告Id\" width=\"300\" /> -->\r\n      <el-table-column fixed=\"right\" label=\"操作\" width=\"200\">\r\n        <template #default=\"row\">\r\n          <el-button type=\"success\" @click=\"handleClick(row)\"> 编辑 </el-button>\r\n          <el-popconfirm title=\"确定要删除吗?\" @confirm=\"del(row)\">\r\n            <template #reference>\r\n              <el-button type=\"danger\">删除</el-button>\r\n            </template>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-dialog v-model=\"dialogVisible1\" title=\"增加\" width=\"30%\">\r\n      <el-form label-width=\"150px\">\r\n        <el-form-item label=\"轮播图Id\">\r\n          <el-input v-model=\"info1.id\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"150px\">\r\n        <el-form-item label=\"照片\">\r\n          <el-input v-model=\"info1.img\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"150px\">\r\n        <el-form-item label=\"标题\">\r\n          <el-input v-model=\"info1.title\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible1 = false\">取消</el-button>\r\n          <el-button type=\"primary\" @click=\"insert()\" v-model=\"dialogVisible1\"\r\n            >确认</el-button\r\n          >\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n    <el-dialog v-model=\"dialogVisible\" title=\"编辑\" width=\"30%\">\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"轮播图Id\">\r\n          <el-input v-model=\"info.id\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"标题\">\r\n          <el-input v-model=\"info.title\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"照片\">\r\n          <el-input v-model=\"info.img\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n          <el-button type=\"primary\" @click=\"edit()\" v-model=\"dialogVisible\"\r\n            >确认</el-button\r\n          >\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n    <el-pagination\r\n      currentPage=\"currentPage\"\r\n      :page-sizes=\"[6, 9, 12, 14]\"\r\n      page-size=\"6\"\r\n      :background=\"background\"\r\n      layout=\"total, sizes, prev, pager, next, jumper\"\r\n      :total=\"tableData.length\"\r\n      @size-change=\"handleSizeChange\"\r\n      @current-change=\"handleCurrentChange\"\r\n    />\r\n  </el-card>\r\n</template>\r\n<script>\r\nimport { get } from '@/utils/request'\r\nimport { ElMessage } from 'element-plus'\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentPage: 1, //默认页码为1\r\n      pagesize: 6, //默认一页显示10条\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      info: {}, //修改数据\r\n      info1: {}, //新增数据\r\n      tableData: []\r\n    }\r\n  },\r\n  //初始化工作\r\n  mounted() {\r\n    get('/getPage').then((res) => {\r\n      this.tableData = res.data\r\n      console.log(res)\r\n    })\r\n  },\r\n\r\n  methods: {\r\n    handleSizeChange: function (size) {\r\n      //一页显示多少条\r\n\r\n      this.pagesize = size\r\n    },\r\n\r\n    handleCurrentChange: function (currentPage) {\r\n      //页码更改方法\r\n\r\n      this.currentPage = currentPage\r\n    },\r\n    handleClick(data) {\r\n      this.info = data.row\r\n      this.dialogVisible = !this.dialogVisible\r\n    },\r\n    del(data) {\r\n      get('/deleteCarousel', { Id: data.row.id }).then((res) => {\r\n        ElMessage({\r\n          message: '删除成功！',\r\n          type: 'success'\r\n        })\r\n        window.location.reload()\r\n      })\r\n    },\r\n    //修改\r\n    edit() {\r\n      console.log('===========')\r\n      console.log(this.info)\r\n      this.dialogVisible = !this.dialogVisible //马上消失\r\n      get('/updateCarousel', this.info).then((res) => {\r\n        ElMessage({\r\n          message: '修改成功！',\r\n          type: 'success'\r\n        })\r\n        window.location.reload()\r\n      })\r\n    },\r\n\r\n    add() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n    },\r\n\r\n    insert() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n      get('/addCarousel', this.info1).then((res) => {\r\n        console.log(res)\r\n        if (res.data == 1) {\r\n          ElMessage({\r\n            message: '添加成功',\r\n            type: 'success'\r\n          })\r\n          // 刷新\r\n          window.location.reload()\r\n        } else {\r\n          ElMessage({\r\n            message: '添加失败',\r\n            type: 'danger'\r\n          })\r\n        }\r\n      })\r\n    },\r\n    // tuijina(row) {\r\n    //   console.log(\"============================\")\r\n    //   console.log(row.row.id)\r\n    //   get('/updateRecommend2', {\r\n    //     docId: row.row.id,\r\n    //     recommend: row.row.recommend\r\n    //   })\r\n    //   