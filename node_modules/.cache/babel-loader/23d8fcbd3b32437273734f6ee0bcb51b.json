{"ast":null,"code":"import axios from 'axios';\nimport { reactive, onMounted } from 'vue';\nimport { ElButton, ElForm, ElFormItem, ElInput, ElCard } from 'element-plus';\nimport Header from '../userLayout/Header/header.vue';\nexport default {\n  components: {\n    ElButton,\n    ElForm,\n    ElFormItem,\n    ElInput,\n    ElCard,\n    Header\n  },\n\n  setup() {\n    const form = reactive({\n      username: '',\n      password: '',\n      telephone: ''\n    });\n    const orders = reactive([]);\n    const favorites = reactive([]);\n\n    const getInfo = async () => {\n      try {\n        const response = await axios.get('/api/user/info');\n        const {\n          username,\n          telephone\n        } = response.data;\n        form.username = username;\n        form.telephone = telephone;\n      } catch (error) {\n        console.error(error);\n      }\n    };\n\n    const getOrders = async () => {\n      try {\n        const response = await axios.get('/api/user/orders');\n        orders.splice(0, orders.length, ...response.data);\n      } catch (error) {\n        console.error(error);\n      }\n    };\n\n    const getFavorites = async () => {\n      try {\n        const response = await axios.get('/api/user/favorites');\n        favorites.splice(0, favorites.length, ...response.data);\n      } catch (error) {\n        console.error(error);\n      }\n    };\n\n    const updateInfo = async () => {\n      try {\n        await axios.put('/api/user/info', form);\n        alert('个人信息已更新！');\n      } catch (error) {\n        console.error(error);\n      }\n    };\n\n    const deleteFavorite = async item => {\n      try {\n        const response = await axios.delete(`/api/user/favorites/${item.id}`);\n\n        if (response.status === 200) {\n          favorites.splice(favorites.indexOf(item), 1);\n          alert('删除成功！');\n        }\n      } catch (error) {\n        console.error(error);\n      }\n    };\n\n    onMounted(() => {\n      getInfo();\n      getOrders();\n      getFavorites();\n    });\n    return {\n      form,\n      orders,\n      favorites,\n      updateInfo,\n      deleteFavorite\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA8BA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAoC,KAApC;AACA,SAASC,QAAT,EAAoBC,MAApB,EAA4BC,UAA5B,EAAwCC,OAAxC,EAAkDC,MAAlD,QAAgE,cAAhE;AACA,OAAOC,MAAP,MAAmB,iCAAnB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVN,YADU;AAEVC,UAFU;AAGVC,cAHU;AAIVC,WAJU;AAKVC,UALU;AAMVC;AANU,GADC;;AASbE,OAAK,GAAG;AACN,UAAMC,IAAG,GAAIV,QAAQ,CAAC;AACpBW,cAAQ,EAAE,EADU;AAEpBC,cAAQ,EAAE,EAFU;AAGpBC,eAAS,EAAE;AAHS,KAAD,CAArB;AAMA,UAAMC,MAAK,GAAId,QAAQ,CAAC,EAAD,CAAvB;AACA,UAAMe,SAAQ,GAAIf,QAAQ,CAAC,EAAD,CAA1B;;AAEA,UAAMgB,OAAM,GAAI,YAAY;AAC1B,UAAI;AACF,cAAMC,QAAO,GAAI,MAAMlB,KAAK,CAACmB,GAAN,CAAU,gBAAV,CAAvB;AACA,cAAM;AAAEP,kBAAF;AAAYE;AAAZ,YAA0BI,QAAQ,CAACE,IAAzC;AACAT,YAAI,CAACC,QAAL,GAAgBA,QAAhB;AACAD,YAAI,CAACG,SAAL,GAAiBA,SAAjB;AACF,OALA,CAKE,OAAOO,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAcA,KAAd;AACF;AACD,KATD;;AAWA,UAAME,SAAQ,GAAI,YAAY;AAC5B,UAAI;AACF,cAAML,QAAO,GAAI,MAAMlB,KAAK,CAACmB,GAAN,CAAU,kBAAV,CAAvB;AACAJ,cAAM,CAACS,MAAP,CAAc,CAAd,EAAiBT,MAAM,CAACU,MAAxB,EAAgC,GAAGP,QAAQ,CAACE,IAA5C;AACF,OAHA,CAGE,OAAOC,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAcA,KAAd;AACF;AACD,KAPD;;AASA,UAAMK,YAAW,GAAI,YAAY;AAC/B,UAAI;AACF,cAAMR,QAAO,GAAI,MAAMlB,KAAK,CAACmB,GAAN,CAAU,qBAAV,CAAvB;AACAH,iBAAS,CAACQ,MAAV,CAAiB,CAAjB,EAAoBR,SAAS,CAACS,MAA9B,EAAsC,GAAGP,QAAQ,CAACE,IAAlD;AACF,OAHA,CAGE,OAAOC,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAcA,KAAd;AACF;AACD,KAPD;;AASA,UAAMM,UAAS,GAAI,YAAY;AAC7B,UAAI;AACF,cAAM3B,KAAK,CAAC4B,GAAN,CAAU,gBAAV,EAA4BjB,IAA5B,CAAN;AACAkB,aAAK,CAAC,UAAD,CAAL;AACF,OAHA,CAGE,OAAOR,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAcA,KAAd;AACF;AACD,KAPD;;AASA,UAAMS,cAAa,GAAI,MAAOC,IAAP,IAAgB;AACrC,UAAI;AACF,cAAMb,QAAO,GAAI,MAAMlB,KAAK,CAACgC,MAAN,CAAc,uBAAsBD,IAAI,CAACE,EAAG,EAA5C,CAAvB;;AACA,YAAIf,QAAQ,CAACgB,MAAT,KAAoB,GAAxB,EAA6B;AAC3BlB,mBAAS,CAACQ,MAAV,CAAiBR,SAAS,CAACmB,OAAV,CAAkBJ,IAAlB,CAAjB,EAA0C,CAA1C;AACAF,eAAK,CAAC,OAAD