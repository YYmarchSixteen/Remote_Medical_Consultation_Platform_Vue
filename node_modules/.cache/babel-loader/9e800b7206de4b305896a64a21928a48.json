{"ast":null,"code":"import axios from 'axios';\nimport Header from '../userLayout/Header/header.vue';\nexport default {\n  components: {\n    Header\n  },\n\n  data() {\n    return {\n      form: {},\n      username: sessionStorage.getItem('username'),\n      userid: sessionStorage.getItem('userid'),\n      isLoading: false,\n      equipmentList: [],\n      equipmentTypes: [],\n      selectedType: '',\n      back: []\n    };\n  },\n\n  mounted() {\n    this.getEquipmentList();\n    this.fetchEquipmentTypes();\n  },\n\n  methods: {\n    changeSelect() {\n      alert(\"当前显示类型\" + this.selectedType);\n      axios.get('http://localhost:8080/typeSearch?type=' + this.selectedType).then(res => {\n        console.log(res.data);\n        this.equipmentList = res.data;\n        console.log(this.equipmentList);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    async fetchEquipmentTypes() {\n      try {\n        const res = await axios.get('http://localhost:8080/equipmenttypeList');\n        this.equipmentTypes.push(...res.data);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async getEquipmentList() {\n      try {\n        const response = await axios.get('http://localhost:8080/equipmentList');\n        this.equipmentList = response.data;\n      } catch (error) {\n        console.error(error);\n      }\n    },\n\n    updateEquipmentList(newList) {\n      this.equipmentList = newList;\n    },\n\n    detail(item) {\n      this.$router.push({\n        path: '/detail',\n        query: {\n          id: item.id\n        }\n      });\n    },\n\n    async insertCollect(item) {\n      if (this.isLoading) {\n        return;\n      }\n\n      try {\n        this.isLoading = true;\n        const response = await axios.get('http://localhost:8080/collect', {\n          params: {\n            userid: sessionStorage.getItem('userid'),\n            equipmentid: item.id\n          }\n        });\n\n        if (response.status === 200) {\n          if (response.data === '当前商品已收藏') {\n            this.$message.error('当前商品已收藏');\n          } else if (response.data === '收藏成功') {\n            this.$message.success('收藏成功');\n            item.isCollected = true;\n          } else if (response.data === '收藏失败，请稍后再试') {\n            this.$message.error('收藏失败，请稍后再试');\n          }\n        }\n      } catch (error) {\n        console.error(error);\n      } finally {\n        this.isLoading = false;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iCAAnB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVD;AADU,GADC;;AAIbE,MAAI,GAAG;AACL,WAAO;AACLC,UAAI,EAAE,EADD;AAELC,cAAQ,EAAEC,cAAc,CAACC,OAAf,CAAuB,UAAvB,CAFL;AAGLC,YAAM,EAAEF,cAAc,CAACC,OAAf,CAAuB,QAAvB,CAHH;AAILE,eAAS,EAAE,KAJN;AAKLC,mBAAa,EAAE,EALV;AAMLC,oBAAc,EAAE,EANX;AAOLC,kBAAY,EAAE,EAPT;AAQLC,UAAI,EAAE;AARD,KAAP;AAUD,GAfY;;AAiBbC,SAAO,GAAG;AACR,SAAKC,gBAAL;AACA,SAAKC,mBAAL;AAED,GArBY;;AAuBbC,SAAO,EAAE;AACPC,gBAAY,GAAG;AACbC,WAAK,CAAC,WAAW,KAAKP,YAAjB,CAAL;AACAZ,WAAK,CAACoB,GAAN,CAAU,2CAA2C,KAAKR,YAA1D,EACGS,IADH,CACQC,GAAE,IAAK;AACXC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACnB,IAAhB;AACA,aAAKO,aAAL,GAAqBY,GAAG,CAACnB,IAAzB;AACAoB,eAAO,CAACC,GAAR,CAAY,KAAKd,aAAjB;AACD,OALH,EAMGe,KANH,CAMSC,KAAI,IAAK;AACdH,eAAO,CAACC,GAAR,CAAYE,KAAZ;AACD,OARH;AASD,KAZM;;AAcP,UAAMV,mBAAN,GAA4B;AAC1B,UAAI;AACF,cAAMM,GAAE,GAAI,MAAMtB,KAAK,CAACoB,GAAN,CAAU,yCAAV,CAAlB;AACA,aAAKT,cAAL,CAAoBgB,IAApB,CAAyB,GAAGL,GAAG,CAACnB,IAAhC;AACF,OAHA,CAGE,OAAOuB,KAAP,EAAc;AACdH,eAAO,CAACC,GAAR,CAAYE,KAAZ;AACF;AACD,KArBM;;AAsBP,UAAMX,gBAAN,GAAyB;AACvB,UAAI;AACF,cAAMa,QAAO,GAAI,MAAM5B,KAAK,CAACoB,GAAN,CAAU,qCAAV,CAAvB;AACA,aAAKV,aAAL,GAAqBkB,QAAQ,CAACzB,IAA9B;AACF,OAHA,CAGE,OAAOuB,KAAP,EAAc;AACdH,eAAO,CAACG,KAAR,CAAcA,KAAd;AACF;AACD,KA7BM;;AA+BPG,uBAAmB,CAACC,OAAD,EAAU;AAC3B,WAAKpB,aAAL,GAAqBoB,OAArB;AACD,KAjCM;;AAmCPC,UAAM,CAACC,IAAD,EAAO;AACX,WAAKC,OAAL,CAAaN,IAAb,CAAkB;AAChBO,YAAI,EAAE,SADU;AAEhBC,aAAK,EAAE;AACLC,YAAE,EAAEJ,IAAI,CAACI;AADJ;AAFS,OAAlB;AAMD,KA1CM;;AA4CP,UAAMC,aAAN,CAAoBL,IAApB,EAA0B;AACxB,UAAI,KAAKvB,SAAT,EAAoB;AAClB;AACF;;AACA,UAAI;AACF,aAAKA,SAAL,GAAiB,IAAjB;AACA,cAAMmB,QAAO,GAAI,MAAM5B,KAAK,CAACoB,GAAN,CAAU,+BAAV,EAA2C;AAChEkB,gBAAM,EAAE;AACN9B,kBAAM,EAAEF,cAAc,CAACC,OAAf,CAAuB,QAAvB,CADF;AAENgC,uBAAW,EAAEP,IAAI,CAACI;AAFZ;AADwD,SAA3C,CAAvB;;AAMA,YAAIR,QAAQ,CAACY,MAAT,KAAoB,GAAxB,EAA6B;AAC3B,cAAIZ,QAAQ,CAACzB,IAAT,KAAkB,SAAtB,EAAiC;AAC/B,iBAAKsC,QAAL,CAAcf,KAAd,CAAoB,SAApB;AACF,WAFA,MAEO,IAAIE,QAAQ,CAACzB,IAAT,KAAkB,MAAtB,EAA8B;AACnC,iBAAKsC,QAAL,CAAcC,OAAd,CAAsB,MAAtB;AACAV,gBAAI,CAACW,WAAL,GAAmB,IAAnB;AACF,WAHO,MAGA,IAAIf,QAAQ,CAACzB,IAAT,KAAkB,YAAtB,EAAoC;AACzC,iBAAKsC,QAAL,CAAcf,KAAd,CAAoB,YAApB;AACF;AACF;AACF,OAlBA,CAkBE,OAAOA,KAAP,EAAc;AACdH,eAAO,CAACG,KAAR,CAAcA,KAAd;AACF,OApBA,SAoBU;AACR,aAAKjB,SAAL,GAAiB,KAAjB;AACF;AACD;;AAvEM;AAvBI,CAAf","names":["axios","Header","components","data","form","username","sessionStorage","getItem","userid","isLoading","equipmentList","equipmentTypes","selectedType","back","mounted","getEquipmentList","fetchEquipmentTypes","methods","changeSelect","alert","get","then","res","console","log","catch","error","push","response","updateEquipmentList","newList","detail","item","$router","path","query","id","insertCollect","params","equipmentid","status","$message","success","isCollected"],"sourceRoot":"","sources":["D:\\Dev\\Front\\demo01\\src\\views\\User\\userIndex.vue"],"sourcesContent":["<template>\r\n  <div class=\"header\">\r\n    <Header></Header>\r\n  </div>\r\n  <div class=\"body\">\r\n    <div class=\"container\">\r\n      <div class=\"select\">\r\n        <el-select v-model=\"selectedType\" placeholder=\"请选择设备类型\" @change=\"changeSelect()\">\r\n          <el-option v-for=\"type in equipmentTypes\" :key=\"type.id\" :label=\"type.typename\" :value=\"type.id\"></el-option>\r\n        </el-select>\r\n      </div>\r\n      <div class=\"header-search\">\r\n        <input type=\"text\" v-model=\"searchTerm\" class=\"search-input\" placeholder=\"请输入搜索内容\" />\r\n        <button class=\"search-btn\" @click=\"handleSearch\">搜索</button>\r\n      </div>\r\n    </div>\r\n    <div class=\"equipment-list\">\r\n      <div class=\"equipment-item\" v-for=\"(item, index) in equipmentList\" :key=\"index\">\r\n        <div class=\"img-wrapper\">\r\n          <img :src=\"item.picture\" alt=\"\" style=\"width: 200px; height: auto;\">\r\n        </div>\r\n        <div class=\"info-wrapper\">\r\n          <div class=\"title\">{{ item.equipmentname }}</div>\r\n          <div class=\"price\">单价: ￥{{ item.price }}</div>\r\n          <div class=\"feature\">\r\n            <h2>功能:</h2> {{ item.feature }}\r\n          </div>\r\n          <div class=\"material\">\r\n            <h2>材质：</h2>{{ item.material }}\r\n          </div>\r\n          <button class=\"buy-button\" @click=\"detail(item)\">查看详情</button>\r\n          <button class=\"collect-button\" @click=\"insertCollect(item)\">加入收藏</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n  \r\n<script>\r\nimport axios from 'axios';\r\nimport Header from '../userLayout/Header/header.vue';\r\n\r\nexport default {\r\n  components: {\r\n    Header\r\n  },\r\n  data() {\r\n    return {\r\n      form: {},\r\n      username: sessionStorage.getItem('username'),\r\n      userid: sessionStorage.getItem('userid'),\r\n      isLoading: false,\r\n      equipmentList: [],\r\n      equipmentTypes: [],\r\n      selectedType: '',\r\n      back: []\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.getEquipmentList();\r\n    this.fetchEquipmentTypes();\r\n\r\n  },\r\n\r\n  methods: {\r\n    changeSelect() {\r\n      alert(\"当前显示类型\" + this.selectedType)\r\n      axios.get('http://localhost:8080/typeSearch?type=' + this.selectedType)\r\n        .then(res => {\r\n          console.log(res.data)\r\n          this.equipmentList = res.data\r\n          console.log(this.equipmentList)\r\n        })\r\n        .catch(error => {\r\n          console.log(error)\r\n        })\r\n    },\r\n\r\n    async fetchEquipmentTypes() {\r\n      try {\r\n        const res = await axios.get('http://localhost:8080/equipmenttypeList');\r\n        this.equipmentTypes.push(...res.data);\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n    async getEquipmentList() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/equipmentList');\r\n        this.equipmentList = response.data;\r\n      } catch (error) {\r\n        console.error(error);\r\n      }\r\n    },\r\n\r\n    updateEquipmentList(newList) {\r\n      this.equipmentList = newList;\r\n    },\r\n\r\n    detail(item) {\r\n      this.$router.push({\r\n        path: '/detail',\r\n        query: {\r\n          id: item.id,\r\n        },\r\n      });\r\n    },\r\n\r\n    async insertCollect(item) {\r\n      if (this.isLoading) {\r\n        return;\r\n      }\r\n      try {\r\n        this.isLoading = true;\r\n        const response = await axios.get('http://localhost:8080/collect', {\r\n          params: {\r\n            userid: sessionStorage.getItem('userid'),\r\n            equipmentid: item.id,\r\n          },\r\n        });\r\n        if (response.status === 200) {\r\n          if (response.data === '当前商品已收藏') {\r\n            this.$message.error('当前商品已收藏');\r\n          } else if (response.data === '收藏成功') {\r\n            this.$message.success('收藏成功');\r\n            item.isCollected = true;\r\n          } else if (response.data === '收藏失败，请稍后再试') {\r\n            this.$message.error('收藏失败，请稍后再试');\r\n          }\r\n        }\r\n      } catch (error) {\r\n        console.error(error);\r\n      } finally {\r\n        this.isLoading = false;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n  \r\n<style scoped>\r\n.body {\r\n  margin-top: 100px;\r\n}\r\n\r\n\r\n\r\n.equipment-item {\r\n  display: flex;\r\n  margin-bottom: 20px;\r\n  background-color: #fff;\r\n  box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.1);\r\n  border: 1px solid #cccccc;\r\n  /* 添加淡黑色边框 */\r\n}\r\n\r\n.equipment-list {\r\n  margin-top: 15px;\r\n}\r\n\r\n.img-wrapper {\r\n  width: 35%;\r\n  padding: 10px 10px 10px 200px;\r\n  /* 左右留白150px，上下10px */\r\n  margin: 0 10px 0 0;\r\n  /* 水平间距10px */\r\n}\r\n\r\n.img-wrapper img {\r\n  display: block;\r\n  width: 100%;\r\n  height: auto;\r\n}\r\n\r\n.info-wrapper {\r\n  width: 165%;\r\n  padding: 10px 150px 10px 10px;\r\n  /* 左右留白150px，上下10px */\r\n  margin: 0 0 0 10px;\r\n  /* 水平间距10px */\r\n}\r\n\r\n.title {\r\n  font-weight: bold;\r\n  font-size: 18px;\r\n  color: #333;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.price {\r\n  font-size: 16px;\r\n  color: red;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.feature,\r\n.material {\r\n  font-size: 14px;\r\n  color: #555;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.buy-button {\r\n  padding: 8px 18px;\r\n  border: none;\r\n  background-color: #1bb1c4;\r\n  color: #fff;\r\n  cursor: pointer;\r\n}\r\n\r\n.collect-button {\r\n  padding: 8px 18px;\r\n  border: none;\r\n  background-color: #da7bc4;\r\n  color: #fff;\r\n  cursor: pointer;\r\n  margin-left: 10px;\r\n}\r\n\r\n.buy-button:hover {\r\n  background-color: #ff9f3e;\r\n}\r\n\r\n.select {\r\n  margin-left: 20;\r\n}\r\n\r\n.header-search {\r\n  margin-left: 20px;\r\n}\r\n\r\n.search-input {\r\n  border: 2px solid #7ec8e3;\r\n  border-radius: 20px;\r\n  padding: 6px 12px;\r\n  font-size: 14px;\r\n  color: #333;\r\n}\r\n\r\n.search-btn {\r\n  border: none;\r\n  border-radius: 20px;\r\n  padding: 6px 16px;\r\n  font-size: 14px;\r\n  color: #fff;\r\n  background-color: #3d7f00;\r\n  cursor: pointer;\r\n}\r\n\r\n.container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n</style>\r\n  "]},"metadata":{},"sourceType":"module"}