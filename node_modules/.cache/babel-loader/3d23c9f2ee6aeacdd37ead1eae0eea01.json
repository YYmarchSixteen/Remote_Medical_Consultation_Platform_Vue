{"ast":null,"code":"import axios from 'axios';\nimport Header from '@/views/userLayout/Header/header.vue';\nexport default {\n  components: {\n    Header\n  },\n\n  data() {\n    return {\n      medicine: [],\n      cartId: ''\n    };\n  },\n\n  mounted() {\n    this.fetchCartData();\n  },\n\n  methods: {\n    fetchCartData() {\n      axios.get(`http://localhost:8081/cart/findByUserId?userId=1`).then(response => {\n        this.medicine = response.data.data;\n        console.log(this.medicine);\n      }).catch(error => {\n        console.error('Error fetching cart data:', error);\n      });\n    },\n\n    async remove(cartId) {\n      try {\n        await axios.delete('http://localhost:8081/cart/delCart', {\n          params: {\n            cartId: cartId\n          }\n        });\n        alert('商品已移出购物车');\n        window.location.reload();\n      } catch (error) {\n        console.error('Error removing item from cart:', error);\n        alert('商品移出购物车失败');\n      }\n    },\n\n    async pay(item) {\n      try {\n        const params = new URLSearchParams();\n        params.append('userId', 1);\n        params.append('medicineId', item.medicineId);\n        params.append('count', item.count);\n        await axios.post('http://localhost:8081/detail/add', params);\n        alert('购买成功');\n      } catch (error) {\n        alert('购买失败');\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2CA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,sCAAnB;AAEA,eAAe;AACbC,YAAU,EAAE;AAAED;AAAF,GADC;;AAEbE,MAAI,GAAG;AACL,WAAO;AACLC,cAAQ,EAAE,EADL;AAELC,YAAM,EAAE;AAFH,KAAP;AAID,GAPY;;AAQbC,SAAO,GAAG;AACR,SAAKC,aAAL;AACD,GAVY;;AAWbC,SAAO,EAAE;AACPD,iBAAa,GAAG;AACdP,WAAK,CAACS,GAAN,CAAW,kDAAX,EACGC,IADH,CACQC,QAAO,IAAK;AAChB,aAAKP,QAAL,GAAgBO,QAAQ,CAACR,IAAT,CAAcA,IAA9B;AACAS,eAAO,CAACC,GAAR,CAAY,KAAKT,QAAjB;AACD,OAJH,EAKGU,KALH,CAKSC,KAAI,IAAK;AACdH,eAAO,CAACG,KAAR,CAAc,2BAAd,EAA2CA,KAA3C;AACD,OAPH;AAQD,KAVM;;AAWP,UAAMC,MAAN,CAAaX,MAAb,EAAqB;AACnB,UAAI;AACF,cAAML,KAAK,CAACiB,M