n    <el-table>\r\n</el-table>\r\n    <el-table-column fixed=\"right\" label=\"操作\" width=\"240\">\r\n      <template #default=\"row\">\r\n        <el-button type=\"success\" @click=\"handleClick(row)\">编辑</el-button>\r\n        <el-button type=\"danger\" @click=\"del(row)\">删除</el-button>\r\n      </template>\r\n    </el-table-column>\r\n  </el-table>\r\n\r\n<!-- 添加框 -->\r\n  <el-dialog v-model=\"dialogVisible1\" title=\"增加\" width=\"30%\">\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"器械名\">\r\n        <el-input v-model=\"info1.equipmentname\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"特点\">\r\n        <el-input v-model=\"info1.feature\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"材质成分\">\r\n        <el-input v-model=\"info1.material\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"价格\">\r\n        <el-input v-model=\"info1.price\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"所属类型\">\r\n        <el-input v-model=\"info1.type\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"图片\">\r\n        <el-input v-model=\"info1.picture\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible1 = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"insert(row)\" v-model=\"dialogVisible1\"\r\n          >确认</el-button\r\n        >\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n\r\n\r\n  <!-- 修改框 -->\r\n  <el-dialog v-model=\"dialogVisible\" title=\"编辑\" width=\"30%\">\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"器械名\">\r\n        <el-input v-model=\"info2.equipmentname\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"特点\">\r\n        <el-input v-model=\"info2.feature\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"材质成分\">\r\n        <el-input v-model=\"info2.material\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"价格\">\r\n        <el-input v-model=\"info2.price\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"所属类型\">\r\n        <el-input v-model=\"info2.type\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"图片\">\r\n        <el-input v-model=\"info2.picture\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"edit(row)\" v-model=\"dialogVisible\">确认</el-button>\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n\r\n\r\n  <el-pagination\r\n    currentPage=\"currentPage\"\r\n    :page-sizes=\"[10]\"\r\n    page-size=\"10\"\r\n    :background=\"background\"\r\n    layout=\"total, sizes, prev, pager, next, jumper\"\r\n    :total=\"list.length\"\r\n    @size-change=\"handleSizeChange\"\r\n    @current-change=\"handleCurrentChange\"\r\n  />\r\n  </el-card>\r\n \r\n</template>\r\n<script>\r\nimport { get } from '@/utils/request'\r\nimport { ElMessage } from 'element-plus'\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentPage: 1, //默认页码为1\r\n      pagesize: 15, //默认一页显示 条\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      info2: {}, //修改数据\r\n      info1: {}, //新增数据\r\n      list: [],\r\n      adg: [],\r\n    }\r\n  },\r\n  //初始化工作\r\n  mounted() {\r\n    get('http://localhost:8080/equipmentList').then((res) => {\r\n      console.log(res)\r\n      this.list = res.data\r\n    })\r\n  },\r\n  methods: {\r\n    del(data) {\r\n      console.log(data.row.id)\r\n      get('http://localhost:8080/deleteEquipment', { id: data.row.id }).then(() => {\r\n        ElMessage({\r\n          message: '删除成功！',\r\n          type: 'success'\r\n        })\r\n      })\r\n      window.location.reload() //刷新\r\n    },\r\n    add() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n    },\r\n    insert() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n      get('http://localhost:8080/insertEquipment', this.info1).then((res) => {\r\n        console.log(res)\r\n        if (res.data == '添加成功') {\r\n          ElMessage({\r\n            message: '添加成功',\r\n            type: 'success'\r\n          })\r\n          window.location.reload()\r\n        } else {\r\n          ElMessage({\r\n            message: '添加失败',\r\n            type: 'danger'\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleClick() {\r\n      this.dialogVisible = !this.dialogVisible\r\n    },\r\n    edit(data) {\r\n      console.log(data.row.id)\r\n      this.dialogVisible = !this.dialogVisible\r\n      get('/http://localhost:8080/updateEquipment', this.info2,{ id: data.row.id }).then((res) => {\r\n        console.log(res)\r\n        if (res.data == '修改成功') {\r\n          ElMessage({\r\n            message: '修改成功',\r\n            type: 'success'\r\n          })\r\n          window.location.reload()\r\n        } else {\r\n          ElMessage({\r\n            message: '修改失败',\r\n            type: 'danger'\r\n          })\r\n        }\r\n        // ElMessage({\r\n        //   message: '成功！',\r\n        //   type: 'success'\r\n        // })\r\n        // window.location.reload()\r\n      })\r\n    },\r\n\r\n    handleSizeChange: function (size) {\r\n      //一页显示多少条\r\n      this.pagesize = size\r\n    },\r\n    handleCurrentChange: function (currentPage) {\r\n      //页码更改方法\r\n      this.currentPage = currentPage\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.box-card {\r\n  margin-top: 5px;\r\n  margin-left: 15px;\r\n  margin-right: 15px;\r\n  height: 95%;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}