{"ast":null,"code":"import axios from 'axios';\nimport Header from '../userLayout/Header/header.vue';\nexport default {\n  components: {\n    Header\n  },\n\n  data() {\n    return {\n      form: {},\n      username: sessionStorage.getItem('username'),\n      userid: sessionStorage.getItem('userid'),\n      isLoading: false,\n      medicineList: [],\n      medicineTypes: [],\n      selectedType: '',\n      back: []\n    };\n  },\n\n  mounted() {\n    this.getMedicineList();\n    this.fetchMedicineTypes();\n  },\n\n  methods: {\n    medicineSearch() {\n      if (!this.searchTerm) {\n        // 如果搜索框为空，就显示全部\n        this.getMedicineList();\n        return;\n      }\n\n      const keyword = this.searchTerm.trim();\n      const filteredList = this.medicineList.filter(item => item.medicinename.indexOf(keyword) !== -1 || item.feature.indexOf(keyword) !== -1 || item.material.indexOf(keyword) !== -1);\n      this.updateMedicineList(filteredList);\n    },\n\n    changeSelect() {\n      alert(\"当前显示类型\" + this.selectedType);\n      axios.get('http://localhost:8080/typeSearch?type=' + this.selectedType).then(res => {\n        console.log(res.data);\n        this.meidcineList = res.data;\n        console.log(this.medicineList);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    async fetchMedicineTypes() {\n      try {\n        const res = await axios.get('http://localhost:8080/allMedicineType');\n        this.medicineTypes.push(...res.data.data);\n        console.log(this.medicineTypes);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async getMedicineList() {\n      try {\n        const response = await axios.get('http://localhost:8080/allMedicine');\n        this.medicineList = response.data.data;\n      } catch (error) {\n        console.error(error);\n      }\n    },\n\n    updateMedicineList(newList) {\n      this.medicineList = newList;\n    },\n\n    detail(item) {\n      this.$router.push({\n        path: '/detail',\n        query: {\n          id: item.id\n        }\n      });\n    },\n\n    async insertCollect(item) {\n      if (this.isLoading) {\n        return;\n      }\n\n      try {\n        this.isLoading = true;\n        const response = await axios.get('http://localhost:8080/collect', {\n          params: {\n            userid: sessionStorage.getItem('userid'),\n            equipmentid: item.id\n          }\n        });\n\n        if (response.status === 200) {\n          if (response.data === '当前商品已收藏') {\n            this.$message.error('当前商品已收藏');\n          } else if (response.data === '收藏成功') {\n            this.$message.success('收藏成功');\n            item.isCollected = true;\n          } else if (response.data === '收藏失败，请稍后再试') {\n            this.$message.error('收藏失败，请稍后再试');\n          }\n        }\n      } catch (error) {\n        console.error(error);\n      } finally {\n        this.isLoading = false;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4CA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iCAAnB;AAEA,eAAe;AACXC,YAAU,EAAE;AACRD;AADQ,GADD;;AAIXE,MAAI,GAAG;AACH,WAAO;AACHC,UAAI,EAAE,EADH;AAEHC,cAAQ,EAAEC,cAAc,CAACC,OAAf,CAAuB,UAAvB,CAFP;AAGHC,YAAM,EAAEF,cAAc,CAACC,OAAf,CAAuB,QAAvB,CAHL;AAIHE,eAAS,EAAE,KAJR;AAKHC,kBAAY,EAAE,EALX;AAMHC,mBAAa,EAAE,EANZ;AAOHC,kBAAY,EAAE,EAPX;AAQHC,UAAI,EAAE;AARH,KAAP;AAUH,GAfU;;AAiBXC,SAAO,GAAG;AACN,SAAKC,eAAL;AACA,SAAKC,kBAAL;AAEH,GArBU;;AAuBXC,SAAO,EAAE;AACLC,kBAAc,GAAG;AACb,UAAI,CAAC,KAAKC,UAAV,EAAsB;AAAE;AACpB,aAAKJ,eAAL;AACA;AACJ;;AACA,YAAMK,OAAM,GAAI,KAAKD,UAAL,CAAgBE,IAAhB,EAAhB;AACA,YAAMC,YAAW,GAAI,KAAKZ,YAAL,CAAkBa,MAAlB,CAA0BC,IAAD,IAC1CA,IAAI,CAACC,YAAL,CAAkBC,OAAlB,CAA0BN,OAA1B,MAAuC,CAAC,CAAxC,IACAI,IAAI,CAACG,OAAL,CAAaD,OAAb,CAAqBN,OAArB,MAAkC,CAAC,CADnC,IAEAI,IAAI,CAACI,QAAL,CAAcF,OAAd,CAAsBN,OAAtB,MAAmC,CAAC,CAHnB,CAArB;AAKA,WAAKS,kBAAL,CAAwBP,YAAxB;AACH,KAbI;;AAcLQ,gBAAY,GAAG;AACXC,WAAK,CAAC,WAAW,KAAKnB,YAAjB,CAAL;AACAZ,WAAK,CAACgC,GAAN,CAAU,2CAA2C,KAAKpB,YAA1D,EACKqB,IADL,CACUC,GAAE,IAA