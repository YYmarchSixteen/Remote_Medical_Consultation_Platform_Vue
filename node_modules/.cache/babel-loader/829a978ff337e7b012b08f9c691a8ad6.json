alStorage.getItem('doctorId')\r\n    console.log(this.doctorId)\r\n  },\r\n  methods: {\r\n    async fetchDoctor() {\r\n      try {\r\n        const response = await axios.get(`http://localhost:8081/doctor/findById?doctorId=${localStorage.getItem('doctorId')}`)\r\n        this.doctor = response.data.data\r\n      } catch (error) {\r\n        console.error('Error fetching doctor:', error)\r\n      }\r\n    },\r\n    toComment(doctor){\r\n      localStorage.setItem('doctorId', doctor.doctorId)\r\n      router.push('/user/doctorInfo')\r\n    },\r\n    formatTitle(title) {\r\n      switch (title) {\r\n        case '1':\r\n          return '医师'\r\n        case '2':\r\n          return '主治医师'\r\n        case '3':\r\n          return '副主任医师'\r\n        case '4':\r\n          return '主任医师'\r\n        default:\r\n          return ''\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  max-width: 800px;\r\n  margin: 100px auto;\r\n}\r\n\r\n.form {\r\n  margin-top: 20px;\r\n}\r\n\r\n.form-item {\r\n  display: flex;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.form-item label {\r\n  width: 80px;\r\n}\r\n\r\n.info {\r\n  padding: 6px 10px;\r\n  border: none;\r\n  border-radius: 4px;\r\n}\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}