{"ast":null,"code":"import axios from 'axios';\nimport Header from '@/views/userLayout/Header/header.vue';\nimport { ElButton } from 'element-plus';\nexport default {\n  components: {\n    Header,\n    ElButton\n  },\n\n  data() {\n    return {\n      medicines: [],\n      types: [] // 新增 types 数组用于存储药品类型\n\n    };\n  },\n\n  mounted() {\n    this.fetchMedicines();\n    this.fetchTypes(); // 获取药品类型\n  },\n\n  computed: {\n    filteredMedicines() {\n      // 根据药品类型过滤药品\n      return this.medicines.filter(medicine => {\n        return this.types.includes(medicine.type); // 只显示包含在 types 数组中的药品类型\n      });\n    }\n\n  },\n  methods: {\n    fetchMedicines() {\n      axios.get('http://localhost:8081/medicine/all').then(response => {\n        this.medicines = response.data.data;\n      }).catch(error => {\n        console.error('Error fetching medicines:', error);\n      });\n    },\n\n    fetchTypes() {\n      axios.get('http://localhost:8081/medicineType/all').then(response => {\n        this.types = response.data.data.map(type => type.name);\n      }).catch(error => {\n        console.error('Error fetching types:', error);\n      });\n    },\n\n    addToCart(medicine) {\n      console.log('Added to cart:', medicine);\n    },\n\n    // eslint-disable-next-line no-unused-vars\n    detail(medicine) {// Implement the detail method\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,sCAAnB;AACA,SAASC,QAAT,QAAyB,cAAzB;AAEA,eAAe;AACbC,YAAU,EAAE;AAAEF,UAAF;AAAUC;AAAV,GADC;;AAEbE,MAAI,GAAG;AACL,WAAO;AACLC,eAAS,EAAE,EADN;AAELC,WAAK,EAAE,EAFF,CAEK;;AAFL,KAAP;AAID,GAPY;;AAQbC,SAAO,GAAG;AACR,SAAKC,cAAL;AACA,SAAKC,UAAL,GAFQ,CAEW;AACpB,GAXY;;AAYbC,UAAQ,EAAE;AACRC,qBAAiB,GAAG;AAClB;AACA,aAAO,KAAKN,SAAL,CAAeO,MAAf,CAAsBC,QAAO,IAAK;AACvC,eAAO,KAAKP,KAAL,CAAWQ,QAAX,CAAoBD,QAAQ,CAACE,IAA7B,CAAP,CADuC,CACI;AAC5C,OAFM,CAAP;AAGF;;AANQ,GAZG;AAoBbC,SAAO,EAAE;AACPR,kBAAc,GAAG;AACfR,WAAK,CAACiB,GAAN,CAAU,oCAAV,EACGC,IADH,CACQC,QAAO,IAAK;AAChB,aAAK