os from 'axios';\r\nimport Header from '@/views/userLayout/Header/header.vue';\r\nimport { ElButton } from 'element-plus';\r\n\r\nexport default {\r\n  components: { Header, ElButton },\r\n  data() {\r\n    return {\r\n      medicines: [],\r\n      types: [] // 新增 types 数组用于存储药品类型\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchMedicines();\r\n    this.fetchTypes(); // 获取药品类型\r\n  },\r\n  computed: {\r\n    filteredMedicines() {\r\n      // 根据药品类型过滤药品\r\n      return this.medicines.filter(medicine => {\r\n        return this.types.includes(medicine.type); // 只显示包含在 types 数组中的药品类型\r\n      });\r\n    }\r\n  },\r\n  methods: {\r\n    fetchMedicines() {\r\n      axios.get('http://localhost:8081/medicine/all')\r\n        .then(response => {\r\n          this.medicines = response.data.data;\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching medicines:', error);\r\n        });\r\n    },\r\n    fetchTypes() {\r\n      axios.get('http://localhost:8081/medicineType/all')\r\n        .then(response => {\r\n          this.types = response.data.data.map(type => type.name);\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching types:', error);\r\n        });\r\n    },\r\n    addToCart(medicine) {\r\n      console.log('Added to cart:', medicine);\r\n    },\r\n    // eslint-disable-next-line no-unused-vars\r\n    detail(medicine) {\r\n      // Implement the detail method\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.all-medicines {\r\n  max-width: 80%;\r\n  margin: 5% auto;\r\n  padding: 20px;\r\n}\r\n\r\n.medicine {\r\n  display: inline-block;\r\n  margin-bottom: 20px;\r\n  width: 19%;\r\n  margin-right: 1%;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n\r\n.medicine:last-child {\r\n  margin-right: 0;\r\n}\r\n\r\n.last-in-row {\r\n  margin-right: 0;\r\n}\r\n\r\n.medicine-image {\r\n  width: 50%;\r\n  height: auto;\r\n}\r\n\r\n.medicine-info {\r\n  padding: 10px;\r\n}\r\n\r\n.medicine-name {\r\n  margin-top: 0;\r\n}\r\n\r\n.medicine-price {\r\n  margin-bottom: 10px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}