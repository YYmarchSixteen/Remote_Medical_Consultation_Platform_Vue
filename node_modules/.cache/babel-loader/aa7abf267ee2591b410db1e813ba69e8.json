{"ast":null,"code":"import axios from 'axios';\nimport Header from '../userLayout/Header/header.vue';\nexport default {\n  components: {\n    Header\n  },\n\n  data() {\n    return {\n      form: {},\n      username: sessionStorage.getItem('username'),\n      userid: sessionStorage.getItem('userid'),\n      isLoading: false,\n      equipmentList: [],\n      equipmentTypes: [],\n      selectedType: '',\n      back: []\n    };\n  },\n\n  mounted() {\n    this.getEquipmentList();\n    this.fetchEquipmentTypes();\n  },\n\n  methods: {\n    changeSelect() {\n      alert(this.selectedType);\n      this.back = JSON.parse(JSON.stringify(obj));\n\n      for (var i = 0; i < this.equipmentList.length; i++) {\n        console.log(this.equipmentList[i].type);\n      }\n    },\n\n    async fetchEquipmentTypes() {\n      try {\n        const res = await axios.get('http://localhost:8080/equipmenttypeList');\n        this.equipmentTypes.push(...res.data);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async getEquipmentList() {\n      try {\n        const response = await axios.get('http://localhost:8080/equipmentList');\n        this.equipmentList = response.data;\n      } catch (error) {\n        console.error(error);\n      }\n    },\n\n    updateEquipmentList(newList) {\n      this.equipmentList = newList;\n    },\n\n    detail(item) {\n      this.$router.push({\n        path: '/detail',\n        query: {\n          id: item.id\n        }\n      });\n    },\n\n    async insertCollect(item) {\n      if (this.isLoading) {\n        return;\n      }\n\n      try {\n        this.isLoading = true;\n        const response = await axios.get('http://localhost:8080/collect', {\n          params: {\n            userid: sessionStorage.getItem('userid'),\n            equipmentid: item.id\n          }\n        });\n\n        if (response.status === 200) {\n          if (response.data === '当前商品已收藏') {\n            this.$message.error('当前商品已收藏');\n          } else if (response.data === '收藏成功') {\n            this.$message.success('收藏成功');\n            item.isCollected = true;\n          } else if (response.data === '收藏失败，请稍后再试') {\n            this.$message.error('收藏失败，请稍后再试');\n          }\n        }\n      } catch (error) {\n        console.error(error);\n      } finally {\n        this.isLoading = false;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+BA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iCAAnB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVD;AADU,GADC;;AAIbE,MAAI,GAAG;AACL,WAAO;AACLC,UAAI,EAAE,EADD;AAELC,cAAQ,EAAEC,cAAc,CAACC,OAAf,CAAuB,UAAvB,CAFL;AAGLC,YAAM,EAAEF,cAAc,CAACC,OAAf,CAAuB,QAAvB,CAHH;AAILE,eAAS,EAAE,KAJN;AAKLC,mBAAa,EAAE,EALV;AAMLC,oBAAc,EAAE,EANX;AAOLC,kBAAY,EAAE,EAPT;AAQLC,UAAI,EAAC;AARA,KAAP;AAUD,GAfY;;AAiBbC,SAAO,GAAG;AACR,SAAKC,gBAAL;AACA,SAAKC,mBAAL;AAED,GArBY;;AAuBbC,SAAO,EAAE;AACPC,gBAAY,GAAG;AACbC,WAAK,CAAC,KAAKP,YAAN,CAAL;AACA,WAAKC,IAAL,GAAUO,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,SAAL,CAAeC,GAAf,CAAX,CAAV;;AACA,WAAI,IAAIC,CAAC,GAAC,CAAV,EAAYA,CAAC,GAAC,KAAKd,aAAL,CAAmBe,MAAjC,EAAwCD,CAAC,EAAzC,EAA4C;AAC1CE,eAAO,CAACC,GAAR,CAAY,KAAKjB,aAAL,CAAmBc,CAAnB,EAAsBI,IAAlC;AACF;AACD,KAPM;;AAQP,UAAMZ,mBAAN,GAA4B;AAC1B,UAAI;AACF,cAAMa,GAAE,GAAI,MAAM7B,KAAK,CAAC8B,GAAN,CAAU,yCAAV,CAAlB;AACA,aAAKnB,cAAL,CAAoBoB,IAApB,CAAyB,GAAGF,GAAG,CAAC1B,IAAhC;AACF,OAHA,CAGE,OAAO6B,KAAP,EAAc;AACdN,eAAO,CAACC,GAAR,CAAYK,KAAZ;AACF;AACD,KAfM;;AAgBP,UAAMjB,gBAAN,GAAyB;AACvB,UAAI;AACF,cAAMkB,QAAO,GAAI,MAAMjC,KAAK,CAAC8B,GAAN,CAAU,qCAAV,CAAvB;AACA,aAAKpB,aAAL,GAAqBuB,QAAQ,CAAC9B,IAA9B;AACF,OAHA,CAGE,OAAO6B,KAAP,EAAc;AACdN,eAAO,CAACM,KAAR,CAAcA,KAAd;AACF;AACD,KAvBM;;AAyBPE,uBAAmB,CAACC,OAAD,EAAU;AAC3B,WAAKzB,aAAL,GAAqByB,OAArB;AACD,KA3BM;;AA6BPC,UAAM,CAACC,IAAD,EAAO;AACX,WAAKC,OAAL,CAAaP,IAAb,CAAkB;AAChBQ,YAAI,EAAE,SADU;AAEhBC,aAAK,EAAE;AACLC,YAAE,EAAEJ,IAAI,CAACI;AADJ;AAFS,OAAlB;AAMD,KApCM;;AAsCP,UAAMC,aAAN,CAAoBL,IAApB,EAA0B;AACxB,UAAI,KAAK5B,SAAT,EAAoB;AAClB;AACF;;AACA,UAAI;AACF,aAAKA,SAAL,GAAiB,IAAjB;AACA,cAAMwB,QAAO,GAAI,M