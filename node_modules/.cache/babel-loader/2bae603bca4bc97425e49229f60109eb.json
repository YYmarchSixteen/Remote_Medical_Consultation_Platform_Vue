{"ast":null,"code":"import \"core-js/modules/web.dom-exception.stack.js\";\nimport \"core-js/modules/es.typed-array.at.js\";\nimport \"core-js/modules/es.typed-array.set.js\";\nimport { get } from '@/utils/request';\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 15,\n      //默认一页显示 条\n      dialogVisible: false,\n      dialogVisible1: false,\n      info2: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      list: [],\n      adg: []\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    get('http://localhost:8080/equipmentList').then(res => {\n      console.log(res);\n      this.list = res.data;\n    });\n  },\n\n  methods: {\n    // 前端使用 FormData 对象上传图片\n    handleUploadChange(file) {\n      const reader = new FileReader();\n      reader.readAsDataURL(file.raw);\n\n      reader.onload = () => {\n        const dataUrl = reader.result;\n        const blobBin = atob(dataUrl.split(',')[1]);\n        const array = [];\n\n        for (let i = 0; i < blobBin.length; i++) {\n          array.push(blobBin.charCodeAt(i));\n        }\n\n        const fileBlob = new Blob([new Uint8Array(array)], {\n          type: 'image/jpeg'\n        });\n        const formData = new FormData();\n        formData.append('image', fileBlob, file.name); // 使用 axios 发送 POST 请求上传图片\n\n        axios.post('/upload/image', formData).then(response => {\n          this.info2.picture = response.data.url;\n        }).catch(error => console.error(error));\n      };\n    },\n\n    del(data) {\n      console.log(data.row.id);\n      get('http://localhost:8080/deleteEquipment', {\n        id: data.row.id\n      }).then(() => {\n        ElMessage({\n          message: '删除成功！',\n          type: 'success'\n        });\n      });\n      window.location.reload(); //刷新\n    },\n\n    add() {\n      this.dialogVisible1 = !this.dialogVisible1;\n    },\n\n    insert() {\n      this.dialogVisible1 = !this.dialogVisible1;\n      get('http://localhost:8080/insertEquipment', this.info1).then(res => {\n        console.log(res);\n\n        if (res.data == '添加成功') {\n          ElMessage({\n            message: '添加成功',\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '添加失败',\n            type: 'danger'\n          });\n        }\n      });\n    },\n\n    handleClick(data) {\n      this.info2 = data.row;\n      this.dialogVisible = !this.dialogVisible;\n    },\n\n    edit() {\n      get('http://localhost:8080/updateEquipment', this.info2).then(res => {\n        console.log(res);\n\n        if (res.data == '修改成功') {\n          ElMessage({\n            message: '修改成功',\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '修改失败',\n            type: 'danger'\n          });\n        }\n      });\n    },\n\n    handleSizeChange: function (size) {\n      //一页显示多少条\n      this.pagesize = size;\n    },\n    handleCurrentChange: function (currentPage) {\n      //页码更改方法\n      this.currentPage = currentPage;\n    }\n  }\n};","map":{"version":3,"mappings":";;;AAqIA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,cAA1B;AACA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,CADR;AACW;AAChBC,cAAQ,EAAE,EAFL;AAES;AACdC,mBAAa,EAAE,KAHV;AAILC,oBAAc,EAAE,KAJX;AAKLC,WAAK,EAAE,EALF;AAKM;AACXC,WAAK,EAAE,EANF;AAMM;AACXC,UAAI,EAAE,EAPD;AAQLC,SAAG,EAAE;AARA,KAAP;AAUD,GAZY;;AAab;AACAC,SAAO,GAAG;AACRX,OAAG,CAAC,qCAAD,CAAH,CAA2CY,IAA3C,CAAiDC,GAAD,IAAS;AACvDC,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,WAAKJ,IAAL,GAAYI,GAAG,CAACX,IAAhB;AACD,KAHD;AAID,GAnBY;;AAoBbc,SAAO,EAAE;AACP;AACJC,sBAAkB,CAACC,IAAD,EAAO;AACvB,YAAMC,MAAK,GAAI,IAAIC,UAAJ,EAAf;AACAD,YAAM,CAACE,aAAP,CAAqBH,IAAI,CAACI,GAA1B;;AACAH,YAAM,CAACI,MAAP,GAAgB,MAAM;AACpB,cAAMC,OAAM,GAAIL,MAAM,CAACM,MAAvB;AACA,cAAMC,OAAM,GAAIC,IAAI,CAACH,OAAO,CAACI,KAAR,CAAc,GAAd,EAAmB,CAAnB,CAAD,CAApB;AACA,cAAMC,KAAI,G