{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      doctor: null\n    };\n  },\n\n  mounted() {\n    this.fetchDoctor();\n  },\n\n  methods: {\n    async fetchDoctor() {\n      try {\n        const response = await axios.get('http://localhost:8081/doctor/findById?doctorId=4');\n        this.doctor = response.data.data;\n      } catch (error) {\n        console.error('Error fetching doctor:', error);\n      }\n    },\n\n    async updateDoctor() {\n      try {\n        const params = new URLSearchParams();\n        params.append('introduction', this.updateForm.introduction);\n        params.append('telephone', this.updateForm.telephone);\n        params.append('doctorId', 13);\n        await axios.put('http://localhost:8081/doctor/changeInfo', params);\n        alert('用户信息已更新');\n      } catch (error) {\n        console.error('Error updating doctor:', error);\n        alert('更新用户信息时出错');\n      }\n    },\n\n    re() {\n      window.location.reload();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6CA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE;AADH,KAAP;AAGD,GALY;;AAMbC,SAAO,GAAG;AACR,SAAKC,WAAL;AACD,GARY;;AASbC,SAAO,EAAE;AACP,UAAMD,WAAN,GAAoB;AAClB,UAAI;AACF,cAAME,QAAO,GAAI,MAAMN,KAAK,CAACO,GAAN,CAAU,kDAAV,CAAvB;AACA,aAAKL,MAAL,GAAcI,QAAQ,CAACL,IAAT,CAAcA,IAA5B;AACF,OAHA,CAGE,OAAOO,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,wBAAd,EAAwCA,KAAxC;AACF;AACD,KARM;;AASP,UAAME,YAAN,GAAqB;AACnB,UAAI;AACF,cAAMC,MAAK,GAAI,IAAIC,eAAJ,EAAf;AACAD,cAAM,CAACE,MAAP,CAAc,cAAd,EAA8B,KAAKC,UAAL,CAAgBC,YAA9C;AACAJ,cAAM,CAACE,MAAP,CAAc,WAAd,EAA2B,KAAKC,UAAL,CAAgBE,SAA3C;AACAL,cAAM,CAACE,MAAP,CAAc,UAAd,EAA0B,EAA1B;AACA,cAAMb,KAAK,CAACiB,GAAN,CAAU,yCAAV,EAAqDN,MAArD,CAAN;AACAO,aAAK,CAAC,SAAD,CAAL;AACF,OAPA,CAOE,OAAOV,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,wBAAd,EAAwCA,KAAxC;AACAU,aAAK,CAAC,WAAD,CAAL;AACF;AACD,KArBM;;AAsBPC,MAAE,GAAG;AACHC,YAAM,CAACC,QAAP,CAAgBC,MAAhB;AACD;;AAxBM;AATI,CAAf","names":["axios","data","doc