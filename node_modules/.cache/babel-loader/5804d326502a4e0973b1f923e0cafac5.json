{"ast":null,"code":"import axios from 'axios';\nimport { reactive, onMounted } from 'vue';\nimport Header from '../userLayout/Header/header.vue';\nexport default {\n  components: {\n    Header\n  },\n\n  data() {\n    return {\n      form: {},\n      username: sessionStorage.getItem('username'),\n      userid: sessionStorage.getItem('userid'),\n      isLoading: false,\n      equipmentList: [],\n      equipmentTypes: []\n    };\n  },\n\n  setup() {\n    const equipmentList = reactive([]); // 发起请求并赋值给equipmentList\n\n    const getEquipmentList = async () => {\n      try {\n        const response = await axios.get('http://localhost:8080/equipmentList');\n        equipmentList.splice(0, equipmentList.length, ...response.data);\n      } catch (error) {\n        console.error(error);\n      }\n    }; // 组件挂载时调用getEquipmentList方法获取数据\n\n\n    onMounted(() => {\n      fetchEquipmentTypes();\n      doSomethingElse(); // 每当组件挂载时执行 doSomethingElse 方法\n    });\n    return {\n      // eslint-disable-next-line vue/no-dupe-keys\n      equipmentList\n    };\n  },\n\n  methods: {\n    async fetchEquipmentTypes() {\n      try {\n        const res = await axios.get('http://localhost:8080/equipmenttypeList');\n        this.equipmentTypes.push(...res.data);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    updateEquipmentList(newList) {\n      this.equipmentList = newList;\n    },\n\n    detail(item) {\n      this.$router.push({\n        path: '/detail',\n        query: {\n          id: item.id\n        }\n      });\n    },\n\n    async insertCollect(item) {\n      if (this.isLoading) {\n        return;\n      }\n\n      try {\n        this.isLoading = true;\n        const response = await axios.get('http://localhost:8080/collect', {\n          params: {\n            userid: sessionStorage.getItem('userid'),\n            equipmentid: item.id\n          }\n        });\n\n        if (response.status === 200) {\n          if (response.data === '当前商品已收藏') {\n            this.$message.error('当前商品已收藏');\n          } else if (response.data === '收藏成功') {\n            this.$message.success('收藏成功');\n            item.isCollected = true;\n          } else if (response.data === '收藏失败，请稍后再试') {\n            this.$message.error('收藏失败，请稍后再试');\n          }\n        }\n      } catch (error) {\n        console.error(error);\n      } finally {\n        this.isLoading = false;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+BA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAoC,KAApC;AACA,OAAOC,MAAP,MAAmB,iCAAnB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVD;AADU,GADC;;AAIbE,MAAI,GAAG;AACL,WAAO;AACLC,UAAI,EAAE,EADD;AAELC,cAAQ,EAAEC,cAAc,CAACC,OAAf,CAAuB,UAAvB,CAFL;AAGLC,YAAM,EAAEF,cAAc,CAACC,OAAf,CAAuB,QAAvB,CAHH;AAILE,eAAS,EAAE,KAJN;AAKLC,mBAAa,EAAE,EALV;AAMLC,oBAAc,EAAC;AANV,KAAP;AASD,GAdY;;AAebC,OAAK,GAAG;AAGN,UAAMF,aAAY,GAAIX,QAAQ,CAAC,EAAD,CAA9B,CAHM,CAIN;;AACA,UAAMc,gBAAe,GAAI,YAAY;AACnC,UAAI;AACF,cAAMC,QAAO,GAAI,MAAMhB,KAAK,CAACiB,GAAN,CAAU,qCAAV,CAAvB;AACAL,qBAAa,CAACM,MAAd,CAAqB,CAArB,EAAwBN,aAAa,CAACO,MAAtC,EAA8C,GAAGH,QAAQ,CAACX,IAA1D;AACF,OAHA,CAGE,OAAOe,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAcA,KAAd;AACF;AACD,KAPD,CALM,CAcN;;;AAEAlB,aAAS,CAAC,MAAM;AACdoB,yBAAmB;AACnBC,qBAAe,GAFD,CAEK;AACpB,KAHQ,CAAT;AAKA,WAAO;AACL;AACAX;AAFK,KAAP;AAKD,GAzCY;;AA0CbY,SAAO,EAAE;AACP,UAAMF,mBAAN,GAA4B;AAC1B,UAAI;AACF,cAAMG,GAAE,GAAI,MAAMzB,KAAK,CAACiB,GAAN,CAAU,yCAAV,CAAlB;AACA,aAAKJ,cAAL,CAAoBa,IAApB,CAAyB,GAAGD,GAAG,CAACpB,IAAhC;AACF,OAHA,CAGE,OAAOe,KAAP,EAAc;AACdC,eAAO,CAACM,GAAR,CAAYP,KAAZ;AACF;AACD,KARM;;AAUPQ,uBAAmB,CAACC,OAAD,EAAU;AAC3B,WAAKjB,aAAL,GAAqBiB,OAArB;AACD,KAZM;;AAaPC,UAAM,CAACC,IAAD,EAAO;AACX,WAAKC,OAAL,CAAaN,IAAb,CAAkB;AAChBO,YAAI,EAAE,SADU;AAEhBC,aAAK,EAAE;AACLC,YAAE,EAAEJ,IAAI,CAACI;AADJ;AAFS,OAAlB;AAMD,KApBM;;AAqBP,UAAMC,aAAN,CAAoBL,IAApB,EAA0B;AACxB,UAAI,KAAKpB,SAAT,EAAoB;AAClB;AACF;;AACA,UAAI;AACF,aAAKA,SAAL,GAAiB,IAAjB;AACA,cAAMK,QAAO,GAAI,MAAMhB,KAAK,CAACiB,GAAN,CAAU,+BAAV,EAA2C;AAChEoB,gBAAM,EAAE;AACN3B,kBAAM,EAAEF,cAAc,CAACC,OAAf,CAAuB,QAAvB,CADF;AAEN6B,uBAAW,EAAEP,IAAI,CAACI;AAFZ;AADwD,SAA3C,CAAvB;;AAMA,YAAInB,QAAQ,CAACuB,MAAT,KAAoB,GAAxB,EAA6B;AAC3B,cAAIvB,QAAQ,CAACX,IAAT,KAAkB,SAAtB,EAAiC;AAC/B,iBAAKmC,QAAL,CAAcpB,KAAd,CAAoB,SAApB;AACF,WAFA,MAEO,IAAIJ,QAAQ,CAACX,IAAT,KAAkB,MAAtB,EAA8B;AACnC,iBAAKmC,QAAL,CAAcC,OAAd,CAAsB,MAAtB;AACAV,gBAAI,CAACW,WAAL,GAAmB,IAAnB;AACF,WAHO,MAGA,IAAI1B,QAAQ,CAACX,IAAT,KAAkB,YAAtB,EAAoC;AACzC,iBAAKmC,QAAL,CAAcpB,KAAd,CAAoB,YAApB;AACF;AACF;AACF,OAlBA,CAkBE,OAAOA,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAcA,KAAd;AACF,OApBA,SAoBU;AACR,aAAKT,SAAL,GAAiB,KAAjB;AACF;AACF;;AAhDO;AA1CI,CAAf","names":["axios","reactive","onMounted","Header","components","data","form","username","sessionStorage","getItem","userid","isLoading","equipmentList","equipmentTypes","setup","getEquipmentList","response","get","splice","length","error","console","fetchEquipmentTypes","doSomethingElse","methods","res","push","log","updateEquipmentList","newList","detail","item","$router","path","query","id","insertCollect","params","equipmentid","status","$message","success","isCollected"],"sourceRoot":"","sources":["D:\\Dev\\Front\\demo01\\src\\views\\User\\userIndex.vue"],"sourcesContent":["<template>\r\n  <div class=\"header\">\r\n    <Header :update-equipment-list=\"updateEquipmentList\"></Header>\r\n  </div>\r\n  <div class=\"body\">\r\n    <el-select v-model=\"selectedType\" placeholder=\"请选择设备类型\" @change=\"changetype\">\r\n            <el-option v-for=\"type in equipmentTypes\" :key=\"type.id\" :label=\"type.typename\" :value=\"type.id\"></el-option>\r\n          </el-select>\r\n    <div class=\"equipment-list\">\r\n      <div class=\"equipment-item\" v-for=\"(item, index) in equipmentList\" :key=\"index\">\r\n        <div class=\"img-wrapper\">\r\n          <img :src=\"item.picture\" alt=\"\" style=\"width: 200px; height: auto;\">\r\n        </div>\r\n        <div class=\"info-wrapper\">\r\n          <div class=\"title\">{{ item.equipmentname }}</div>\r\n          <div class=\"price\">单价: ￥{{ item.price }}</div>\r\n          <div class=\"feature\">\r\n            <h2>功能:</h2> {{ item.feature }}\r\n          </div>\r\n          <div class=\"material\">\r\n            <h2>材质：</h2>{{ item.material }}\r\n          </div>\r\n          <button class=\"buy-button\" @click=\"detail(item)\">查看详情</button>\r\n          <button class=\"collect-button\" @click=\"insertCollect(item)\">加入收藏</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n  \r\n<script>\r\nimport axios from 'axios';\r\nimport { reactive, onMounted } from 'vue';\r\nimport Header from '../userLayout/Header/header.vue';\r\n\r\nexport default {\r\n  components: {\r\n    Header\r\n  },\r\n  data() {\r\n    return {\r\n      form: {},\r\n      username: sessionStorage.getItem('username'),\r\n      userid: sessionStorage.getItem('userid'),\r\n      isLoading: false,\r\n      equipmentList: [],\r\n      equipmentTypes:[]\r\n\r\n    }\r\n  },\r\n  setup() {    \r\n\r\n\r\n    const equipmentList = reactive([]);\r\n    // 发起请求并赋值给equipmentList\r\n    const getEquipmentList = async () => {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/equipmentList');\r\n        equipmentList.splice(0, equipmentList.length, ...response.data);\r\n      } catch (error) {\r\n        console.error(error);\r\n      }\r\n    };\r\n\r\n    // 组件挂载时调用getEquipmentList方法获取数据\r\n\r\n    onMounted(() => {\r\n      fetchEquipmentTypes();\r\n      doSomethingElse(); // 每当组件挂载时执行 doSomethingElse 方法\r\n    });\r\n\r\n    return {\r\n      // eslint-disable-next-line vue/no-dupe-keys\r\n      equipmentList,\r\n\r\n    };\r\n  },\r\n  methods: {\r\n    async fetchEquipmentTypes() {\r\n      try {\r\n        const res = await axios.get('http://localhost:8080/equipmenttypeList');\r\n        this.equipmentTypes.push(...res.data);\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n\r\n    updateEquipmentList(newList) {\r\n      this.equipmentList = newList;\r\n    },\r\n    detail(item) {\r\n      this.$router.push({\r\n        path: '/detail',\r\n        query: {\r\n          id: item.id\r\n        }\r\n      });\r\n    },\r\n    async insertCollect(item) {\r\n      if (this.isLoading) {\r\n        return;\r\n      }\r\n      try {\r\n        this.isLoading = true;\r\n        const response = await axios.get('http://localhost:8080/collect', {\r\n          params: {\r\n            userid: sessionStorage.getItem('userid'),\r\n            equipmentid: item.id,\r\n          }\r\n        });\r\n        if (response.status === 200) {\r\n          if (response.data === '当前商品已收藏') {\r\n            this.$message.error('当前商品已收藏');\r\n          } else if (response.data === '收藏成功') {\r\n            this.$message.success('收藏成功');\r\n            item.isCollected = true;\r\n          } else if (response.data === '收藏失败，请稍后再试') {\r\n            this.$message.error('收藏失败，请稍后再试');\r\n          }\r\n        }\r\n      } catch (error) {\r\n        console.error(error);\r\n      } finally {\r\n        this.isLoading = false;\r\n      }\r\n    }\r\n\r\n  }\r\n\r\n  ,\r\n};\r\n</script>\r\n  \r\n<style scoped>\r\nh2 {\r\n  font-weight: bolder;\r\n}\r\n\r\n.body {\r\n  margin-top: 100px;\r\n}\r\n\r\n.equipment-item {\r\n  display: flex;\r\n  margin-bottom: 20px;\r\n  background-color: #fff;\r\n  box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.1);\r\n  border: 1px solid #cccccc;\r\n  /* 添加淡黑色边框 */\r\n}\r\n\r\n.img-wrapper {\r\n  width: 35%;\r\n  padding: 10px 10px 10px 200px;\r\n  /* 左右留白150px，上下10px */\r\n  margin: 0 10px 0 0;\r\n  /* 水平间距10px */\r\n}\r\n\r\n.img-wrapper img {\r\n  display: block;\r\n  width: 100%;\r\n  height: auto;\r\n}\r\n\r\n.info-wrapper {\r\n  width: 165%;\r\n  padding: 10px 150px 10px 10px;\r\n  /* 左右留白150px，上下10px */\r\n  margin: 0 0 0 10px;\r\n  /* 水平间距10px */\r\n}\r\n\r\n.title {\r\n  font-weight: bold;\r\n  font-size: 18px;\r\n  color: #333;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.price {\r\n  font-size: 16px;\r\n  color: red;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.feature,\r\n.material {\r\n  font-size: 14px;\r\n  color: #555;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.buy-button {\r\n  padding: 8px 18px;\r\n  border: none;\r\n  background-color: #1bb1c4;\r\n  color: #fff;\r\n  cursor: pointer;\r\n}\r\n\r\n.collect-button {\r\n  padding: 8px 18px;\r\n  border: none;\r\n  background-color: #da7bc4;\r\n  color: #fff;\r\n  cursor: pointer;\r\n  margin-left: 10px;\r\n}\r\n\r\n.buy-button:hover {\r\n  background-color: #ff9f3e;\r\n}\r\n</style>\r\n  "]},"metadata":{},"sourceType":"module"}