{"ast":null,"code":"import axios from 'axios';\nimport { ElDialog } from 'element-plus';\nexport default {\n  components: {\n    ElDialog\n  },\n\n  data() {\n    return {\n      notices: [],\n      dialogVisible: false,\n      updateForm: {\n        title: '',\n        content: ''\n      },\n      addForm: {\n        noticeId: this.notices.noticeId,\n        title: '',\n        content: ''\n      }\n    };\n  },\n\n  mounted() {\n    this.fetchNotices();\n  },\n\n  methods: {\n    async fetchNotices() {\n      try {\n        const response = await axios.get('http://localhost:8081/notice/all');\n        this.notices = response.data.data;\n      } catch (error) {\n        console.error('Error fetching notices:', error);\n      }\n    },\n\n    async disableNotice(notice) {\n      try {\n        await axios.put(`http://localhost:8081/notice/disabled?noticeId=${notice.noticeId}`);\n        this.$message.success('当前公告已禁用');\n        this.fetchNotices();\n      } catch (error) {\n        console.error('Error disabling notice:', error);\n      }\n    },\n\n    async activateNotice(notice) {\n      try {\n        await axios.put(`http://localhost:8081/notice/activate?noticeId=${notice.noticeId}`);\n        this.$message.success('当前公告已激活');\n        this.fetchNotices();\n      } catch (error) {\n        console.error('Error activating notice:', error);\n      }\n    },\n\n    showDialog() {\n      this.dialogVisible = true;\n    },\n\n    async updateNotice() {\n      try {\n        const params = new URLSearchParams();\n        params.append('title', this.updateForm.title);\n        params.append('content', this.updateForm.content);\n        params.append('noticeId', this.updateForm.noticeId);\n        axios.put('http://localhost:8081/notice/update', params);\n        this.$message.success('编辑成功');\n        this.dialogVisible = false;\n        await this.fetchNotices(); //window.location.reload()\n      } catch (error) {\n        console.error('Error adding Notice:', error);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmDA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,QAAT,QAAyB,cAAzB;AAEA,eAAe;AACbC,YAAU,EAAE;AAAED;AAAF,GADC;;AAEbE,MAAI,GAAG;AACL,WAAO;AACLC,aAAO,EAAE,EADJ;AAELC,mBAAa,EAAE,KAFV;AAGLC,gBAAU,EAAE;AACVC,aAAK,EAAE,EADG;AAEVC,eAAO,EAAE;AAFC,OAHP;AAOLC,aAAO,EAAE;AACPC,gBAAQ,EAAC,KAAKN,OAAL,CAAaM,QADf;AAEPH,aAAK,EAAE,EAFA;AAGPC,eAAO,EAAE;AAHF;AAPJ,KAAP;AAaD,GAhBY;;AAiBbG,SAAO,GAAG;AACR,SAAKC,YAAL;AACD,GAnBY;;AAoBbC,SAAO,EAAE;AACP,UAAMD,YAAN,GAAqB;AACnB,UAAI;AACF,cAAME,QAAO,GAAI,MAAMd,KAAK,CAACe,GAAN,CAAU,kCAAV,CAAvB;AACA,aAAKX,OAAL,GAAeU,QAAQ,CAACX,IAAT,CAAcA,IAA7B;AACF,OAHA,CAGE,OAAOa,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,yBAAd,EAAyCA,KAAzC;AACF;AACD,KARM;;AASP,UAAME,aAAN,CAAoBC,MAApB,EAA4B;AAC1B,UAAI;AACF,cAAMnB,KAAK,CAACoB,GAAN,CAAW,kDAAiDD,MAAM,CAACT,QAAS,EAA5E,CAAN;AACA,aAAKW,QAAL,CAAcC,OAAd,CAAsB,SAAtB;AACA,aAAKV,YAAL;AACF,OAJA,CAIE,OAAOI,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,yBAAd,EAAyCA,KAAzC;AACF;AACD,KAjBM;;AAkBP,UAAMO,cAAN,CAAqBJ,MAArB,EAA6B;AAC3B,UAAI;AACF,cAAMnB,KAAK,CAACoB,GAAN,CAAW,kDAAiDD,MAAM,CAACT,QAAS,EAA5E,CAAN;AACA,aAAKW,QAAL,CAAcC,OAAd,CAAsB,SAAtB;AACA,aAAKV,YAAL;AACF,OAJA,CAIE,OAAOI,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,0BAAd,EAA0CA,KAA1C;AACF;AACD,KA1BM;;AA2BPQ,cAAU,GAAG;AACX,WAAKnB,aAAL,GAAqB,IAArB;AACD,KA7BM;;AA8BP,UAAMoB,YAAN,GAAqB;AACnB,UAAI;AACF,cAAMC,MAAK,GAAI,IAAIC,eAAJ,EAAf;AACAD,cAAM,CAACE,MAAP,CAAc,OAAd,EAAuB,KAAKtB,UAAL,CAAgBC,KAAvC;AACAmB,cAAM,CAACE,MAAP,CAAc,SAAd,EAAyB,KAAKtB,UAAL,CAAgBE,OAAzC;AACAkB,cAAM,CAACE,MAAP,CAAc,UAAd,EAA0B,KAAKtB,UAAL,CAAgBI,QAA1C;AACAV,aAAK,CAACoB,GAAN,CAAU,qCAAV,EAAiDM,MAAjD;AACA,aAAKL,QAAL,CAAcC,OAAd,CAAsB,MAAtB;AACA,aAAKjB,aAAL,GAAqB,KAArB;AACA,cAAM,KAAKO,YAAL,EAAN,CARE,CASF;AACF,OAVA,CAUE,OAAOI,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,sBAAd,EAAsCA,KAAtC;AACF;AACF;;AA5CO;AApBI,CAAf","names":["axios","ElDialog","components","data","notices","dialogVisible","updateForm","title","content","addForm","noticeId","mounted","fetchNotices","methods","response","get","error","console","disableNotice","notice","put","$message","success","activateNotice","showDialog","updateNotice","params","URLSearchParams","append"],"sourceRoot":"","sources":["D:\\Dev\\Front\\Remote_Medical_Consultation_Platform_Vue\\src\\views\\Admin\\notice.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h1>Notice List</h1>\r\n    <el-table :data=\"notices\" border stripe>\r\n      <el-table-column prop=\"noticeId\" label=\"公告ID\"></el-table-column>\r\n      <el-table-column prop=\"receiver\" label=\"接收者\">\r\n        <template v-slot=\"scope\">\r\n          <span v-if=\"scope.row.receiver === 1\">用户</span>\r\n          <span v-else-if=\"scope.row.receiver === 2\">医生</span>\r\n          <span v-else-if=\"scope.row.receiver === 3\">医生和用户</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"title\" label=\"标题\"></el-table-column>\r\n      <el-table-column prop=\"content\" label=\"内容\" width=\"200\"></el-table-column>\r\n      <el-table-column prop=\"status\" label=\"当前状态\">\r\n        <template v-slot=\"scope\">\r\n          <span v-if=\"scope.row.status === 1\">使用中</span>\r\n          <span v-else-if=\"scope.row.status === 0\">已禁用</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"sendTime\" label=\"发送时间\"></el-table-column>\r\n      <el-table-column label=\"操作\" width=\"200\">\r\n        <template #default=\"scope\">\r\n          <el-button v-if=\"scope.row.status === 1\" type=\"danger\" size=\"small\" @click=\"disableNotice(scope.row)\">\r\n            点击禁用\r\n          </el-button>\r\n          <el-button v-else-if=\"scope.row.status === 0\" type=\"success\" size=\"small\" @click=\"activateNotice(scope.row)\">\r\n            激活公告\r\n          </el-button>\r\n          <el-button type=\"primary\" @click=\"showDialog(scope.row.noticeId)\" size=\"small\">编辑公告</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-dialog title=\"编辑公告\" v-model=\"dialogVisible\">\r\n      <el-form ref=\"updateForm\" :model=\"updateForm\" label-width=\"100px\">\r\n        <el-form-item label=\"标题\" prop=\"title\">\r\n          <el-input v-model=\"updateForm.title\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"公告内容\" prop=\"content\">\r\n          <el-input v-model=\"updateForm.content\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span id=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"updateNotice\">添加</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport { ElDialog } from 'element-plus'\r\n\r\nexport default {\r\n  components: { ElDialog },\r\n  data() {\r\n    return {\r\n      notices: [],\r\n      dialogVisible: false,\r\n      updateForm: {\r\n        title: '',\r\n        content: ''\r\n      },\r\n      addForm: {\r\n        noticeId:this.notices.noticeId,\r\n        title: '',\r\n        content: ''\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchNotices()\r\n  },\r\n  methods: {\r\n    async fetchNotices() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8081/notice/all')\r\n        this.notices = response.data.data\r\n      } catch (error) {\r\n        console.error('Error fetching notices:', error)\r\n      }\r\n    },\r\n    async disableNotice(notice) {\r\n      try {\r\n        await axios.put(`http://localhost:8081/notice/disabled?noticeId=${notice.noticeId}`)\r\n        this.$message.success('当前公告已禁用')\r\n        this.fetchNotices()\r\n      } catch (error) {\r\n        console.error('Error disabling notice:', error)\r\n      }\r\n    },\r\n    async activateNotice(notice) {\r\n      try {\r\n        await axios.put(`http://localhost:8081/notice/activate?noticeId=${notice.noticeId}`)\r\n        this.$message.success('当前公告已激活')\r\n        this.fetchNotices()\r\n      } catch (error) {\r\n        console.error('Error activating notice:', error)\r\n      }\r\n    },\r\n    showDialog() {\r\n      this.dialogVisible = true\r\n    },\r\n    async updateNotice() {\r\n      try {\r\n        const params = new URLSearchParams()\r\n        params.append('title', this.updateForm.title)\r\n        params.append('content', this.updateForm.content)\r\n        params.append('noticeId', this.updateForm.noticeId)\r\n        axios.put('http://localhost:8081/notice/update', params)\r\n        this.$message.success('编辑成功')\r\n        this.dialogVisible = false\r\n        await this.fetchNotices()\r\n        //window.location.reload()\r\n      } catch (error) {\r\n        console.error('Error adding Notice:', error)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  margin: 20px auto;\r\n  max-width: 1000px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}