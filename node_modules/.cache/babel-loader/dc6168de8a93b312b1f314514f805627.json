{"ast":null,"code":"import axios from 'axios';\nimport { ElDialog } from 'element-plus';\nexport default {\n  components: {\n    ElDialog\n  },\n\n  data() {\n    return {\n      admins: [],\n      dialogVisible: false,\n      addAdminForm: {\n        username: ''\n      }\n    };\n  },\n\n  mounted() {\n    this.fetchAdmins();\n  },\n\n  methods: {\n    async fetchAdmins() {\n      try {\n        const response = await axios.get('http://localhost:8081/admin/all');\n        this.admins = response.data.data;\n        window.local.reload;\n      } catch (error) {\n        console.error('Error fetching admins:', error);\n      }\n    },\n\n    async disableAdmin(admin) {\n      try {\n        await axios.put(`http://localhost:8081/admin/disable?adminId=${admin.adminId}`);\n        this.$message.success('当前账号已禁用');\n        this.fetchAdmins();\n      } catch (error) {\n        console.error('Error disabling admin:', error);\n      }\n    },\n\n    async activateAdmin(admin) {\n      try {\n        await axios.put(`http://localhost:8081/admin/active?adminId=${admin.adminId}`);\n        this.$message.success('当前账号已激活');\n        this.fetchAdmins();\n      } catch (error) {\n        console.error('Error activating admin:', error);\n      }\n    },\n\n    showAddAdminDialog() {\n      this.dialogVisible = true;\n    },\n\n    async addAdmin() {\n      try {\n        const params = new URLSearchParams();\n        params.append('username', this.addAdminForm.username);\n        params.append('role', '0');\n        axios.post('http://localhost:8081/admin/add', params);\n        this.$message.success('管理员添加成功');\n        this.dialogVisible = false;\n        await this.fetchAdmins();\n      } catch (error) {\n        console.error('Error adding admin:', error);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,QAAT,QAAyB,cAAzB;AAGA,eAAe;AACbC,YAAU,EAAE;AACVD;AADU,GADC;;AAIbE,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,EADH;AAELC,mBAAa,EAAE,KAFV;AAGLC,kBAAY,EAAE;AACZC,gBAAQ,EAAE;AADE;AAHT,KAAP;AAOD,GAZY;;AAabC,SAAO,GAAG;AACR,SAAKC,WAAL;AACD,GAfY;;AAgBbC,SAAO,EAAE;AACP,UAAMD,WAAN,GAAoB;AAClB,UAAI;AACF,cAAME,QAAO,GAAI,MAAMX,KAAK,CAACY,GAAN,CAAU,iCAAV,CAAvB;AACA,aAAKR,MAAL,GAAcO,QAAQ,CAACR,IAAT,CAAcA,IAA5B;AACAU,cAAM,CAACC,KAAP,CAAaC,MAAb;AACF,OAJA,CAIE,OAAOC,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,wBAAd,EAAwCA,KAAxC;AACF;AACD,KATM;;AAUP,UAAME,YAAN,CAAmBC,KAAnB,EAA0B;AACxB,UAAI;AACF,cAAMnB,KAAK,CAACoB,GAAN,CAAW,+CAA8CD,KAAK,CAACE,OAAQ,EAAvE,CAAN;AACA,aAAKC,QAAL,CAAcC,OAAd,CAAsB,SAAtB;AACA,aAAKd,WAAL;AACF,OAJA,CAIE,OAAOO,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,wBAAd,EAAwCA,KAAxC;AACF;AACD,KAlBM;;AAmBP,UAAMQ,aAAN,CAAoBL,KAApB,EAA2B;AACzB,UAAI;AACF,cAAMnB,KAAK,CAACoB,GAAN,CAAW,8CAA6CD,KAAK,CAACE,OAAQ,EAAtE,CAAN;AACA,aAAKC,QAAL,CAAcC,OAAd,CAAsB,SAAtB;AACA,aAAKd,WAAL;AACF,OAJA,CAIE,OAAOO,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,yBAAd,EAAyCA,KAAzC;AACF;AACD,KA3BM;;AA4BPS,sBAAkB,GAAG;AACnB,WAAKpB,aAAL,GAAqB,IAArB;AACD,KA9BM;;AA+BP,UAAMqB,QAAN,GAAiB;AACf,UAAI;AACF,cAAMC,MAAK,GAAI,IAAIC,eAAJ,EAAf;AACAD,cAAM,CAACE,MAAP,CAAc,UAAd,EAA0B,KAAKvB,YAAL,CAAkBC,QAA5C;AACAoB,cAAM,CAACE,MAAP,CAAc,MAAd,EAAsB,GAAtB;AACA7B,aAAK,CAAC8B,IAAN,CAAW,iCAAX,EAA8CH,MAA9C;AACA,aAAKL,QAAL,CAAcC,OAAd,CAAsB,SAAtB;AACA,aAAKlB,aAAL,GAAqB,KAArB;AACA,cAAM,KAAKI,WAAL,EAAN;AACF,OARA,CAQE,OAAOO,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,qBAAd,EAAqCA,KAArC;AACF;AACF;;AA3CO;AAhBI,CAAf","names":["axios","ElDialog","components","data","admins","dialogVisible","addAdminForm","username","mounted","fetchAdmins","methods","response","get","window","local","reload","error","console","disableAdmin","admin","put","adminId","$message","success","activateAdmin","showAddAdminDialog","addAdmin","params","URLSearchParams","append","post"],"sourceRoot":"","sources":["D:\\Dev\\Front\\Remote_Medical_Consultation_Platform_Vue\\src\\views\\Admin\\admin.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <el-button type=\"primary\" @click=\"showAddAdminDialog\">添加管理员</el-button>\r\n    <el-table :data=\"admins\" border stripe>\r\n      <el-table-column prop=\"adminId\" label=\"管理员ID\"></el-table-column>\r\n      <el-table-column prop=\"username\" label=\"用户名\"></el-table-column>\r\n      <el-table-column prop=\"telephone\" label=\"联系方式\"></el-table-column>\r\n      <el-table-column label=\"权限\">\r\n        <template v-slot=\"scope\">\r\n          <span v-if=\"scope.row.role === 0\">已禁用</span>\r\n          <span v-else-if=\"scope.row.role === 1\">超级管理员</span>\r\n          <span v-else-if=\"scope.row.role === 2\">使用中</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"120\">\r\n        <template #default=\"scope\">\r\n          <el-button v-if=\"scope.row.role === 1\" size=\"small\" disabled type=\"info\">不可操作</el-button>\r\n          <el-button v-else-if=\"scope.row.role === 2\" type=\"danger\" size=\"small\" @click=\"disableAdmin(scope.row)\">\r\n            点击禁用\r\n          </el-button>\r\n          <el-button v-else-if=\"scope.row.role === 0\" type=\"success\" size=\"small\" @click=\"activateAdmin(scope.row)\">\r\n            激活账号\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-dialog title=\"添加管理员\" v-model=\"dialogVisible\">\r\n      <el-form ref=\"addAdminForm\" :model=\"addAdminForm\" label-width=\"100px\">\r\n        <el-form-item label=\"用户名\" prop=\"username\">\r\n          <el-input v-model=\"addAdminForm.username\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span id=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"addAdmin\">添加</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport { ElDialog } from 'element-plus'\r\n\r\n\r\nexport default {\r\n  components: {\r\n    ElDialog\r\n  },\r\n  data() {\r\n    return {\r\n      admins: [],\r\n      dialogVisible: false,\r\n      addAdminForm: {\r\n        username: ''\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchAdmins()\r\n  },\r\n  methods: {\r\n    async fetchAdmins() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8081/admin/all')\r\n        this.admins = response.data.data\r\n        window.local.reload\r\n      } catch (error) {\r\n        console.error('Error fetching admins:', error)\r\n      }\r\n    },\r\n    async disableAdmin(admin) {\r\n      try {\r\n        await axios.put(`http://localhost:8081/admin/disable?adminId=${admin.adminId}`)\r\n        this.$message.success('当前账号已禁用')\r\n        this.fetchAdmins()\r\n      } catch (error) {\r\n        console.error('Error disabling admin:', error)\r\n      }\r\n    },\r\n    async activateAdmin(admin) {\r\n      try {\r\n        await axios.put(`http://localhost:8081/admin/active?adminId=${admin.adminId}`)\r\n        this.$message.success('当前账号已激活')\r\n        this.fetchAdmins()\r\n      } catch (error) {\r\n        console.error('Error activating admin:', error)\r\n      }\r\n    },\r\n    showAddAdminDialog() {\r\n      this.dialogVisible = true\r\n    },\r\n    async addAdmin() {\r\n      try {\r\n        const params = new URLSearchParams()\r\n        params.append('username', this.addAdminForm.username)\r\n        params.append('role', '0')\r\n        axios.post('http://localhost:8081/admin/add', params)\r\n        this.$message.success('管理员添加成功')\r\n        this.dialogVisible = false\r\n        await this.fetchAdmins()\r\n      } catch (error) {\r\n        console.error('Error adding admin:', error)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  margin: 20px auto;\r\n  max-width: 800px;\r\n}\r\n\r\n.dialog-footer {\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}