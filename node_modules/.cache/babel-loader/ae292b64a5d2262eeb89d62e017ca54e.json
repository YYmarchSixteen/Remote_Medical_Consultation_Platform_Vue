{"ast":null,"code":"import axios from 'axios';\nimport Header from '../userLayout/Header/header.vue';\nexport default {\n  components: {\n    Header\n  },\n\n  data() {\n    return {\n      medicineId: '',\n      medicineName: '',\n      feature: '',\n      picture: '',\n      price: null,\n      details: [] // 定义一个空数组用于存放数据\n\n    };\n  },\n\n  mounted() {\n    this.medicineId = this.$route.query.medicineId;\n    axios.get(`http://localhost:8080/detail?medicineId=${this.medicineId}`).then(res => {\n      //   this.details = res.data; // 将获取到的数据存储到data中\n      this.medicineName = res.data.medicineName;\n      this.picture = res.data.picture;\n      this.feature = res.data.feature;\n      this.medicineId = res.data.medicineId;\n      this.price = res.data.price;\n      console.log(\"++++++++++++++++++++\");\n      console.log(this.details);\n      console.log(\"+++++++++++++++++++\");\n    }).catch(error => {\n      console.log(error);\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAyCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iCAAnB;AAEA,eAAe;AACXC,YAAU,EAAE;AACRD;AADQ,GADD;;AAIXE,MAAI,GAAG;AACP,WAAO;AACHC,gBAAU,EAAE,EADT;AAEHC,kBAAY,EAAC,EAFV;AAGHC,aAAO,EAAC,EAHL;AAIHC,aAAO,EAAC,EAJL;AAKHC,WAAK,EAAC,IALH;AAOHC,aAAO,EAAE,EAPN,CAOU;;AAPV,KAAP;AASH,GAdc;;AAefC,SAAO,GAAG;AACN,SAAKN,UAAL,GAAkB,KAAKO,MAAL,CAAYC,KAAZ,CAAkBR,UAApC;AACAJ,SAAK,CAACa,GAAN,CAAW,2CAA0C,KAAKT,UAAW,EAArE,EAAwEU,IAAxE,CAA8EC,GAAD,IAAS;AACtF;AACA,WAAKV,YAAL,GAAkBU,GAAG,CAACZ,IAAJ,CAASE,YAA3B;AACA,WAAKE,OAAL,GAAaQ,GAAG,CAACZ,IAAJ,CAASI,OAAtB;AACA,WAAKD,OAAL,GAAaS,GAAG,CAACZ,IAAJ,CAASG,OAAtB;AACA,WAAKF,UAAL,GAAgBW,GAAG,CAACZ,IAAJ,CAASC,UAAzB;AACA,WAAKI,KAAL,GAAWO,GAAG,CAACZ,IAAJ,CAASK,KAApB;AACEQ,aAAO,CAACC,GAAR,CAAY,sBAAZ;AACAD,aAAO,CAACC,GAAR,CAAY,KAAKR,OAAjB;AACAO,aAAO,CAACC,GAAR,CAAY,qBAAZ;AACD,KAVD,EAUGC,KAVH,CAUUC,KAAD,IAAW;AAClBH,aAAO,CAACC,GAAR,CAAYE,KAAZ;AACD,KAZD;AAaJ;;AA9Be,CAAf","names":["axios","Header","components","data","medicineId","medicineName","feature","picture","price","details","mounted","$route","query","get","then","res","console","log","catch","error"],"sourceRoot":"","sources":["D:\\Dev\\Front\\finalProject\\src\\views\\User\\detail.vue"],"sourcesContent":["<template>\r\n      <div class=\"header\">\r\n            <Header></Header>\r\n        </div>\r\n    <div class=\"equipment-item\" >\r\n  \r\n      <div class=\"center_con clearfix\">\r\n        <div class=\"main_menu fl\">\r\n          <img :src=\"picture\" />\r\n        </div>\r\n        <div class=\"goods_detail_list fr\">\r\n           <!-- <h1 style=\"color:red\"> {{ index }}</h1> -->\r\n          <h3>{{ medicineName }}</h3><br>\r\n          <h2>功能:</h2>\r\n          <p>{{ feature }}</p>\r\n          <h2>材质:</h2>\r\n          <p>{{ typeName }}</p>\r\n          <div class=\"prize_bar\">\r\n            <div class=\"show_prize fl\">￥<em>{{ price }}</em></div>\r\n            <div class=\"show_unit fl\">单位：个</div>\r\n          </div>\r\n          <div class=\"goods_num clearfix\">\r\n            \r\n            <div class=\"num_add fl\">\r\n              <input type=\"text\" class=\"num_show fl\" value=\"1\" />\r\n              <a href=\"javascript:;\" class=\"add fr\">+</a>\r\n              <a href=\"javascript:;\" class=\"minus fr\">-</a>\r\n            </div>\r\n          </div>\r\n          <div class=\"total\">总价：<em>{{ price }}元</em></div>\r\n          <div class=\"operate_btn\">\r\n            <a href=\"\" class=\"buy_btn\">立即购买</a>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n</template>\r\n\r\n    \r\n<script>\r\nimport axios from 'axios';\r\nimport Header from '../userLayout/Header/header.vue';\r\n\r\nexport default {\r\n    components: {\r\n        Header\r\n    },\r\n    data() {\r\n    return {\r\n        medicineId: '',\r\n        medicineName:'',\r\n        feature:'',\r\n        picture:'',\r\n        price:null,\r\n\r\n        details: [], // 定义一个空数组用于存放数据\r\n    };\r\n},\r\nmounted() {\r\n    this.medicineId = this.$route.query.medicineId;\r\n    axios.get(`http://localhost:8080/detail?medicineId=${this.medicineId}`).then((res) => {\r\n    //   this.details = res.data; // 将获取到的数据存储到data中\r\n    this.medicineName=res.data.medicineName\r\n    this.picture=res.data.picture\r\n    this.feature=res.data.feature\r\n    this.medicineId=res.data.medicineId\r\n    this.price=res.data.price\r\n      console.log(\"++++++++++++++++++++\")\r\n      console.log(this.details)\r\n      console.log(\"+++++++++++++++++++\")\r\n    }).catch((error) => {\r\n      console.log(error);\r\n    });\r\n}\r\n\r\n}\r\n\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.equipment-item{\r\n    margin-top: 10%;\r\n}\r\n\r\n.center_con .main_menu img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n\r\n.main {\r\n    margin-top: 100px;\r\n}\r\n\r\nbody {\r\n    font-family: 'Microsoft YaHei';\r\n    color: #666;\r\n    font-size: 12px;\r\n}\r\n\r\n.center_con {\r\n    width: 1200px;\r\n    height: 350px;\r\n    margin: 0 auto;\r\n    /*background: yellow;*/\r\n}\r\n\r\n.center_con .main_menu {\r\n    width: 350px;\r\n    height: 350px;\r\n    overflow: hidden;\r\n}\r\n\r\n.goods_detail_list {\r\n    width: 730px;\r\n    height: 350px;\r\n}\r\n\r\n.goods_detail_list h3 {\r\n    font-size: 24px;\r\n    line-height: 24px;\r\n    color: #666;\r\n    font-weight: normal;\r\n}\r\n\r\n.goods_detail_list p {\r\n    color: #666;\r\n    line-height: 40px;\r\n}\r\n\r\n.prize_bar {\r\n    height: 72px;\r\n    background-color: #fff5f5;\r\n    line-height: 72px;\r\n}\r\n\r\n.prize_bar .show_prize {\r\n    font-size: 20px;\r\n    color: #ff3e3e;\r\n    padding-left: 20px\r\n}\r\n\r\n.prize_bar .show_pirze em {\r\n    font-style: normal;\r\n    font-size: 36px;\r\n    padding-left: 10px\r\n}\r\n\r\n.prize_bar .show_unit {\r\n    padding-left: 150px;\r\n}\r\n\r\n.goods_num {\r\n    height: 52px;\r\n    margin-top: 19px;\r\n    /*background: yellow;*/\r\n}\r\n\r\n.goods_num .num_name {\r\n    width: 70px;\r\n    height: 52px;\r\n    line-height: 52px;\r\n}\r\n\r\n.goods_num .num_add {\r\n    width: 75px;\r\n    height: 50px;\r\n    border: 1px solid #dddddd;\r\n}\r\n\r\n.goods_num .num_add .num_show {\r\n    width: 49px;\r\n    height: 50px;\r\n    text-align: center;\r\n    line-height: 50px;\r\n    border: 0px;\r\n    outline: none;\r\n    font-size: 14px;\r\n    color: #666;\r\n}\r\n\r\n.goods_num .num_add .add,\r\n.goods_num .num_add .minus {\r\n    width: 25px;\r\n    line-height: 25px;\r\n    text-align: center;\r\n    border-left: 1px solid #ddd;\r\n    border-bottom: 1px solid #ddd;\r\n    color: #666;\r\n    font-size: 14px;\r\n}\r\n\r\n.goods_num .num_add .minus {\r\n    border-bottom: 0px;\r\n}\r\n\r\n.total {\r\n    height: 35px;\r\n    line-height: 35px;\r\n    margin-top: 25px;\r\n    /*background: yellow;*/\r\n}\r\n\r\n.total em {\r\n    font-style: normal;\r\n    color: #ff3e3e;\r\n    font-size: 18px\r\n}\r\n\r\n.operate_btn {\r\n    height: 40px;\r\n    margin-top: 35px;\r\n    font-size: 0;\r\n    position: relative;\r\n}\r\n\r\n.operate_btn .buy_btn,\r\n.operate_btn .add_cart {\r\n    display: inline-block;\r\n    width: 178px;\r\n    height: 38px;\r\n    border: 1px solid #c40000;\r\n    font-size: 14px;\r\n    color: #c40000;\r\n    line-height: 38px;\r\n    text-align: center;\r\n    background-color: #ffeded;\r\n}\r\n\r\n.operate_btn .add_cart {\r\n    background-color: #c40000;\r\n    color: #fff;\r\n    margin-left: 10px;\r\n    position: relative;\r\n    z-index: 10;\r\n}\r\n\r\nbody,\r\np,\r\nh1,\r\nh2,\r\nh3,\r\nh4,\r\nh5,\r\nh6,\r\nul,\r\ndl,\r\ndt,\r\nform,\r\ninput {\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\nul {\r\n    list-style: none;\r\n}\r\n\r\na {\r\n    text-decoration: none;\r\n}\r\n\r\nem {\r\n    font-style: normal;\r\n}\r\n\r\nimg {\r\n    border: 0px;\r\n}\r\n\r\nh1,\r\nh2,\r\nh3,\r\nh4,\r\nh5,\r\nh6 {\r\n    font-size: 100%;\r\n    font-weight: bolder;\r\n}\r\n\r\n.clearfix:before,\r\n.clearfix:after {\r\n    content: \"\";\r\n    display: table;\r\n}\r\n\r\n.clearfix:after {\r\n    clear: both;\r\n}\r\n\r\n.clearfix {\r\n    zoom: 1;\r\n}\r\n\r\n\r\n.fl {\r\n    float: left;\r\n}\r\n\r\n.fr {\r\n    float: right;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}