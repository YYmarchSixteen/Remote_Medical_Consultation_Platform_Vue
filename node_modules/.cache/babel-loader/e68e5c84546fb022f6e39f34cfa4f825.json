{"ast":null,"code":"import { get, post } from \"@/utils/request\";\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 15,\n      //默认一页显示 条\n      dialogVisible: false,\n      dialogVisible1: false,\n      info2: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      list: [],\n      adg: []\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    get(\"http://localhost:8080/userList\").then(res => {\n      console.log(res);\n      this.list = res.data;\n    });\n  },\n\n  methods: {\n    disable(row) {\n      const data = {\n        id: row.row.id\n      };\n      post(\"http://localhost:8080/softDelete\", data).then(res => {\n        if (res.code === 200) {\n          this.$message.success(\"用户已被禁用\");\n          get(\"http://localhost:8080/userList\").then(res => {\n            console.log(res);\n            this.list = res.data;\n          });\n        } else {\n          this.$message.error(\"操作失败\");\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    // 激活用户方法\n    recover(row) {\n      const data = {\n        id: row.row.id\n      };\n      post(\"http://localhost:8080/recover\", data).then(res => {\n        if (res.code === 200) {\n          this.$message.success(\"用户已被激活\");\n          get(\"http://localhost:8080/userList\").then(res => {\n            console.log(res);\n            this.list = res.data;\n          });\n        } else {\n          this.$message.error(\"操作失败\");\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    // 重置密码方法\n    reset(row) {\n      const data = {\n        id: row.row.id\n      };\n      post(\"http://localhost:8080/resetPassword\", data).then(res => {\n        if (res.code === 200) {\n          this.$message.success(\"密码已重置为 123456\");\n        } else {\n          this.$message.error(\"操作失败\");\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    handleSizeChange: f