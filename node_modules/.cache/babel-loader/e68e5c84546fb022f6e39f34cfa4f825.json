l-table-column>\r\n    </el-table>\r\n\r\n    <el-pagination\r\n      v-model=\"currentPage\"\r\n      :page-sizes=\"[10]\"\r\n      :page-size=\"pagesize\"\r\n      :background=\"background\"\r\n      layout=\"total, sizes, prev, pager, next, jumper\"\r\n      :total=\"list.length\"\r\n      @size-change=\"handleSizeChange\"\r\n      @current-change=\"handleCurrentChange\"\r\n    />\r\n  </el-card>\r\n</template>\r\n\r\n<script>\r\nimport { get, post } from \"@/utils/request\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentPage: 1, //默认页码为1\r\n      pagesize: 15, //默认一页显示 条\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      info2: {}, //修改数据\r\n      info1: {}, //新增数据\r\n      list: [],\r\n      adg: [],\r\n    };\r\n  },\r\n  //初始化工作\r\n  mounted() {\r\n    get(\"http://localhost:8080/userList\").then((res) => {\r\n      console.log(res);\r\n      this.list = res.data;\r\n    });\r\n  },\r\n  methods: {\r\n    disable(row) {\r\n      const data = {\r\n        id: row.row.id,\r\n      };\r\n      post(\"http://localhost:8080/softDelete\", data)\r\n        .then((res) => {\r\n          if (res.code === 200) {\r\n            this.$message.success(\"用户已被禁用\");\r\n            get(\"http://localhost:8080/userList\").then((res) => {\r\n              console.log(res);\r\n              this.list = res.data;\r\n            });\r\n          } else {\r\n            this.$message.error(\"操作失败\");\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    // 激活用户方法\r\n    recover(row) {\r\n      const data = {\r\n        id: row.row.id,\r\n      };\r\n      post(\"http://localhost:8080/recover\", data)\r\n        .then((res) => {\r\n          if (res.code === 200) {\r\n            this.$message.success(\"用户已被激活\");\r\n            get(\"http://localhost:8080/userList\").then((res) => {\r\n              console.log(res);\r\n              this.list = res.data;\r\n            });\r\n          } else {\r\n            this.$message.error(\"操作失败\");\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    // 重置密码方法\r\n    reset(row) {\r\n      const data = {\r\n        id: row.row.id,\r\n      };\r\n      post(\"http://localhost:8080/resetPassword\", data)\r\n        .then((res) => {\r\n          if (res.code === 200) {\r\n            this.$message.success(\"密码已重置为 123456\");\r\n          } else {\r\n            this.$message.error(\"操作失败\");\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    handleSizeChange: function (size) {\r\n      //一页显示多少条\r\n      this.pagesize = size;\r\n    },\r\n    handleCurrentChange: function (currentPage) {\r\n      //页码更改方法\r\n      this.currentPage = currentPage;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.box-card {\r\n  margin-top: 5px;\r\n  margin-left: 15px;\r\n  margin-right: 15px;\r\n  height: 95%;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}