{"ast":null,"code":"import axios from 'axios';\nimport Header from '@/views/userLayout/Header/header.vue';\nexport default {\n  components: {\n    Header\n  },\n\n  data() {\n    return {\n      appointments: []\n    };\n  },\n\n  mounted() {\n    this.loadAppointments();\n  },\n\n  methods: {\n    async loadAppointments() {\n      try {\n        const userId = 1;\n        const apiUrl = 'http://localhost:8081/reserve/myReserve';\n        const response = await axios.get(apiUrl, {\n          params: {\n            userId: userId\n          }\n        });\n        this.appointments = this.processAppointments(response.data.data);\n      } catch (error) {\n        console.error('Error loading appointments:', error);\n      }\n    },\n\n    processAppointments(data) {\n      // 处理预约时间数据，按照规律递增 15 分钟\n      return data.map((appointment, index) => {\n        const hour = Math.floor(index / 4) + 8; // 从 8 点开始，每 4 个预约时间增加 1 小时\n\n        const minute = index % 4 * 15; // 每个预约时间递增 15 分钟\n\n        const formattedTime = `${hour}:${minute.toString().padStart(2, '0')}`; // 格式化时间，例如 \"8:00\"\n\n        return { ...appointment,\n          reserveTime: formattedTime\n        };\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAaA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,sCAAnB;AAEA,eAAe;AACbC,YAAU,EAAE;AAAED;AAAF,GADC;;AAEbE,MAAI,GAAG;AACL,WAAO;AACLC,kBAAY,EAAE;AADT,KAAP;AAGD,GANY;;AAObC,SAAO,GAAG;AACR,SAAKC,gBAAL;AACD,GATY;;AAUbC,SAAO,EAAE;AACP,UAAMD,gBAAN,GAAyB;AACvB,UAAI;AACF,cAAME,MAAK,GAAI,CAAf;AACA,cAAMC,MAAK,GAAI,yCAAf;AACA,cAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,GAAN,CAAUF,MAAV,EAAkB;AACvCG,gBAAM,EAAE;AACNJ,kBAAM,EAAEA;AADF;AAD+B,SAAlB,CAAvB;AAKA,aAAKJ,YAAL,GAAoB,KAAKS,mBAAL,CAAyBH,QAAQ,CAACP,IAAT,CAAcA,IAAvC,CAApB;AACF,OATA,CASE,OAAOW,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,6BAAd,EAA6CA,KAA7C;AACF;AACD,KAdM;;AAePD,uBAAmB,CAACV,IAAD,EAAO;AACxB;AACA,aAAOA,IAAI,CAACa,GAAL,CAAS,CAACC,WAAD,EAAcC,KAAd,KAAwB;AACtC,cAAMC,IAAG,GAAIC,IAAI,CAACC,KAAL,CAAWH,KAAI,GAAI,CAAnB,IAA