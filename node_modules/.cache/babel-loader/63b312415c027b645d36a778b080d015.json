{"ast":null,"code":"import axios from 'axios';\nimport { ElDialog } from 'element-plus';\nexport default {\n  components: {\n    ElDialog\n  },\n\n  data() {\n    return {\n      products: [],\n      dialogVisible: false,\n      updateForm: {\n        price: ''\n      },\n      addForm: {\n        medicineName: '',\n        price: '',\n        feature: '',\n        type: '',\n        rx: ''\n      }\n    };\n  },\n\n  mounted() {\n    this.fetchProducts();\n  },\n\n  methods: {\n    async fetchProducts() {\n      try {\n        const response = await axios.get('http://localhost:8081/medicine/all');\n        this.products = response.data.data;\n      } catch (error) {\n        console.error('Error fetching notices:', error);\n      }\n    },\n\n    async disableProduct(product) {\n      try {\n        await axios.put(`http://localhost:8081/medicine/disabled?medicineId=${product.medicineId}`);\n        this.$message.success('当前商品已禁用');\n        this.fetchProducts();\n      } catch (error) {\n        console.error('Error disabling Product:', error);\n      }\n    },\n\n    async activateProduct(product) {\n      try {\n        await axios.put(`http://localhost:8081/product/activate?medicineId=${product.medicineId}`);\n        this.$message.success('当前商品已激活');\n        this.fetchProducts();\n      } catch (error) {\n        console.error('Error activating Product:', error);\n      }\n    },\n\n    showDialog() {\n      this.dialogVisible = true;\n    },\n\n    async updateProduct() {\n      try {\n        const params = new URLSearchParams();\n        params.append('price', this.updateForm.price);\n        params.append('medicineId', this.updateForm.medicineId);\n        axios.put('http://localhost:8081/medicine/changePrice', params);\n        this.$message.success('调价成功');\n        this.dialogVisible = false;\n        await this.fetchProducts();\n        window.location.reload();\n      } catch (error) {\n        console.error('Error adding Product:', error);\n      }\n    },\n\n    async addProduct() {\n      try {\n        const params = new URLSearchParams();\n        params.append('medicineName', this.addForm.medicineName);\n        params.append('price', this.addForm.price);\n        params.append('feature', this.addForm.feature);\n        params.append('type', this.addForm.type);\n        params.append('rx', this.addForm.rx);\n        axios.post('http://localhost:8081/medicine/addMedicine', params);\n        this.$message.success('新增成功');\n        this.dialogVisible = false;\n        await this.fetchProducts();\n      } catch (error) {\n        console.error('Error adding Product:', error);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8EA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,QAAT,QAAyB,cAAzB;AAEA,eAAe;AACbC,YAAU,EAAE;AAAED;AAAF,GADC;;AAEbE,MAAI,GAAG;AACL,WAAO;AACLC,cAAQ,EAAE,EADL;AAELC,mBAAa,EAAE,KAFV;AAGLC,gBAAU,EAAE;AACVC,aAAK,EAAE;AADG,OAHP;AAMLC,aAAO,EAAE;AACPC,oBAAY,EAAE,EADP;AAEPF,aAAK,EAAE,EAFA;AAGPG,eAAO,EAAC,EAHD;AAIPC,YAAI,EAAC,EAJE;AAKPC,UAAE,EAAC;AALI;AANJ,KAAP;AAcD,GAjBY;;AAkBbC,SAAO,GAAG;AACR,SAAKC,aAAL;AACD,GApBY;;AAqBbC,SAAO,EAAE;AACP,UAAMD,aAAN,GAAsB;AACpB,UAAI;AACF,cAAME,QAAO,GAAI,MAAMhB,KAAK,CAACiB,GAAN,CAAU,oCAAV,CAAvB;AACA,aAAKb,QAAL,GAAgBY,QAAQ,CAACb,IAAT,CAAcA,IAA9B;AACF,OAHA,CAGE,OAAOe,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,yBAAd,EAAyCA,KAAzC;AACF;AACD,KARM;;AASP,UAAME,cAAN,CAAqBC,OAArB,EAA8B;AAC5B,UAAI;AACF,cAAMrB,KAAK,CAACsB,GAAN,CAAW,sDAAqDD,OAAO,CAACE,UAAW,EAAnF,CAAN;AACA,aAAKC,QAAL,CAAcC,OAAd,CAAsB,SAAtB;AACA,aAAKX,aAAL;AACF,OAJA,CAIE,OAAOI,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,0BAAd,EAA0CA,KAA1C;AACF;AACD,KAjBM;;AAkBP,UAAMQ,eAAN,CAAsBL,OAAtB,EAA+B;AAC7B,UAAI;AACF,cAAMrB,KAAK,CAACsB,GAAN,CAAW,qDAAoDD,OAAO,CAACE,UAAW,EAAlF,CAAN;AACA,aAAKC,QAAL,CAAcC,OAAd,CAAsB,SAAtB;AACA,aAAKX,aAAL;AACF,OAJA,CAIE,OAAOI,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,2BAAd,EAA2CA,KAA3C;AACF;AACD,KA1BM;;AA2BPS,cAAU,GAAG;AACX,WAAKtB,aAAL,GAAqB,IAArB;AACD,KA7BM;;AA8BP,UAAMuB,aAAN,GAAsB;AACpB,UAAI;AACF,cAAMC,MAAK,GAAI,