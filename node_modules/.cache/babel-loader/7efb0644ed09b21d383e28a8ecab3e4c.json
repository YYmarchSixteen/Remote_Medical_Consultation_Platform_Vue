{"ast":null,"code":"import { ElMessage } from 'element-plus';\nimport { get } from '@/utils/request';\nexport default {\n  data() {\n    return {\n      dialogVisible: false,\n      //修改对话框是否可见\n      dialogVisible1: false,\n      //添加对话框是否可见\n      info: {},\n      //添加的科室内容{科室名、地址等}\n      info1: {},\n      tableData: [] //空数组\n\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    get('/getAllDepartments').then(res => {\n      console.log(res);\n      this.tableData = res.data; // console.log(this.tableData)\n    });\n  },\n\n  methods: {\n    handleClick(row) {\n      this.info = row.row; //console.log(this.info)\n\n      this.dialogVisible = !this.dialogVisible;\n    },\n\n    del(row) {\n      concolr.log(row.row.departmentId);\n      get('/deleteDepartment', {\n        departmentId: row.row.departmentId\n      });\n\n      res => {\n        ElMessage({\n          message: \"删除成功！\",\n          type: \"success\"\n        });\n        window.location.reload(); //刷新\n      };\n    },\n\n    edit() {\n      this.dialogVisible = !this.dialogVisible;\n      var dt = new Date(this.info.createTime);\n      let year = dt.getFullYear();\n      let month = dt.getMonth() + 1;\n      let date = dt.getDate();\n      this.info.createTime = year + '/' + month + '/' + date;\n      get('updateDepartment', this.info).then(res => {\n        ElMessage({\n          message: '修改成功',\n          type: 'success'\n        });\n      });\n    },\n\n    add() {\n      this.dialogVisible1 = !this.dialogVisible1;\n    },\n\n    insert() {\n      this.dialogVisible1 = !this.dialogVisible1;\n      get('/addDepartment', this.info1).then(res => {\n        console.log(res);\n\n        if (res, data == 1) {\n          ElMessage({\n            message: \"添加成功\",\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '添加失败',\n            type: 'danger'\n          });\n        }\n      }); // console.log(this.info1)\n