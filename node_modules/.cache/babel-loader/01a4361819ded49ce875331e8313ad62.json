{"ast":null,"code":"import { get } from '@/utils/request';\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 7,\n      //默认一页显示 条\n      dialogVisible: false,\n      dialogVisible1: false,\n      info2: {\n        localPictureFile: undefined // 新增本地图片文件变量\n\n      },\n      //修改数据\n      info1: {},\n      //新增数据\n      list: [],\n      adg: [],\n      dialogVisibleupdate: false,\n      updateid: 0,\n      equipmentTypes: [] // 器械类型列表\n\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    // 获取器械类型列表\n    get('http://localhost:8080/equipmenttypeList').then(res => {\n      console.log(res);\n      this.equipmentTypes = res.data;\n    });\n    get('http://localhost:8080/equipmentList').then(res => {\n      console.log(res);\n      this.list = res.data;\n    });\n  },\n\n  methods: {\n    formatText(text, limit) {\n      // 自定义函数，接收文本和限制长度作为参数\n      if (text && text.length > limit) {\n        // 判断文本是否需要截取\n        return text.slice(0, limit) + '...'; // 截取并添加省略号\n      }\n\n      return text; // 不需要截取则直接返回原文本\n    },\n\n    del(data) {\n      console.log(data.row.id);\n      get('http://localhost:8080/deleteEquipment', {\n        id: data.row.id\n      }).then(() => {\n        ElMessage({\n          message: '删除成功！',\n          type: 'success'\n        });\n      });\n      window.location.reload(); //刷新\n    },\n\n    add() {\n      this.dialogVisible1 = !this.dialogVisible1;\n    },\n\n    insert() {\n      this.dialogVisible1 = !this.dialogVisible1;\n      get('http://localhost:8080/insertEquipment', this.info1).then(res => {\n        console.log(res);\n\n        if (res.data == '添加成功') {\n          ElMessage({\n            message: '添加成功',\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '添加失败',\n            type: 'danger'\n          });\n        }\n      });\n    },\n\n    handleClick(data) {\n      this.info2 = data.row;\n      this.dialogVisible = !this.dialogVisible;\n    },\n\n    edit() {\n      get('http://localhost:8080/updateEquipment', this.info2).then(res => {\n        console.log(res);\n\n        if (res.data == '修改成功') {\n          ElMessage({\n            message: '修改成功',\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '修改失败',\n            type: 'danger'\n          });\n        }\n      });\n    },\n\n    handleSizeChange: function (size) {\n      //一页显示多少条\n      this.pagesize = size;\n    },\n    handleCurrentChange: function (currentPage) {\n      //页码更改方法\n      this.currentPage = currentPage;\n    }\n  }\n};","map":{"version":3,"mappings":"AAkIA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,cAA1B;AACA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,CADR;AACW;AAClBC,cAAQ,EAAE,CAFH;AAEM;AACbC,mBAAa,EAAE,KAHR;AAIPC,oBAAc,EAAE,KAJT;AAKPC,WAAK,EAAE;AACLC,wBAAgB,EAAEC,SADb,CACwB;;AADxB,OALA;AAOJ;AACHC,WAAK,EAAE,EARA;AAQI;AACXC,UAAI,EAAE,EATC;AAUPC,SAAG,EAAE,EAVE;AAWPC,yBAAmB,EAAE,KAXd;AAYPC,cAAQ,EAAE,CAZH;AAaPC,oBAAc,EAAE,EAbT,CAaa;;AAbb,KAAP;AAeD,GAjBY;;AAkBb;AACAC,SAAO,GAAG;AACP;AACHhB,OAAG,CAAC,yCAAD,CAAH,CAA+CiB,IAA/C,CAAqDC,GAAD,IAAS;AAC3DC,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,WAAKH,cAAL,GAAsBG,GAAG,CAAChB,IAA1B;AACD,KAHD;AAIEF,OAAG,CAAC,qCAAD,CAAH,CAA2CiB,IAA3C,CAAiDC,GAAD,IAAS;AACvDC,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,WAAKP,IAAL,GAAYO,GAAG,CAAChB,IAAhB;AACD,KAHD;AAID,GA7BY;;AA8BbmB,SAAO,EAAE;AACPC,cAAU,CAACC,IAAD,EAAOC,KAAP,EAAc;AAAE;AACxB,UAAID,IAAG,IAAKA,IAAI,CAACE,MAAL,GAAcD,KAA1B,EAAiC;AAAE;AACjC,eAAOD,IAAI,CAACG,KAAL,CAAW,CAAX,EAAcF,KAAd,IAAuB,KAA9B,CAD+B,CACM;AACvC;;AACA,aAAOD,IAAP,CAJsB,CAIT;AACd,KANM;;AAOPI,OAAG,CAACzB,IAAD,EAAO;AACRiB,aAAO,CAACC,GAAR,CAAYlB,IAAI,CAAC0B,GAAL,CAASC,EAArB;AACA7B,SAAG,CAAC,uCAAD,EAA0C;AAAE6B,UAAE,EAAE3B,IAAI,CAAC0B,GAAL,CAASC;AAAf,OAA1C,CAAH,CAAkEZ,IAAlE,CAAuE,MAAM;AAC3EhB,iBAAS,CAAC;AACR6B,iBAAO,EAAE,