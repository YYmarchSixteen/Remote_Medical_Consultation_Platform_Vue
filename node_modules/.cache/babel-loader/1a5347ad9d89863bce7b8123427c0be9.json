{"ast":null,"code":"import { get } from \"@/utils/request\";\nimport { put } from \"@/utils/request\";\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 15,\n      //默认一页显示 条\n      dialogVisible: false,\n      dialogVisible1: false,\n      info2: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      list: [],\n      adg: []\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    get(\"http://localhost:8080/allUser\").then(res => {\n      console.log(res);\n      this.list = res.data.data;\n    });\n  },\n\n  methods: {\n    disable(row) {\n      const userId = row.row.userId;\n      axios.put(`http://localhost:8080/softDelete?userId=${userId}`).then(res => {\n        if (res.data.code === 200) {\n          this.$message.success(\"用户已被禁用\");\n          get(\"http://localhost:8080/allUser\").then(res => {\n            console.log(res);\n            this.list = res.data;\n          });\n        } else {\n          this.$message.error(\"操作失败\");\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    recover(row) {\n      const id = row.row.id;\n      get(`http://localhost:8080/recover?id=${id}`).then(res => {\n        if (res.data === \"恢复成功\") {\n          this.$message.success(\"用户已被激活\");\n          get(\"http://localhost:8080/allUser\").then(res => {\n            console.log(res);\n            this.list = res.data;\n          });\n        } else {\n          this.$message.error(\"操作失败\");\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    reset(row) {\n      const id = row.row.id;\n      get(`http://localhost:8080/resetPassword?id=${id}`).then(res => {\n        if (res.data === \"密码重置成功\") {\n          this.$message.success(\"密码已重置为 123\");\n        } else {\n          this.$message.error(\"操作失败\");\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    handleSizeChange: function (size) {\n      //一页显示多少条\n      this.pagesize = size;\n    },\n    handleCurrentChange: function (currentPage) {\n      //页码更改方法\n      this.currentPage = currentPage;\n    }\n  }\n};","map":{"version":3,"mappings":"AAmCA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,GAAT,QAAoB,iBAApB;AACA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,CADR;AACW;AAChBC,cAAQ,EAAE,EAFL;AAES;AACdC,mBAAa,EAAE,KAHV;AAILC,oBAAc,EAAE,KAJX;AAKLC,WAAK,EAAE,EALF;AAKM;AACXC,WAAK,EAAE,EANF;AAMM;AACXC,UAAI,EAAE,EAPD;AAQLC,SAAG,EAAE;AARA,KAAP;AAUD,GAZY;;AAab;AACAC,SAAO,GAAG;AACRX,OAAG,CAAC,+BAAD,CAAH,CAAqCY,IAArC,CAA2CC,GAAD,IAAS;AACjDC,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,WAAKJ,IAAL,GAAYI,GAAG,CAACX,IAAJ,CAASA,IAArB;AACD,KAHD;AAID,GAnBY;;AAoBbc,SAAO,EAAE;AACPC,WAAO,CAACC,GAAD,EAAM;AACX,YAAMC,MAAK,GAAID,GAAG,CAACA,GAAJ,CAAQC,MAAvB;AACAC,WAAK,CAACnB,GAAN,CAAW,2CAA0CkB,MAAO,EAA5D,EACGP,IADH,CACSC,GAAD,IAAS;AACb,YAAIA,GAAG,CAACX,IAAJ,CAASmB,IAAT,KAAkB,GAAtB,EAA2B;AACzB,eAAKC,QAAL,CAAcC,OAAd,CAAsB,QAAtB;AACAvB,aAAG,CAAC,+BAAD,CAAH,CAAqCY,IAArC,CAA2CC,GAAD,IAAS;AACjDC,mBAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,iBAAKJ,IAAL,GAAYI,GAAG,CAACX,IAAhB;AACD,WAHD;AAIF,SANA,MAMO;AACL,eAAKoB,QAAL,CAAcE,KAAd,CAAoB,MAApB;AACF;AACD,OAXH,EAYGC,KAZH,CAYUD,KAAD,IAAW;AAChBV,eAAO,CAACC,GAAR,CAAYS,KAAZ;AACD,OAdH;AAeD,KAlBM;;AAoBPE,WAAO,CAACR,GAAD,EAAM;AACX,YAAMS,EAAC,GAAIT,GAAG,CAACA,GAAJ,CAAQS,EAAnB;AACA3B,SAAG,CAAE,oCAAmC2B,EAAG,EAAxC,CAAH,CACGf,IADH,CACSC,GAAD,IAAS;AACb,YAAIA,GAAG,CAACX,IAAJ,KAAa,MAAjB,EAAyB;AACvB,eAAKoB,QAAL,CAAcC,OAAd,CAAsB,QAAtB;AACAvB,aAAG,CAAC,+BAAD,CAAH,CAAqCY,IAArC,CAA2CC,GAAD,IAAS;AACjDC,mBAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,iBAAKJ,IAAL,GAAYI,GAAG,CAACX,IAAhB;AACD,WAHD;AAIF,SANA,MAMO;AACL,eAAKoB,QAAL,CAAcE,KAAd,CAAoB,MAApB;AACF;AACD,OAXH,EAYGC,KAZH,CAYUD,KAAD,IAAW;AAChBV,eAAO,CAACC,GAAR,CAAYS,KAAZ;AACD,OAdH;AAeD,KArCM;;AAuCPI,SAAK,CAACV,GAAD,EAAM;AACT,YAAMS,EAAC,GAAIT,GAAG,CAACA,GAAJ,CAAQS,EAAnB;AACA3B,SAAG,CAAE,0CAAyC2B,EAAG,EAA9C,CAAH,CACGf,IADH,CACSC,GAAD,IAAS;AACb,YAAIA,GAAG,CAACX,IAAJ,KAAa,QAAjB,EAA2B;AACzB,eAAKoB,QAAL,CAAcC,OAAd,CAAsB,YAAtB;AACF,SAFA,MAEO;AACL,eAAKD,QAAL,CAAcE,KAAd,CAAoB,MAApB;AACF;AACD,OAPH,EAQGC,KARH,CAQUD,KAAD,IAAW;AAChBV,eAAO,CAACC,GAAR,CAAYS,KAAZ;AACD,OAVH;AAWD,KApDM;;AAuDPK,oBAAgB,EAAE,UAAUC,IAAV,EAAgB;AAChC;AACA,WAAK1B,QAAL,GAAgB0B,IAAhB;AACD,KA1DM;AA2DPC,uBAAmB,EAAE,UAAU5B,WAAV,EAAuB;AAC1C;AACA,WAAKA,WAAL,GAAmBA,WAAnB;AACD;AA9DM;AApBI,CAAf","names":["get","put","data","currentPage","pagesize","dialogVisible","dialogVisible1","info2","info1","list","adg","mounted","then","res","console","log","methods","disable","row","userId","axios","code","$message","success","error","catch","recover","id","reset","handleSizeChange","size","handleCurrentChange"],"sourceRoot":"","sources":["D:\\Dev\\Front\\finalProject\\src\\views\\Admin\\user.vue"],"sourcesContent":["<template>\r\n  <el-card class=\"box-card\">\r\n    <el-table :data=\"list.slice((currentPage - 1) * pagesize, currentPage * pagesize)\" style=\"width: 100%\">\r\n      <el-table-column prop=\"userId\" label=\"ID\" width=\"200\" />\r\n      <el-table-column prop=\"username\" label=\"用户名\" width=\"220\" />\r\n      <el-table-column prop=\"password\" label=\"密码\" width=\"220\" />\r\n      <el-table-column prop=\"telephone\" label=\"电话\" width=\"220\" />\r\n      <el-table-column prop=\"address\" label=\"地址\" width=\"220\" />\r\n      <el-table-column prop=\"disabled\" label=\"当前状态\" width=\"220\">\r\n        <template #default=\"{ row }\">\r\n          <span v-if=\"row.disabled == '0'\">正常使用</span>\r\n          <span v-else>禁用中</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column fixed=\"right\" label=\"操作\" width=\"240\">\r\n        <template #default=\"row\">\r\n          <div v-if=\"row.row.disabled == '0'\">\r\n            <el-button type=\"danger\" @click=\"disable(row)\">禁用用户</el-button>\r\n            <el-button type=\"primary\" @click=\"reset(row)\">重置密码</el-button>\r\n          </div>\r\n          <div v-if=\"row.row.disabled == '1'\">\r\n            <el-button type=\"success\" @click=\"recover(row)\">激活用户</el-button>\r\n            <el-button type=\"primary\" @click=\"reset(row)\">重置密码</el-button>\r\n          </div>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <el-pagination v-model=\"currentPage\" :page-sizes=\"[10]\" :page-size=\"pagesize\" :background=\"background\"\r\n      layout=\"total, sizes, prev, pager, next, jumper\" :total=\"list.length\" @size-change=\"handleSizeChange\"\r\n      @current-change=\"handleCurrentChange\" />\r\n  </el-card>\r\n</template>\r\n\r\n<script>\r\nimport { get } from \"@/utils/request\";\r\nimport { put } from \"@/utils/request\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentPage: 1, //默认页码为1\r\n      pagesize: 15, //默认一页显示 条\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      info2: {}, //修改数据\r\n      info1: {}, //新增数据\r\n      list: [],\r\n      adg: [],\r\n    };\r\n  },\r\n  //初始化工作\r\n  mounted() {\r\n    get(\"http://localhost:8080/allUser\").then((res) => {\r\n      console.log(res);\r\n      this.list = res.data.data;\r\n    });\r\n  },\r\n  methods: {\r\n    disable(row) {\r\n      const userId = row.row.userId;\r\n      axios.put(`http://localhost:8080/softDelete?userId=${userId}`)\r\n        .then((res) => {\r\n          if (res.data.code === 200) {\r\n            this.$message.success(\"用户已被禁用\");\r\n            get(\"http://localhost:8080/allUser\").then((res) => {\r\n              console.log(res);\r\n              this.list = res.data;\r\n            });\r\n          } else {\r\n            this.$message.error(\"操作失败\");\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    recover(row) {\r\n      const id = row.row.id;\r\n      get(`http://localhost:8080/recover?id=${id}`)\r\n        .then((res) => {\r\n          if (res.data === \"恢复成功\") {\r\n            this.$message.success(\"用户已被激活\");\r\n            get(\"http://localhost:8080/allUser\").then((res) => {\r\n              console.log(res);\r\n              this.list = res.data;\r\n            });\r\n          } else {\r\n            this.$message.error(\"操作失败\");\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    reset(row) {\r\n      const id = row.row.id;\r\n      get(`http://localhost:8080/resetPassword?id=${id}`)\r\n        .then((res) => {\r\n          if (res.data === \"密码重置成功\") {\r\n            this.$message.success(\"密码已重置为 123\");\r\n          } else {\r\n            this.$message.error(\"操作失败\");\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n\r\n    handleSizeChange: function (size) {\r\n      //一页显示多少条\r\n      this.pagesize = size;\r\n    },\r\n    handleCurrentChange: function (currentPage) {\r\n      //页码更改方法\r\n      this.currentPage = currentPage;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.box-card {\r\n  margin-top: 5px;\r\n  margin-left: 15px;\r\n  margin-right: 15px;\r\n  height: 95%;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}