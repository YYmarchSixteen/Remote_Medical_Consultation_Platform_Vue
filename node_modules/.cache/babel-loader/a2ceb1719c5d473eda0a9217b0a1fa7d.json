{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      selectedDate: '',\n      reservations: [],\n      doctorId: '' // 上一页传递过来的医生ID\n\n    };\n  },\n\n  mounted() {\n    this.doctorId = this.$route.params.doctorId; // 获取医生ID\n\n    console.log(this.doctorId);\n  },\n\n  methods: {\n    async fetchReservations() {\n      try {\n        const formattedDate = this.formatDate(this.selectedDate);\n        console.log(this.selectedDate);\n        console.log(formattedDate);\n        const response = await axios.get(`http://localhost:8081/reserve/all?doctorId=${this.doctorId}&date=${formattedDate}`);\n        this.reservations = response.data.data;\n      } catch (error) {\n        console.error('Error fetching reservations:', error);\n      }\n    },\n\n    formatDate(date) {\n      const year = date.getFullYear();\n      const month = String(date.getMonth() + 1).padStart(2, '0');\n      const day = String(date.getDate()).padStart(2, '0');\n      return `${year}-${month}-${day}`;\n    },\n\n    async reserveTime() {\n      try {// 在这里执行预约逻辑，你可以使用axios.post或者其他方式向后端发送预约请求\n        // 在成功预约后，更新对应时间段的状态\n        // 例如：timeSlot.status = 'reserved';\n      } catch (error) {\n        console.error('Error reserving time:', error);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6BA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,kBAAY,EAAE,EADT;AAELC,kBAAY,EAAE,EAFT;AAGLC,cAAQ,EAAE,EAHL,CAGQ;;AAHR,KAAP;AAKD,GAPY;;AAQbC,SAAO,GAAG;AACR,SAAKD,QAAL,GAAgB,KAAKE,MAAL,CAAYC,MAAZ,CAAmBH,QAAnC,CADQ,CACqC;;AAC7CI,WAAO,CAACC,GAAR,CAAY,KAAKL,QAAjB;AACD,GAXY;;AAYbM,SAAO,EAAE;AACP,UAAMC,iBAAN,GAA0B;AACxB,UAAI;AACF,cAAMC,aAAY,GAAI,KAAKC,UAAL,CAAgB,KAAKX,YAArB,CAAtB;AACAM,eAAO,CAACC,GAAR,CAAY,KAAKP,YAAjB;AACAM,eAAO,CAACC,GAAR,CAAYG,aAAZ;AACA,cAAME,QAAO,GAAI,MAAMd,KAAK,CAACe,GAAN,CAAW,8CAA6C,KAAKX,QAAS,SAAQQ,aAAc,EAA5F,CAAvB;AACA,aAAKT,YAAL,GAAoBW,QAAQ,CAACb,IAAT,CAAcA,IAAlC;AACF,OANA,CAME,OAAOe,KAAP,EAAc;AACdR,eAAO,CAACQ