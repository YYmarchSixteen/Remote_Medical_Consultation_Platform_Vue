om 'axios'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      selectedDate: '',\r\n      reservations: [],\r\n      doctorId: '' // 上一页传递过来的医生ID\r\n    }\r\n  },\r\n  mounted() {\r\n    this.doctorId = this.$route.params.doctorId; // 获取医生ID\r\n    console.log(this.doctorId)\r\n  },\r\n  methods: {\r\n    async fetchReservations() {\r\n      try {\r\n        const formattedDate = this.formatDate(this.selectedDate);\r\n        console.log(this.selectedDate)\r\n        console.log(formattedDate)\r\n        const response = await axios.get(`http://localhost:8081/reserve/all?doctorId=${this.doctorId}&date=${formattedDate}`);\r\n        this.reservations = response.data.data;\r\n      } catch (error) {\r\n        console.error('Error fetching reservations:', error);\r\n      }\r\n    },formatDate(date) {\r\n      const year = date.getFullYear();\r\n      const month = String(date.getMonth() + 1).padStart(2, '0');\r\n      const day = String(date.getDate()).padStart(2, '0');\r\n      return `${year}-${month}-${day}`;\r\n    },\r\n    async reserveTime() {\r\n      try {\r\n        // 在这里执行预约逻辑，你可以使用axios.post或者其他方式向后端发送预约请求\r\n        // 在成功预约后，更新对应时间段的状态\r\n        // 例如：timeSlot.status = 'reserved';\r\n      } catch (error) {\r\n        console.error('Error reserving time:', error);\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  margin: 20px auto;\r\n  max-width: 800px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}