{"ast":null,"code":"import axios from \"axios\";\nimport Header from \"../userLayout/Header/header.vue\";\nexport default {\n  components: {\n    Header\n  },\n\n  data() {\n    return {\n      form: {},\n      username: sessionStorage.getItem(\"username\"),\n      userid: sessionStorage.getItem(\"userid\"),\n      isLoading: false,\n      medicineList: [],\n      medicineTypes: [],\n      selectedType: \"\",\n      searchTerm: \"\"\n    };\n  },\n\n  mounted() {\n    this.getMedicineList();\n    this.fetchMedicineTypes();\n  },\n\n  computed: {\n    filteredMedicineList() {\n      if (this.searchTerm === \"\") {\n        return this.medicineList;\n      } else {\n        const reg = new RegExp(this.searchTerm, \"i\");\n        return this.medicineList.filter(item => item.medicineName.search(reg) !== -1);\n      }\n    }\n\n  },\n  methods: {\n    async fetchMedicineTypes() {\n      try {\n        const res = await axios.get(\"http://localhost:8080/allMedicineType\");\n        this.medicineTypes.push(...res.data.data);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async getMedicineList() {\n      try {\n        const response = await axios.get(\"http://localhost:8080/allMedicine\");\n        this.medicineList = response.data.data;\n      } catch (error) {\n        console.error(error);\n      }\n    },\n\n    equipmentSearch() {\n      // 根据用户输入的搜索内容进行筛选过滤\n      this.filteredMedicineList;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuDI,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iCAAnB;AAEA,eAAe;AACfC,YAAU,EAAE;AACZD;AADY,GADG;;AAIfE,MAAI,GAAG;AACP,WAAO;AACPC,UAAI,EAAE,EADC;AAEPC,cAAQ,EAAEC,cAAc,CAACC,OAAf,CAAuB,UAAvB,CAFH;AAGPC,YAAM,EAAEF,cAAc,CAACC,OAAf,CAAuB,QAAvB,CAHD;AAIPE,eAAS,EAAE,KAJJ;AAKPC,kBAAY,EAAE,EALP;AAMPC,mBAAa,EAAE,EANR;AAOPC,kBAAY,EAAE,EAPP;AAQPC,gBAAU,EAAE;AARL,KAAP;AAUC,GAfc;;AAiBfC,SAAO,GAAG;AACV,SAAKC,eAAL;AACA,SAAKC,kBAAL;AACC,GApBc;;AAsBfC,UAAQ,EAAE;AACVC,wBAAoB,GAAG;AACvB,UAAI,KAAKL,UAAL,KAAoB,EAAxB,E