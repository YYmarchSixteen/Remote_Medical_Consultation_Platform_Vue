{"ast":null,"code":"import { get } from '@/utils/request';\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 2,\n      //默认一页显示10条\n      dialogVisible: false,\n      dialogVisible1: false,\n      info: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      tableData: []\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    get('/getNotice').then(res => {\n      this.tableData = res.data;\n      console.log(res);\n    });\n  },\n\n  methods: {\n    handleSizeChange: function (size) {\n      //一页显示多少条\n      this.pagesize = size;\n    },\n    handleCurrentChange: function (currentPage) {\n      //页码更改方法\n      this.currentPage = currentPage;\n    },\n\n    handleClick(data) {\n      this.info = data.row;\n      this.dialogVisible = !this.dialogVisible;\n    },\n\n    del(data) {\n      get('/deleteNotice', {\n        NoticeId: data.row.id\n      }).then(res => {\n        ElMessage({\n          message: '删除成功！',\n          type: 'success'\n        });\n        window.location.reload();\n      });\n    },\n\n    //修改\n    edit() {\n      console.log(\"===========\");\n      console.log(this.info);\n      this.dialogVisible = !this.dialogVisible; //马上消失\n      //作用是调整下createTime的格式\n\n      var dt = new Date(this.info.day);\n      let year = dt.getFullYear();\n      let month = dt.getMonth() + 1;\n      let date = dt.getDate();\n      this.info.day = year + '/' + month + '/' + date;\n      get('/updateNotice', this.info).then(res => {\n        ElMessage({\n          message: '修改成功！',\n          type: 'success'\n        });\n        window.location.reload();\n      });\n    },\n\n    add() {\n      this.dialogVisible1 = !this.dialogVisible1;\n    },\n\n    insert() {\n      this.dialogVisible1 = !this.dialogVisible1;\n      get('/addNotice', this.info1).then(res => {\n        console.log(res);\n\n        if (res.data == 1) {\n          ElMessage({\n            message: '添加成功',\n            type: 'success'\n          }); // 刷新\n\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '添加失败',\n            type: 'danger'\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwFA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,cAA1B;AACA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,CADR;AACW;AAChBC,cAAQ,EAAE,CAFL;AAEQ;AACbC,mBAAa,EAAE,KAHV;AAILC,oBAAc,EAAE,KAJX;AAKLC,UAAI,EAAE,EALD;AAKK;AACVC,WAAK,EAAE,EANF;AAMM;AACXC,eAAS,EAAE;AAPN,KAAP;AASD,GAXY;;AAYb;AACAC,SAAO,GAAG;AACRV,OAAG,CAAC,YAAD,CAAH,CAAkBW,IAAlB,CAAwBC,GAAD,IAAS;AAC9B,WAAKH,SAAL,GAAiBG,GAAG,CAACV,IAArB;AACAW,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACD,KAHD;AAID,GAlBY;;AAoBbG,SAAO,EAAE;AACPC,oBAAgB,EAAE,UAAUC,IAAV,EAAgB;AAChC;AAEA,WAAKb,QAAL,GAAgBa,IAAhB;AACD,KALM;AAOPC,uBAAmB,EAAE,UAAUf,WAAV,EAAuB;AAC1C;AAEA,WAAKA,WAAL,GAAmBA,WAAnB;AACD,KAXM;;AAYPgB,eAAW,CAACjB,IAAD,EAAO;AAChB,WAAKK,IAAL,GAAYL,IAAI,CAACkB,GAAjB;AACA,WAAKf,aAAL,GAAqB,CAAC,KAAKA,aAA3B;AACD,KAfM;;AAgBPgB,OAAG,CAACnB,IAAD,EAAO;AACRF,SAAG,CAAC,eAAD,EAAkB;AAAEsB,gBAAQ,EAAEpB,IAAI,CAACkB,GAAL,CAASG;AAArB,OAAlB,CAAH,CAAgDZ,IAAhD,CACGC,GAAD,IAAS;AACPX,iBAAS,CAAC;AACRuB,iBAAO,EAAE,OADD;AAERC,cAAI,EAAE;AAFE,SAAD,CAAT;AAIAC,cAAM,CAACC,QAAP,CAAgBC,MAAhB;AACF,OAPF;AASD,KA1BM;;AA2BP;AACAC,QAAI,GAAG;AAAChB,aAAO,CAACC,GAAR,CAAY,aAAZ;AACRD,aAAO,CAACC,GAAR,CAAY,KAAKP,IAAjB;AACE,WAAKF,aAAL,GAAqB,CAAC,KAAKA,aAA3B,CAFK,CAEoC;AACzC;;AACA,UAAIyB,EAAC,GAAI,IAAIC,IAAJ,CAAS,KAAKxB,IAAL,CAAUyB,GAAnB,CAAT;AACA,UAAIC,IAAG,GAAIH,EAAE,CAACI,WAAH,EAAX;AACA,UAAIC,KAAI,GAAIL,EAAE,CAACM,QAAH,KAAgB,CAA5B;AACA,UAAIC,IAAG,GAAIP,EAAE,CAACQ,OAAH,EAAX;AACA,WAAK/B,IAAL,CAAUyB,GAAV,GAAgBC,IAAG,GAAI,GAAP,GAAaE,KAAb,GAAqB,GAArB,GAA2BE,IAA3C;AACArC,SAAG,CAAC,eAAD,EAAkB,KAAKO,IAAvB,CAAH,CAAgCI,IAAhC,CAAsCC,GAAD,IAAS;AAC5CX,iBAAS,CAAC;AACRuB,iBAAO,EAAE,OADD;AAERC,cAAI,EAAE;AAFE,SAAD,CAAT;AAIAC,cAAM,CAACC,QAAP,CAAgBC,MAAhB;AACD,OAND;AAOD,KA5CM;;AA8CPW,OAAG,GAAG;AACJ,WAAKjC,cAAL,GAAsB,CAAC,KAAKA,cAA5B;AACD,KAhDM;;AAkDPkC,UAAM,GAAG;AACP,WAAKlC,cAAL,GAAsB,CAAC,KAAKA,cAA5B;AACAN,SAAG,CAAC,YAAD,EAAe,KAAKQ,KAApB,CAAH,CAA8BG,IAA9B,CAAoCC,GAAD,IAAS;AAC1CC,eAAO,CAACC,GAAR,CAAYF,GAAZ;;AACA,YAAIA,GAAG,CAACV,IAAJ,IAAY,CAAhB,EAAmB;AACjBD,mBAAS,CAAC;AACRuB,mBAAO,EAAE,MADD;AAERC,gBAAI,EAAE;AAFE,WAAD,CAAT,CADiB,CAKjB;;AACAC,gBAAM,CAACC,QAAP,CAAgBC,MAAhB;AACF,SAPA,MAOO;AACL3B,mBAAS,CAAC;AACRuB,mBAAO,EAAE,MADD;AAERC,gBAAI,EAAE;AAFE,WAAD,CAAT;AAIF;AACD,OAfD;AAgBF;;AApEO;AApBI,CAAf","names":["get","ElMessage","data","currentPage","pagesize","dialogVisible","dialogVisible1","info","info1","tableData","mounted","then","res","console","log","methods","handleSizeChange","size","handleCurrentChange","handleClick","row","del","NoticeId","id","message","type","window","location","reload","edit","dt","Date","day","year","getFullYear","month","getMonth","date","getDate","add","insert"],"sourceRoot":"","sources":["D:\\小学期vscode\\demo01\\src\\views\\System\\carousel.vue"],"sourcesContent":["<template>\r\n  <el-button type=\"primary\" @click=\"add(props)\">添加</el-button>\r\n  <el-table\r\n  :data=\"tableData.slice((currentPage - 1) * pagesize, currentPage * pagesize)\"\r\n  style=\"width: 100%\">\r\n <el-table-column prop=\"id\" label=\"轮播图Id\" width=\"200\" />\r\n    <el-table-column prop=\"title\" label=\"标题\" width=\"300\" />\r\n    <el-table-column prop=\"content\" label=\"具体内容\" width=\"300\" />\r\n    <el-table-column prop=\"img\" label=\"照片\" width=\"300\" />\r\n    <!-- <el-table-column prop=\"noticeId\" label=\"公告Id\" width=\"300\" /> -->\r\n    <el-table-column fixed=\"right\" label=\"操作\" width=\"200\">\r\n      <template #default=\"row\">\r\n        <el-button type=\"success\" @click=\"handleClick(row)\"> 编辑 </el-button>\r\n        <el-popconfirm title=\"确定要删除吗?\" @confirm=\"del(row)\">\r\n          <template #reference>\r\n            <el-button type=\"danger\">删除</el-button>\r\n          </template>\r\n        </el-popconfirm>\r\n      </template>\r\n    </el-table-column>\r\n  </el-table>\r\n  <el-dialog v-model=\"dialogVisible1\" title=\"增加\" width=\"30%\">\r\n    <el-form label-width=\"150px\">\r\n      <el-form-item label=\"轮播图Id\">\r\n        <el-input v-model=\"info1.id\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"150px\">\r\n      <el-form-item label=\"具体内容\">\r\n        <el-input v-model=\"info1.content\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"150px\">\r\n      <el-form-item label=\"照片\">\r\n        <el-input v-model=\"info1.img\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"150px\">\r\n      <el-form-item label=\"标题\">\r\n        <el-input v-model=\"info1.title\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible1 = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"insert()\" v-model=\"dialogVisible1\"\r\n          >确认</el-button\r\n        >\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n  <el-dialog v-model=\"dialogVisible\" title=\"编辑\" width=\"30%\">\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"公告Id\">\r\n        <el-input v-model=\"info.id\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"标题\">\r\n        <el-input v-model=\"info.title\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"照片\">\r\n        <el-input v-model=\"info.img\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"edit()\" v-model=\"dialogVisible\"\r\n          >确认</el-button\r\n        >\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n  <el-pagination\r\n    currentPage=\"currentPage\"\r\n    :page-sizes=\"[2, 4, 8, 10]\"\r\n    page-size=\"10\"\r\n    :background=\"background\"\r\n    layout=\"total, sizes, prev, pager, next, jumper\"\r\n    :total=\"tableData.length\"\r\n    @size-change=\"handleSizeChange\"\r\n    @current-change=\"handleCurrentChange\"\r\n  />\r\n</template>\r\n<script>\r\nimport { get } from '@/utils/request'\r\nimport { ElMessage } from 'element-plus'\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentPage: 1, //默认页码为1\r\n      pagesize: 2, //默认一页显示10条\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      info: {}, //修改数据\r\n      info1: {}, //新增数据\r\n      tableData: []\r\n    }\r\n  },\r\n  //初始化工作\r\n  mounted() {\r\n    get('/getNotice').then((res) => {\r\n      this.tableData = res.data\r\n      console.log(res)\r\n    })\r\n  },\r\n\r\n  methods: {\r\n    handleSizeChange: function (size) {\r\n      //一页显示多少条\r\n\r\n      this.pagesize = size\r\n    },\r\n\r\n    handleCurrentChange: function (currentPage) {\r\n      //页码更改方法\r\n\r\n      this.currentPage = currentPage\r\n    },\r\n    handleClick(data) {\r\n      this.info = data.row\r\n      this.dialogVisible = !this.dialogVisible\r\n    },\r\n    del(data) {\r\n      get('/deleteNotice', { NoticeId: data.row.id }).then(\r\n        (res) => {\r\n          ElMessage({\r\n            message: '删除成功！',\r\n            type: 'success'\r\n          })\r\n          window.location.reload()\r\n        }\r\n      )\r\n    },\r\n    //修改\r\n    edit() {console.log(\"===========\")\r\n    console.log(this.info)\r\n      this.dialogVisible = !this.dialogVisible //马上消失\r\n      //作用是调整下createTime的格式\r\n      var dt = new Date(this.info.day)\r\n      let year = dt.getFullYear()\r\n      let month = dt.getMonth() + 1\r\n      let date = dt.getDate()\r\n      this.info.day = year + '/' + month + '/' + date\r\n      get('/updateNotice', this.info).then((res) => {\r\n        ElMessage({\r\n          message: '修改成功！',\r\n          type: 'success'\r\n        })\r\n        window.location.reload()\r\n      })\r\n    },\r\n\r\n    add() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n    },\r\n\r\n    insert() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n      get('/addNotice', this.info1).then((res) => {\r\n        console.log(res)\r\n        if (res.data == 1) {\r\n          ElMessage({\r\n            message: '添加成功',\r\n            type: 'success'\r\n          })\r\n          // 刷新\r\n          window.location.reload()\r\n        } else {\r\n          ElMessage({\r\n            message: '添加失败',\r\n            type: 'danger'\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}