dError\"\r\n            list-type=\"picture-card\"\r\n            :limit=\"1\"\r\n          >\r\n            <i class=\"el-icon-plus\"></i>\r\n          </el-upload>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"updateUser\">更新用户信息</el-button>\r\n          <el-button type=\"primary\" @click=\"changePhoto\">更改照片</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n    <div v-else>\r\n      <el-alert\r\n        title=\"正在加载用户信息...\"\r\n        type=\"info\"\r\n        show-icon\r\n      ></el-alert>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      user: null\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchUser();\r\n  },\r\n  methods: {\r\n    async fetchUser() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8081/doctor/findById');\r\n        this.user = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching user:', error);\r\n      }\r\n    },\r\n    async updateUser() {\r\n      try {\r\n        await axios.put('http://localhost:8081/doctor/update', this.user);\r\n        alert('用户信息已更新');\r\n      } catch (error) {\r\n        console.error('Error updating user:', error);\r\n        alert('更新用户信息时出错');\r\n      }\r\n    },\r\n    handleUploadSuccess(response, file) {\r\n      // 更新用户照片信息\r\n      this.user.photoUrl = response.data.url; // 假设接口返回的数据包含照片的URL\r\n      alert('照片上传成功');\r\n    },\r\n    handleRemove(file, fileList) {\r\n      console.log(file, fileList);\r\n      // 可以在这里处理文件移除的逻辑\r\n    },\r\n    handleUploadError(err, file, fileList) {\r\n      console.error(err, file, fileList);\r\n      // 可以在这里处理上传失败的逻辑\r\n      alert('照片上传失败');\r\n    },\r\n    changePhoto() {\r\n      // 此处不需要特别处理，因为el-upload已经处理了上传逻辑\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  margin: 20px auto;\r\n  max-width: 800px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}