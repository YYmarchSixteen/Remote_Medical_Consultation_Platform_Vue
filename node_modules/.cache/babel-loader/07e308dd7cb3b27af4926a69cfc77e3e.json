{"ast":null,"code":"import axios from 'axios';\nimport { ElDialog } from 'element-plus';\nexport default {\n  components: {\n    ElDialog\n  },\n\n  data() {\n    return {\n      notices: [],\n      dialogVisible: false,\n      updateForm: {\n        title: '',\n        content: ''\n      },\n      addForm: {\n        title: '',\n        content: ''\n      }\n    };\n  },\n\n  mounted() {\n    this.fetchNotices();\n  },\n\n  methods: {\n    async fetchNotices() {\n      try {\n        const response = await axios.get('http://localhost:8081/notice/all');\n        this.notices = response.data.data;\n      } catch (error) {\n        console.error('Error fetching notices:', error);\n      }\n    },\n\n    async disableNotice(notice) {\n      try {\n        await axios.put(`http://localhost:8081/notice/disabled?noticeId=${notice.noticeId}`);\n        this.$message.success('当前公告已禁用');\n        this.fetchNotices();\n      } catch (error) {\n        console.error('Error disabling notice:', error);\n      }\n    },\n\n    async activateNotice(notice) {\n      try {\n        await axios.put(`http://localhost:8081/notice/activate?noticeId=${notice.noticeId}`);\n        this.$message.success('当前公告已激活');\n        this.fetchNotices();\n      } catch (error) {\n        console.error('Error activating notice:', error);\n      }\n    },\n\n    showDialog() {\n      this.dialogVisible = true;\n    },\n\n    async updateNotice() {\n      try {\n        const params = new URLSearchParams();\n        params.append('title', this.updateForm.title);\n        params.append('content', this.updateForm.content);\n        params.append('noticeId', this.updateForm.noticeId);\n        axios.put('http://localhost:8081/notice/update', params);\n        this.$message.success('编辑成功');\n        this.dialogVisible = false;\n        await this.fetchNotices();\n        window.location.reload();\n      } catch (error) {\n        console.error('Error adding Notice:', error);\n      }\n    },\n\n    async addNotice() {\n      try {\n        const params = new URLSearchParams();\n        params.append('title', this.addForm.title);\n        params.append('content', this.addForm.content);\n        params.append('receiver', this.addForm.receiver);\n        axios.post('http://localhost:8081/notice/add', params);\n        this.$message.success('新增成功');\n        this.dialogVisible = false;\n        await this.fetchNotices();\n      } catch (error) {\n        console.error('Error adding Notice:', error);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoFA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,QAAT,QAAyB,cAAzB;AAEA,eAAe;AACbC,YAAU,EAAE;AAAED;AAAF,GADC;;AAEbE,MAAI,GAAG;AACL,WAAO;AACLC,aAAO,EAAE,EADJ;AAELC,mBAAa,EAAE,KAFV;AAGLC,gBAAU,EAAE;AACVC,aAAK,EAAE,EADG;AAEVC,eAAO,EAAE;AAFC,OAHP;AAOLC,aAAO,EAAE;AACPF,aAAK,EAAE,EADA;AAEPC,eAAO,EAAE;AAFF;AAPJ,KAAP;AAYD,GAfY;;AAgBbE,SAAO,GAAG;AACR,SAAKC,YAAL;AACD,GAlBY;;AAmBbC,SAAO,EAAE;AACP,UAAMD,YAAN,GAAqB;AACnB,UAAI;AACF,cAAME,QAAO,GAAI,MAAMb,KAAK,CAACc,GAAN,CAAU,kCAAV,CAAvB;AACA,aAAKV,OAAL,GAAeS,QAAQ,CAACV,IAAT,CAAcA,IAA7B;AACF,OAHA,CAGE,OAAOY,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,yBAAd,EAAyCA,KAAzC;AACF;AACD,KARM;;AASP,UAAME,aAAN,CAAoBC,MAApB,EAA4B;AAC1B,UAAI;AACF,cAAMlB,KAAK,CAACmB,GAAN,CAAW,kDAAiDD,MAAM,CAACE,QAAS,EAA5E,CAAN;AACA,aAAKC,QAAL,CAAcC,OAAd,CAAsB,SAAtB;AACA,aAAKX,YAAL;AACF,OAJA,CAIE,OAAOI,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,yBAAd,EAAyCA,KAAzC;AACF;AACD,KAjBM;;AAkBP,UAAMQ,cAAN,CAAqBL,MAArB,EAA6B;AAC3B,UAAI;AACF,cAAMlB,KAAK,CAACmB,GAAN,CAAW,kDAAiDD,MAAM,CAACE,QAAS,EAA5E,CAAN;AACA,aAAKC,QAAL,CAAcC,OAAd,CAAsB,SAAtB;AACA,aAAKX,YAAL;AACF,OAJA,CAIE,OAAOI,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,0BAAd,EAA0CA,KAA1C;AACF;AACD,KA1BM;;AA2BPS,cAAU,GAAG;AACX,WAAKnB,aAAL,GAAqB,IAArB;AACD,KA7BM;;AA8BP,UAAMoB,YAAN,GAAqB;AACnB,UAAI;AACF,cAAMC,MAAK,GAAI,IAAIC,eAAJ,EAAf;AACAD,cAAM,CAACE,MAAP,CAAc,OAAd,EAAuB,KAAKtB,UAAL,CAAgBC,KAAvC;AACAmB,cAAM,CAACE,MAAP,CAAc,SAAd,EAAyB,KAAKtB,UAAL,CAAgBE,OAAzC;AACAkB,cAAM,CAACE,MAAP,CAAc,UAAd,EAA0B,KAAKtB