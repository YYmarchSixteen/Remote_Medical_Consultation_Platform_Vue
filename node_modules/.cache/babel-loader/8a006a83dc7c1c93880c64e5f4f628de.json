n prop=\"id\" label=\"ID\" width=\"200\" />\r\n      <el-table-column prop=\"equipmentname\" label=\"器械名\" width=\"150\" />\r\n      <el-table-column prop=\"feature\" label=\"功能\" width=\"150\">\r\n        <template #default=\"{ row }\">\r\n          <div>{{ formatText(row.feature, 15) }}</div>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"material\" label=\"材质成分\" width=\"150\">\r\n        <template #default=\"{ row }\">\r\n          <div>{{ formatText(row.material, 15) }}</div>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"price\" label=\"价格\" width=\"150\" />\r\n      <el-table-column prop=\"type\" label=\"所属类型\" width=\"150\" />\r\n      <el-table-column prop=\"picture\" label=\"图片\" width=\"300\">\r\n        <template #default=\"scope\">\r\n          <el-image style=\"width: 70px; height: 70px\" :src=\"scope.row.picture\" alt=\"\" :fit=\"fill\"></el-image>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table>\r\n      </el-table>\r\n      <el-table-column fixed=\"right\" label=\"操作\" width=\"240\">\r\n        <template #default=\"row\">\r\n          <el-button type=\"success\" @click=\"handleClick(row)\">编辑</el-button>\r\n          <el-button type=\"danger\" @click=\"del(row)\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 添加框 -->\r\n    <el-dialog v-model=\"dialogVisible1\" title=\"增加\" width=\"30%\">\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"器械名\">\r\n          <el-input v-model=\"info1.equipmentname\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"功能\">\r\n          <el-input v-model=\"info1.feature\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"材质成分\">\r\n          <el-input v-model=\"info1.material\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"价格\">\r\n          <el-input v-model=\"info1.price\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"所属类型\">\r\n          <el-select v-model=\"info1.type\" placeholder=\"请选择器械类型\">\r\n            <el-option v-for=\"type in equipmentTypes\" :key=\"type.id\" :label=\"type.typename\"\r\n              :value=\"type.typename\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form-item label=\"上传图片\">\r\n        <el-upload class=\"upload-demo\" drag \r\n          :action=\"'http://localhost:8080/upload?id=' + updateform.id\" :name=\"'image'\" ···\r\n           multiple>\r\n          <i class=\"el-icon-upload\"></i>\r\n           <div class=\"el-upload__text\">将文件拖到此处，或<em>点击上传</em></div>\r\n          </el-upload>\r\n        </el-form-item>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible1 = false\">取消</el-button>\r\n          <el-button type=\"primary\" @click=\"insert()\" v-model=\"dialogVisible1\">确认</el-button>\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n\r\n    <!--编辑框 -->\r\n    <el-dialog v-model=\"dialogVisible\" title=\"编辑\" width=\"30%\">\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"器械名\">\r\n          <el-input v-model=\"info2.equipmentname\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"功能\">\r\n          <el-input v-model=\"info2.feature\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"材质成分\">\r\n          <el-input v-model=\"info2.material\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"价格\">\r\n          <el-input v-model=\"info2.price\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"所属类型\">\r\n          <el-select v-model=\"info2.type\" placeholder=\"请选择器械类型\">\r\n            <el-option v-for=\"type in equipmentTypes\" :key=\"type.id\" :label=\"type.typename\"\r\n              :value=\"type.typename\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form label-width=\"120px\">\r\n        <el-form-item label=\"图片\">\r\n          <el-input v-model=\"info2.picture\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n          <el-button type=\"primary\" @click=\"edit()\" v-model=\"dialogVisible\">确认</el-button>\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n\r\n\r\n    <el-pagination currentPage=\"currentPage\" :page-sizes=\"[7]\" page-size=\"7\" :background=\"background\"\r\n      layout=\"total, sizes, prev, pager, next, jumper\" :total=\"list.length\" @size-change=\"handleSizeChange\"\r\n      @current-change=\"handleCurrentChange\" />\r\n  </el-card>\r\n</template>\r\n<script>\r\nimport { get } from '@/utils/request'\r\nimport { ElMessage } from 'element-plus'\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentPage: 1, //默认页码为1\r\n      pagesize: 7, //默认一页显示 条\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      info2: {}, //修改数据\r\n      info1: {}, //新增数据\r\n      list: [],\r\n      adg: [],\r\n      dialogVisibleupdate: false,\r\n      updateid: 0,\r\n      equipmentTypes: [], // 器械类型列表\r\n    }\r\n  },\r\n  //初始化工作\r\n  mounted() {\r\n    // 获取器械类型列表\r\n    get('http://localhost:8080/equipmenttypeList').then((res) => {\r\n      console.log(res)\r\n      this.equipmentTypes = res.data\r\n    })\r\n    get('http://localhost:8080/equipmentList').then((res) => {\r\n      console.log(res)\r\n      this.list = res.data\r\n    })\r\n  },\r\n  methods: {\r\n    formatText(text, limit) { // 自定义函数，接收文本和限制长度作为参数\r\n      if (text && text.length > limit) { // 判断文本是否需要截取\r\n        return text.slice(0, limit) + '...'; // 截取并添加省略号\r\n      }\r\n      return text; // 不需要截取则直接返回原文本\r\n    },\r\n    del(data) {\r\n      console.log(data.row.id)\r\n      get('http://localhost:8080/deleteEquipment', { id: data.row.id }).then(() => {\r\n        ElMessage({\r\n          message: '删除成功！',\r\n          type: 'success'\r\n        })\r\n      })\r\n      window.location.reload() //刷新\r\n    },\r\n    add() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n    },\r\n    insert() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n      get('http://localhost:8080/insertEquipment', this.info1).then((res) => {\r\n        console.log(res)\r\n        if (res.data == '添加成功') {\r\n          ElMessage({\r\n            message: '添加成功',\r\n            type: 'success'\r\n          })\r\n          window.location.reload()\r\n        } else {\r\n          ElMessage({\r\n            message: '添加失败',\r\n            type: 'danger'\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleClick(data) {\r\n      this.info2 = data.row\r\n      this.dialogVisible = !this.dialogVisible\r\n    },\r\n    edit() {\r\n\r\n      get('http://localhost:8080/updateEquipment', this.info2).then((res) => {\r\n        console.log(res)\r\n        if (res.data == '修改成功') {\r\n          ElMessage({\r\n            message: '修改成功',\r\n            type: 'success'\r\n          })\r\n          window.location.reload()\r\n        } else {\r\n          ElMessage({\r\n            message: '修改失败',\r\n            type: 'danger'\r\n          })\r\n        }\r\n      })\r\n    },\r\n\r\n    handleSizeChange: function (size) {\r\n      //一页显示多少条\r\n      this.pagesize = size\r\n    },\r\n    handleCurrentChange: function (currentPage) {\r\n      //页码更改方法\r\n      this.currentPage = currentPage\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.box-card {\r\n  margin-top: 5px;\r\n  margin-left: 15px;\r\n  margin-right: 15px;\r\n  height: 95%;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}