{"ast":null,"code":"import { get } from '@/utils/request';\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 7,\n      //默认一页显示 条\n      dialogVisible: false,\n      dialogVisible1: false,\n      info2: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      list: [],\n      adg: [],\n      dialogVisibleupdate: false,\n      updateid: 0,\n      equipmentTypes: [] // 器械类型列表\n\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    // 获取器械类型列表\n    get('http://localhost:8080/equipmenttypeList').then(res => {\n      console.log(res);\n      this.equipmentTypes = res.data;\n    });\n    get('http://localhost:8080/equipmentList').then(res => {\n      console.log(res);\n      this.list = res.data;\n    });\n  },\n\n  methods: {\n    formatText(text, limit) {\n      // 自定义函数，接收文本和限制长度作为参数\n      if (text && text.length > limit) {\n        // 判断文本是否需要截取\n        return text.slice(0, limit) + '...'; // 截取并添加省略号\n      }\n\n      return text; // 不需要截取则直接返回原文本\n    },\n\n    del(data) {\n      console.log(data.row.id);\n      get('http://localhost:8080/deleteEquipment', {\n        id: data.row.id\n      }).then(() => {\n        ElMessage({\n          message: '删除成功！',\n          type: 'success'\n        });\n      });\n      window.location.reload(); //刷新\n    },\n\n    add() {\n      this.dialogVisible1 = !this.dialogVisible1;\n    },\n\n    insert() {\n      this.dialogVisible1 = !this.dialogVisible1;\n      get('http://localhost:8080/insertEquipment', this.info1).then(res => {\n        console.log(res);\n\n        if (res.data == '添加成功') {\n          ElMessage({\n            message: '添加成功',\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '添加失败',\n            type: 'danger'\n          });\n        }\n      });\n    },\n\n    handleClick(data) {\n      this.info2 = data.row;\n      this.dialogVisible = !this.dialogVisible;\n    },\n\n    edit() {\n      get('http://localhost:8080/updateEquipment', this.info2).then(res => {\n        console.log(res);\n\n        if (res.data == '修改成功') {\n          ElMessage({\n            message: '修改成功',\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '修改失败',\n            type: 'danger'\n          });\n        }\n      });\n    },\n\n    handleSizeChange: function (size) {\n      //一页显示多少条\n      this.pagesize = size;\n    },\n    handleCurrentChange: function (currentPage) {\n      //页码更改方法\n      this.currentPage = currentPage;\n    }\n  }\n};","map":{"version":3,"mappings":"AAgIA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,cAA1B;AACA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,CADR;AACW;AAChBC,cAAQ,EAAE,CAFL;AAEQ;AACbC,mBAAa,EAAE,KAHV;AAILC,oBAAc,EAAE,KAJX;AAKLC,WAAK,EAAE,EALF;AAKM;AACXC,WAAK,EAAE,EANF;AAMM;AACXC,UAAI,EAAE,EAPD;AAQLC,SAAG,EAAE,EARA;AASLC,yBAAmB,EAAE,KAThB;AAULC,cAAQ,EAAE,CAVL;AAWLC,oBAAc,EAAE,EAXX,CAWe;;AAXf,KAAP;AAaD,GAfY;;AAgBb;AACAC,SAAO,GAAG;AACR;AACAd,OAAG,CAAC,yCAAD,CAAH,CAA+Ce,IAA/C,CAAqDC,GAAD,IAAS;AAC3DC,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,WAAKH,cAAL,GAAsBG,GAAG,CAACd,IAA1B;AACD,KAHD;AAIAF,OAAG,CAAC,qCAAD,CAAH,CAA2Ce,IAA3C,CAAiDC,GAAD,IAAS;AACvDC,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,WAAKP,IAAL,GAAYO,GAAG,CAACd,IAAhB;AACD,KAHD;AAID,GA3BY;;AA4BbiB,SAAO,EAAE;AACPC,cAAU,CAACC,IAAD,EAAOC,KAAP,EAAc;AAAE;AACxB,UAAID,IAAG,IAAKA,IAAI,CAACE,MAAL,GAAcD,KAA1B,EAAiC;AAAE;AACjC,eAAOD,IAAI,CAACG,KAAL,CAAW,CAAX,EAAcF,KAAd,IAAuB,KAA9B,CAD+B,CACM;AACvC;;AACA,aAAOD,IAAP,CAJsB,CAIT;AACd,KANM;;AAOPI,OAAG,CAACvB,IAAD,EAAO;AACRe,aAAO,CAACC,GAAR,CAAYhB,IAAI,CAACwB,GAAL,CAASC,EAArB;AACA3B,SAAG,CAAC,uCAAD,EAA0C;AAAE2B,UAAE,EAAEzB,IAAI,CAACwB,GAAL,CAASC;AAAf,OAA1C,CAAH,CAAkEZ,IAAlE,CAAuE,MAAM;AAC3Ed,iBAAS,CAAC;AACR2B,iBAAO,EAAE,OADD;AAERC,cAAI,EAAE;AAFE,SAAD,CAAT;AAID,OALD;AAMAC,YAAM,CAACC,QAAP,CAAgBC,MAAhB,GARQ,CAQiB;AAC1B,KAhBM;;