{"ast":null,"code":"import { get } from '@/utils/request';\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 10,\n      //默认一页显示10条\n      dialogVisible: false,\n      dialogVisible1: false,\n      info: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      tbData: []\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    get('/getAllDepartments').then(res => {\n      console.log(res);\n      this.tbData = res.data;\n    });\n  },\n\n  methods: {\n    handleClick(data) {\n      this.info = data.row;\n      this.dialogVisible = !this.dialogVisible;\n    },\n\n    del(data) {\n      console.log(data.row.departmentId);\n      get('/deleteDepartment', {\n        departmentId: data.row.departmentId\n      }).then(res => {\n        ElMessage({\n          message: '删除成功！',\n          type: 'success'\n        });\n      });\n      window.location.reload(); //刷新\n    },\n\n    edit() {\n      this.dialogVisible = !this.dialogVisible; //马上消失\n      //作用是调整下createTime的格式\n\n      var dt = new Date(this.info.createTime);\n      let year = dt.getFullYear();\n      let month = dt.getMonth() + 1;\n      let date = dt.getDate();\n      this.info.createTime = year + '/' + month + '/' + date;\n      get('/updateDepartment', this.info).then(res => {\n        ElMessage({\n          message: '修改成功！',\n          type: 'success'\n        });\n        window.location.reload();\n      });\n    },\n\n    add() {\n      this.dialogVisible1 = !this.dialogVisible1;\n    },\n\n    insert() {\n      this.dialogVisible1 = !this.dialogVisible1;\n      get('/addDepartment', this.info1).then(res => {\n        console.log(res);\n\n        if (res.data == 1) {\n          ElMessage({\n            message: '添加成功',\n            type: 'success'\n          }); // 刷新\n\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '添加失败',\n            type: 'danger'\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0FA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,cAA1B;AACA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,CADR;AACW;AAChBC,cAAQ,EAAE,EAFL;AAES;AACdC,mBAAa,EAAE,KAHV;AAILC,oBAAc,EAAE,KAJX;AAKLC,UAAI,EAAE,EALD;AAKK;AACVC,WAAK,EAAE,EANF;AAMM;AACXC,YAAM,EAAE;AAPH,KAAP;AASD,GAXY;;AAYb;AACAC,SAAO,GAAG;AACRV,OAAG,CAAC,oBAAD,CAAH,CAA0BW,IAA1B,CAAgCC,GAAD,IAAS;AACtCC,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,WAAKH,MAAL,GAAcG,GAAG,CAACV,IAAlB;AACD,KAHD;AAID,GAlBY;;AAmBba,SAAO,EAAE;AACPC,eAAW,CAACd,IAAD,EAAO;AAChB,WAAKK,IAAL,GAAYL,IAAI,CAACe,GAAjB;AACA,WAAKZ,aAAL,GAAqB,CAAC,KAAKA,aAA3B;AACD,KAJM;;AAKPa,OAAG,CAAChB,IAAD,EAAO;AACRW,aAAO,CAACC,GAAR,CAAYZ,IAAI,CAACe,GAAL,CAASE,YAArB;AACAnB,SAAG,CAAC,mBAAD,EAAsB;AAAEmB,oBAAY,EAAEjB,IAAI,CAACe,GAAL,CAASE;AAAzB,OAAtB,CAAH,CAAkER,IAAlE,CACGC,GAAD,IAAS;AACPX,iBAAS,CAAC;AACRmB,iBAAO,EAAE,OADD;AAERC,cAAI,EAAE;AAFE,SAAD,CAAT;AAIF,OANF;AAQAC,YAAM,CAACC,QAAP,CAAgBC,MAAhB,GAVQ,CAUiB;AAC1B,KAhBM;;AAiBPC,QAAI,GAAG;AACL,WAAKpB,aAAL,GAAqB,CAAC,KAAKA,aAA3B,CADK,CACoC;AACzC;;AACA,UAAIqB,EAAC,GAAI,IAAIC,IAAJ,CAAS,KAAKpB,IAAL,CAAUqB,UAAnB,CAAT;AACA,UAAIC,IAAG,GAAIH,EAAE,CAACI,WAAH,EAAX;AACA,UAAIC,KAAI,GAAIL,EAAE,CAACM,QAAH,KAAgB,CAA5B;AACA,UAAIC,IAAG,GAAIP,EAAE,CAACQ,OAAH,EAAX;AACA,WAAK3B,IAAL,CAAUqB,UAAV,GAAuBC,IAAG,GAAI,GAAP,GAAaE,KAAb,GAAqB,GAArB,GAA2BE,IAAlD;AACAjC,SAAG,CAAC,mBAAD,EAAsB,KAAKO,IAA3B,CAAH,CAAoCI,IAApC,CAA0CC,GAAD,IAAS;AAChDX,iBAAS,CAAC;AACRmB,iBAAO,EAAE,OADD;AAERC,cAAI,EAAE;AAFE,SAAD,CAAT;AAIAC,cAAM,CAACC,QAAP,CAAgBC,MAAhB;AACD,OAND;AAOD,KAhCM;;AAiCPW,OAAG,GAAG;AACJ,WAAK7B,cAAL,GAAsB,CAAC,KAAKA,cAA5B;AACD,KAnCM;;AAoCP8B,UAAM,GAAG;AACP,WAAK9B,cAAL,GAAsB,CAAC,KAAKA,cAA5B;AACAN,SAAG,CAAC,gBAAD,EAAmB,KAAKQ,KAAxB,CAAH,CAAkCG,IAAlC,CAAwCC,GAAD,IAAS;AAC9CC,eAAO,CAACC,GAAR,CAAYF,GAAZ;;AACA,YAAIA,GAAG,CAACV,IAAJ,IAAY,CAAhB,EAAmB;AACjBD,mBAAS,CAAC;AACRmB,mBAAO,EAAE,MADD;AAERC,gBAAI,EAAE;AAFE,WAAD,CAAT,CADiB,CAKjB;;AACAC,gBAAM,CAACC,QAAP,CAAgBC,MAAhB;AACF,SAPA,MAOO;AACLvB,mBAAS,C