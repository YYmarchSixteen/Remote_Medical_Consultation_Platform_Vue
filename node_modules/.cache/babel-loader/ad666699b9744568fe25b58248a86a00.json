{"ast":null,"code":"import { get } from '@/utils/request';\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 15,\n      //默认一页显示 条\n      dialogVisible: false,\n      dialogVisible1: false,\n      info2: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      list: [],\n      adg: [],\n      imageUrl: ''\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    get('http://localhost:8080/equipmentList').then(res => {\n      console.log(res);\n      this.list = res.data;\n    });\n  },\n\n  methods: {\n    handleAvatarSuccess(res) {\n      this.imageUrl = res.url;\n\n      if (this.dialogVisible) {\n        // 修改框\n        this.info2.picture = res.url; // 赋值图片 URL\n      } else {\n        // 添加框\n        this.info1.picture = res.url; // 赋值图片 URL\n      }\n    },\n\n    del(data) {\n      console.log(data.row.id);\n      get('http://localhost:8080/deleteEquipment', {\n        id: data.row.id\n      }).then(() => {\n        ElMessage({\n          message: '删除成功！',\n          type: 'success'\n        });\n      });\n      window.location.reload(); //刷新\n    },\n\n    add() {\n      this.dialogVisible1 = !this.dialogVisible1;\n    },\n\n    insert() {\n      this.dialogVisible1 = !this.dialogVisible1;\n      get('http://localhost:8080/insertEquipment', this.info1).then(res => {\n        console.log(res);\n\n        if (res.data == '添加成功') {\n          ElMessage({\n            message: '添加成功',\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '添加失败',\n            type: 'danger'\n          });\n        }\n      });\n    },\n\n    handleClick(data) {\n      this.info2 = data.row;\n      this.dialogVisible = !this.dialogVisible;\n    },\n\n    edit() {\n      get('http://localhost:8080/updateEquipment', this.info2).then(res => {\n        console.log(res);\n\n        if (res.data == '修改成功') {\n          ElMessage({\n            message: '修改成功',\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '修改失败',\n            type: 'danger'\n          });\n        }\n      });\n    },\n\n    handleSizeChange: function (size) {\n      //一页显示多少条\n      this.pagesize = size;\n    },\n    handleCurrentChange: function (currentPage) {\n      //页码更改方法\n      this.currentPage = currentPage;\n    },\n\n    handleAvatarSuccess(res) {\n      this.imageUrl = res.url;\n\n      if (this.dialogVisible) {\n        this.info2.picture = res.url;\n      } else {\n        this.info1.picture = res.url;\n      }\n    },\n\n    beforeAvatarUpload(file) {\n      const isJPG = file.type === 'image/jpeg';\n      const isPNG = file.type === 'image/png';\n      const isGIF = file.type === 'image/gif';\n      const isLt2M = file.size / 1024 / 1024 < 2;\n\n      if (!isJPG && !isPNG && !isGIF) {\n        this.$message.error('上传图片只能是 JPG/GIF/PNG 格式!');\n        return false;\n      }\n\n      if (!isLt2M) {\n        this.$message.error('上传图片大小不能超过 2MB!');\n        return false;\n      }\n\n      return true;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuIA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,cAA1B;AAEA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,CADR;AACW;AAChBC,cAAQ,EAAE,EAFL;AAES;AACdC,mBAAa,EAAE,KAHV;AAILC,oBAAc,EAAE,KAJX;AAKLC,WAAK,EAAE,EALF;AAKM;AACXC,WAAK,EAAE,EANF;AAMM;AACXC,UAAI,EAAE,EAPD;AAQLC,SAAG,EAAE,EARA;AASLC,cAAQ,EAAE;AATL,KAAP;AAWD,GAbY;;AAcb;AACAC,SAAO,GAAG;AACRZ,OAAG,CAAC,qCAAD,CAAH,CAA2Ca,IAA3C,CAAiDC,GAAD,IAAS;AACvDC,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,WAAKL,IAAL,GAAYK,GAAG,CAACZ,IAAhB;AACD,KAHD;AAID,GApBY;;AAqBbe,SAAO,EAAE;AACPC,uBAAmB,CAACJ,GAAD,EAAM;AAC3B,WAAKH,QAAL,GAAgBG,GAAG,CAACK,GAApB;;AACA,UAAI,KAAKd,aAAT,EAAwB;AAAE;AACxB,aAAKE,KAAL,CAAWa,OAAX,GAAqBN,GAAG,CAACK,GAAzB,CADsB,CACQ;AAChC,OAFA,MAEO;AAAE;AACP,aAAKX,KAAL,CAAWY,OAAX,GAAqBN,GAAG,CAACK,GAAzB,CADK,CACyB;AAChC;A