\\finalProject\\src\\views\\User\\detail.vue"],"sourcesContent":["<template>\r\n    <div class=\"header\">\r\n        <Header></Header>\r\n    </div>\r\n    <div class=\"equipment-item\">\r\n        <div class=\"center_con clearfix\">\r\n            <div class=\"main_menu fl\">\r\n                <img :src=\"picture\" />\r\n            </div>\r\n            <div class=\"goods_detail_list fr\">\r\n                <h3>{{ medicineName }}</h3><br>\r\n                <h2>功能:</h2>\r\n                <p>{{ feature }}</p>\r\n                <h2>类型:</h2>\r\n                <p>{{ typeName }}</p>\r\n                <div class=\"prize_bar\">\r\n                    <div class=\"show_prize fl\">￥<em>{{ price }}</em></div>\r\n                    <div class=\"show_unit fl\">单位：个</div>\r\n                </div>\r\n                <br><br><br>\r\n                <el-input-number v-model=\"count\" :min=\"1\" :max=\"99\" @change=\"handleChange\" />\r\n                <div class=\"operate_btn\">\r\n                    <button class=\"buy_btn\" >立即购买</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n    \r\n<script>\r\n\r\nimport axios from 'axios';\r\nimport Header from '../userLayout/Header/header.vue';\r\n\r\nexport default {\r\n    components: {\r\n        Header\r\n    },\r\n    data() {\r\n        return {\r\n            userId: sessionStorage.getItem(\"userId\"),\r\n            medicineId: '',\r\n            medicineName: '',\r\n            feature: '',\r\n            picture: '',\r\n            price: null,\r\n            typeName: '',\r\n            count: '',\r\n\r\n            details: [], // 定义一个空数组用于存放数据\r\n        };\r\n    },\r\n    mounted() {\r\n        this.medicineId = this.$route.query.medicineId;\r\n        axios.get(`http://localhost:8080/detail?medicineId=${this.medicineId}`).then((res) => {\r\n            //   this.details = res.data; // 将获取到的数据存储到data中\r\n            this.medicineName = res.data.medicineName\r\n            this.picture = res.data.picture\r\n            this.feature = res.data.feature\r\n            this.medicineId = res.data.medicineId\r\n            this.price = res.data.price\r\n            this.typeName = res.data.typeName\r\n            console.log(\"++++++++++++++++++++\")\r\n            console.log(this.details)\r\n            console.log(\"+++++++++++++++++++\")\r\n        }).catch((error) => {\r\n            console.log(error);\r\n        });\r\n    },\r\n    methods: {\r\n        buyNow() {\r\n    if (!this.count) {\r\n      alert(\"请选择购买数量\");\r\n      return;\r\n    }\r\n\r\n    axios.get(\"http://localhost:8080/insertOrder\", {\r\n        params: {\r\n          userId: this.userId,\r\n          medicineId: this.medicineId,\r\n          medicineName: this.medicineName,\r\n          price: this.price,\r\n          count: this.count\r\n        }\r\n      })\r\n      .then(res => {\r\n        if (res.data.code === 0) {\r\n          this.$router.push(\"/order\");\r\n        } else {\r\n          alert(\"下单失败：\" + res.data.msg);\r\n        }\r\n      })\r\n      .catch(error => {\r\n        console.log(error);\r\n        alert(\"下单失败\");\r\n      });\r\n  },\r\n        handleChange(value) {\r\n            console.log(value);\r\n        }\r\n\r\n    },\r\n}\r\n\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.equipment-item {\r\n    margin-top: 10%;\r\n}\r\n\r\n.center_con .main_menu img {\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: cover;\r\n}\r\n\r\n.main {\r\n    margin-top: 100px;\r\n}\r\n\r\nbody {\r\n    font-family: 'Microsoft YaHei';\r\n    color: #666;\r\n    font-size: 12px;\r\n}\r\n\r\n.center_con {\r\n    width: 1200px;\r\n    height: 350px;\r\n    margin: 0 auto;\r\n    /*background: yellow;*/\r\n}\r\n\r\n.center_con .main_menu {\r\n    width: 350px;\r\n    height: 350px;\r\n    overflow: hidden;\r\n}\r\n\r\n.goods_detail_list {\r\n    width: 730px;\r\n    height: 350px;\r\n}\r\n\r\n.goods_detail_list h3 {\r\n    fo