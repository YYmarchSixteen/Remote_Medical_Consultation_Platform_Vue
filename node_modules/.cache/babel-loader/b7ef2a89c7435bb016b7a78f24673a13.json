{"ast":null,"code":"import { get } from '@/utils/request';\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      pageParams: {\n        // 页码  每页几条\n        page: 1,\n        pagesize: 3\n      },\n      curPage: 0,\n      // 当前页数\n      list: [],\n      // 角色列表\n      total: 0,\n      // 一共几条数据\n      dialogVisible: false,\n      dialogVisible1: false,\n      info2: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      list: [],\n      adg: []\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    get('http://localhost:8080/userList').then(res => {\n      console.log(res);\n      this.list = res.data;\n    });\n  },\n\n  methods: {\n    // 分页\n    hCurrentChange(curPage) {\n      // alert(curPage)\n      this.pageParams.page = curPage;\n      this.loadRoles();\n    },\n\n    hSizeChange(pagesize) {\n      // 1. 更新每页条数\n      this.pageParams.pagesize = pagesize; // 2. 重发请求\n\n      this.loadRoles();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoCA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,cAA1B;AACA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,gBAAU,EAAE;AACE;AACAC,YAAI,EAAE,CAFR;AAGEC,gBAAQ,EAAE;AAHZ,OADP;AAMKC,aAAO,EAAE,CANd;AAMiB;AACZC,UAAI,EAAE,EAPX;AAOe;AACVC,WAAK,EAAE,CARZ;AAQe;AAEpBC,mBAAa,EAAE,KAVV;AAWLC,oBAAc,EAAE,KAXX;AAYLC,WAAK,EAAE,EAZF;AAYM;AACXC,WAAK,EAAE,EAbF;AAaM;AACXL,UAAI,EAAE,EAdD;AAeLM,SAAG,EAAE;AAfA,KAAP;AAiBD,GAnBY;;AAoBb;AACAC,SAAO,GAAG;AACRd,OAAG,CAAC,gCAAD,CAAH,CAAsCe,IAAtC,CAA4CC,GAAD,IAAS;AAClDC,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,WAAKT,IAAL,GAAYS,GAAG,CAACd,IAAhB;AACD,KAHD;AAID,GA1BY;;AA2BbiB,SAAO,EAAE;AACF;AACAC,kBAAc,CAACd,OAAD,EAAU;AACjB;AACA,WAAKH,UAAL,CAAgBC,IAAhB,GAAuBE,OAAvB;AACA,WAAKe,SAAL;AACH,KANF;;AAOCC,eAAW,CAACjB,QAAD,EAAW;AAClB;AACA,WAAKF,UAAL,CAAgBE,QAAhB,GAA2BA,QAA3B,CAFkB,CAGlB;;AACA,WAAKgB,SAAL;AACH;;AAZF;AA3BI,CAAf","names":["get","ElMessage","data","pageParams","page","pagesize","curPage","list","total","dialogVisible","dialogVisible1","info2","info1","adg","mounted","then","res","cons