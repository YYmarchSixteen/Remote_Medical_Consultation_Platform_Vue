        total: 0, // 一共几条数据\r\n            \r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      info2: {}, //修改数据\r\n      info1: {}, //新增数据\r\n      list: [],\r\n      adg: [],\r\n    }\r\n  },\r\n  //初始化工作\r\n  mounted() {\r\n    get('http://localhost:8080/userList').then((res) => {\r\n      console.log(res)\r\n      this.list = res.data\r\n    })\r\n  },\r\n  methods: {\r\n         // 分页\r\n         hCurrentChange(curPage) {\r\n                // alert(curPage)\r\n                this.pageParams.page = curPage\r\n                this.loadRoles()\r\n            },\r\n            hSizeChange(pagesize) {\r\n                // 1. 更新每页条数\r\n                this.pageParams.pagesize = pagesize\r\n                // 2. 重发请求\r\n                this.loadRoles()\r\n            },\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n.box-card {\r\n  margin-top: 5px;\r\n  margin-left: 15px;\r\n  margin-right: 15px;\r\n  height: 96%;\r\n}\r\n.a{\r\n  margin-bottom: 99%;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}