{"ast":null,"code":"import axios from 'axios';\nimport { reactive, onMounted } from 'vue';\nimport Header from '../userLayout/Header/header.vue';\nexport default {\n  components: {\n    Header\n  },\n\n  data() {\n    return {\n      form: {},\n      username: sessionStorage.getItem('username'),\n      userid: sessionStorage.getItem('userid')\n    };\n  },\n\n  setup() {\n    const equipmentList = reactive([]);\n\n    const handleBuy = index => {\n      alert(`购买了${equipmentList[index].equipmentname}`);\n    }; // 发起请求并赋值给equipmentList\n\n\n    const getEquipmentList = async () => {\n      try {\n        const response = await axios.get('http://localhost:8080/myCollect', {\n          params: {\n            username: sessionStorage.getItem('username')\n          }\n        });\n        equipmentList.splice(0, equipmentList.length, ...response.data);\n      } catch (error) {\n        console.error(error);\n      }\n    }; // 组件挂载时调用getEquipmentList方法获取数据\n\n\n    onMounted(getEquipmentList);\n    return {\n      equipmentList,\n      handleBuy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA4BA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAoC,KAApC;AACA,OAAOC,MAAP,MAAmB,iCAAnB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVD;AADU,GADC;;AAIbE,MAAI,GAAG;AACL,WAAO;AACLC,UAAI,EAAE,EADD;AAELC,cAAQ,EAAEC,cAAc,CAACC,OAAf,CAAuB,UAAvB,CAFL;AAGLC,YAAM,EAAEF,cAAc,CAACC,OAAf,CAAuB,QAAvB;AAHH,KAAP;AAKD,GAVY;;AAWbE,OAAK,GAAG;AACN,UAAMC,aAAY,GAAIX,QAAQ,CAAC,EAAD,CAA9B;;AAEA,UAAMY,SAAQ,GAAKC,KAAD,IAAW;AAC3BC,WAAK,CAAE,MAAKH,aAAa,CAACE,KAAD,CAAb,CAAqBE,aAAc,EAA1C,CAAL;AACD,KAFD,CAHM,CAON;;;AACA,UAAMC,gBAAe,GAAI,YAAY;AACnC,UAAI;AACF,cAAMC,QAAO,GAAI,MAAMlB,KAAK,CAACmB,GAAN,CAAU,iCAAV,EAA6C;AAClEC,gBAAM,EAAE;AACNb,oBAAQ,EAAEC,cAAc,CAACC,OAAf,CAAuB,UAAvB;AADJ;AAD0D,SAA7C,CAAvB;AAKAG,qBAAa,CAACS,MAAd,CAAqB,CAArB,EAAwBT,aAAa,CAACU,MAAtC,EAA8C,GAAGJ,QAAQ,CAACb,IAA1D;AACF,OAPA,CAOE,OAAOkB,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAcA,KAAd;AACF;AACD,KAXD,CARM,CAqBN;;;AACArB,aAAS,CAACe,gBAAD,CAAT;AAEA,WAAO;AACLL,mBADK;AAELC;AAFK,KAAP;AAID;;AAvCY,CAAf","names":["axios","reactive","onMounted","Header","components","data","form","username","sessionStorage","getItem","userid","setup","equipmentList","handleBuy","index","alert","equipmentname","getEquipmentList","response","get","params","splice","length","error","console"],"sourceRoot":"","sources":["D:\\Dev\\Front\\demo01\\src\\views\\User\\myCollect.vue"],"sourcesContent":["<template>\r\n  <div class=\"header\">\r\n    <Header></Header>\r\n  </div>\r\n  <div class=\"body\">\r\n    <div class=\"equipment-list\">\r\n      <div class=\"equipment-item\" v-for=\"(item, index) in equipmentList\" :key=\"index\">\r\n        <div class=\"img-wrapper\">\r\n          <img :src=\"item.picture\" alt=\"\" style=\"width: 200px; height: auto;\">\r\n        </div>\r\n        <div class=\"info-wrapper\">\r\n          <div class=\"title\">{{ item.equipmentname }}</div>\r\n          <div class=\"price\">单价: ￥{{ item.price }}</div>\r\n          <div class=\"feature\">\r\n            <h2>功能:</h2> {{ item.feature }}\r\n          </div>\r\n          <div class=\"material\">\r\n            <h2>材质：</h2>{{ item.material }}\r\n          </div>\r\n          <button class=\"buy-button\" @click=\"handleBuy(index)\">立即购买</button>\r\n          <button class=\"collect-button\" @click=\"deleteCollect(item)\">取消收藏</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n  \r\n<script>\r\nimport axios from 'axios';\r\nimport { reactive, onMounted } from 'vue';\r\nimport Header from '../userLayout/Header/header.vue';\r\n\r\nexport default {\r\n  components: {\r\n    Header\r\n  },\r\n  data() {\r\n    return {\r\n      form: {},\r\n      username: sessionStorage.getItem('username'),\r\n      userid: sessionStorage.getItem('userid'),\r\n    }\r\n  },\r\n  setup() {\r\n    const equipmentList = reactive([]);\r\n\r\n    const handleBuy = (index) => {\r\n      alert(`购买了${equipmentList[index].equipmentname}`);\r\n    };\r\n\r\n    // 发起请求并赋值给equipmentList\r\n    const getEquipmentList = async () => {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/myCollect', {\r\n          params: {\r\n            username: sessionStorage.getItem('username')\r\n          }\r\n        })\r\n        equipmentList.splice(0, equipmentList.length, ...response.data);\r\n      } catch (error) {\r\n        console.error(error);\r\n      }\r\n    };\r\n\r\n    // 组件挂载时调用getEquipmentList方法获取数据\r\n    onMounted(getEquipmentList);\r\n\r\n    return {\r\n      equipmentList,\r\n      handleBuy\r\n    };\r\n  },\r\n  \r\n};\r\n</script>\r\n  \r\n<style scoped>\r\nh2 {\r\n  font-weight: bolder;\r\n}\r\n\r\n.body {\r\n  margin-top: 100px;\r\n}\r\n\r\n.equipment-item {\r\n  display: flex;\r\n  margin-bottom: 20px;\r\n  background-color: #fff;\r\n  box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.1);\r\n  border: 1px solid #cccccc;\r\n  /* 添加淡黑色边框 */\r\n}\r\n\r\n.img-wrapper {\r\n  width: 35%;\r\n  padding: 10px 10px 10px 200px;\r\n  /* 左右留白150px，上下10px */\r\n  margin: 0 10px 0 0;\r\n  /* 水平间距10px */\r\n}\r\n\r\n.img-wrapper img {\r\n  display: block;\r\n  width: 100%;\r\n  height: auto;\r\n}\r\n\r\n.info-wrapper {\r\n  width: 165%;\r\n  padding: 10px 150px 10px 10px;\r\n  /* 左右留白150px，上下10px */\r\n  margin: 0 0 0 10px;\r\n  /* 水平间距10px */\r\n}\r\n\r\n.title {\r\n  font-weight: bold;\r\n  font-size: 18px;\r\n  color: #333;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.price {\r\n  font-size: 16px;\r\n  color: red;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.feature,\r\n.material {\r\n  font-size: 14px;\r\n  color: #555;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.buy-button {\r\n  padding: 8px 18px;\r\n  border: none;\r\n  background-color: #ff8a00;\r\n  color: #fff;\r\n  cursor: pointer;\r\n}\r\n\r\n.collect-button {\r\n  padding: 8px 18px;\r\n  border: none;\r\n  background-color: #707070;\r\n  color: #fff;\r\n  cursor: pointer;\r\n  margin-left: 10px;\r\n}\r\n\r\n.buy-button:hover {\r\n  background-color: #ff9f3e;\r\n}\r\n</style>\r\n  "]},"metadata":{},"sourceType":"module"}