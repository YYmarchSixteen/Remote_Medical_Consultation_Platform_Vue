{"ast":null,"code":"import axios from 'axios';\nimport Header from '../userLayout/Header/header.vue';\nexport default {\n  components: {\n    Header\n  },\n\n  data() {\n    return {\n      form: {},\n      username: sessionStorage.getItem('username'),\n      userid: sessionStorage.getItem('userid'),\n      isLoading: false,\n      medicineList: [],\n      equipmentTypes: [],\n      selectedType: '',\n      back: []\n    };\n  },\n\n  mounted() {\n    this.getEquipmentList();\n    this.fetchEquipmentTypes();\n  },\n\n  methods: {\n    equipmentSearch() {\n      if (!this.searchTerm) {\n        // 如果搜索框为空，就显示全部\n        this.getEquipmentList();\n        return;\n      }\n\n      const keyword = this.searchTerm.trim();\n      const filteredList = this.equipmentList.filter(item => item.equipmentname.indexOf(keyword) !== -1 || item.feature.indexOf(keyword) !== -1 || item.material.indexOf(keyword) !== -1);\n      this.updateEquipmentList(filteredList);\n    },\n\n    changeSelect() {\n      alert(\"当前显示类型\" + this.selectedType);\n      axios.get('http://localhost:8080/typeSearch?type=' + this.selectedType).then(res => {\n        console.log(res.data);\n        this.equipmentList = res.data;\n        console.log(this.equipmentList);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    async fetchEquipmentTypes() {\n      try {\n        const res = await axios.get('http://localhost:8080/equipmenttypeList');\n        this.equipmentTypes.push(...res.data);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async getEquipmentList() {\n      try {\n        const response = await axios.get('http://localhost:8080/allMedicine');\n        this.medicineList = response.data.data;\n      } catch (error) {\n        console.error(error);\n      }\n    },\n\n    updateEquipmentList(newList) {\n      this.equipmentList = newList;\n    },\n\n    detail(item) {\n      this.$router.push({\n        path: '/detail',\n        query: {\n          id: item.id\n        }\n      });\n    },\n\n    async insertCollect(item) {\n      if (this.isLoading) {\n        return;\n      }\n\n      try {\n        this.isLoading = true;\n        const response = await axios.get('http://localhost:8080/collect', {\n          params: {\n            userid: sessionStorage.getItem('userid'),\n            equipmentid: item.id\n          }\n        });\n\n        if (response.status === 200) {\n          if (response.data === '当前商品已收藏') {\n            this.$message.error('当前商品已收藏');\n          } else if (response.data === '收藏成功') {\n            this.$message.success('收藏成功');\n            item.isCollected = true;\n          } else if (response.data === '收藏失败，请稍后再试') {\n            this.$message.error('收藏失败，请稍后再试');\n          }\n        }\n      } catch (error) {\n        console.error(error);\n      } finally {\n        this.isLoading = false;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iCAAnB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVD;AADU,GADC;;AAIbE,MAAI,GAAG;AACL,WAAO;AACLC,UAAI,EAAE,EADD;AAELC,cAAQ,EAAEC,cAAc,CAACC,OAAf,CAAuB,UAAvB,CAFL;AAGLC,YAAM,EAAEF,cAAc,CAACC,OAAf,CAAuB,QAAvB,CAHH;AAILE,eAAS,EAAE,KAJN;AAKLC,kBAAY,EAAE,EALT;AAMLC,oBAAc,EAAE,EANX;AAOLC,kBAAY,EAAE,EAPT;AAQLC,UAAI,EAAE;AARD,KAAP;AAUD,GAfY;;AAiBbC,SAAO,GAAG;AACR,SAAKC,gBAAL;AACA,SAAKC,mBAAL;AAED,GArBY;;AAuBbC,SAAO,EAAE;AACPC,mBAAe,GAAG;AACpB,UAAI,CAAC,KAAKC,UAAV,EAAsB;AAAE;AACtB,aAAKJ,gBAAL;AACA;AACF;;AACA,YAAMK,OAAM,GAAI,KAAKD,UAAL,CAAgBE,IAAhB,EAAhB;AACA,YAAMC,YAAW,GAAI,KAAKC,aAAL,CAAmBC,MAAnB,CAA2BC,IAAD,IAC7CA,IAAI,CAACC,aAAL,CAAmBC,OAAnB,CAA2BP,OAA3B,MAAwC,CAAC,CAAzC,IACAK,IAAI,CAACG,OAAL,CAAaD,OAAb,CAAqBP,OAArB,MAAkC,CAAC,CADnC,IAEAK,IAAI,CAACI,QAAL,CAAcF,OAAd,CAAsBP,OAAtB,MAAmC,CAAC,CAHjB,CAArB;AAKA,WAAKU,mBAAL,CAAyBR,YAAzB;AACD,KAbU;;AAcPS,gBAAY,GAAG;AACbC,WAAK,CAAC,WAAW,KAAKpB,YAAjB,CAAL;AACAZ,WAAK,CAACiC,GAAN,CAAU,2CAA2C,KAAKrB,YAA1D,EACGsB,IADH,CACQC,GAAE,IAAK;AACXC,eAAO,CAACC,GAAR,CAA