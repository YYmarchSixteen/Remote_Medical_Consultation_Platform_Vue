{"ast":null,"code":"import axios from 'axios';\nimport { reactive, onMounted } from 'vue';\nimport Header from '../userLayout/Header/header.vue';\nexport default {\n  components: {\n    Header\n  },\n\n  setup() {\n    const equipmentList = reactive([]);\n\n    const handleBuy = index => {\n      alert(`购买了${equipmentList[index].equipmentname}`);\n    }; // 发起请求并赋值给equipmentList\n\n\n    const getEquipmentList = async () => {\n      try {\n        const response = await axios.get('http://localhost:8080/equipmentList');\n        equipmentList.splice(0, equipmentList.length, ...response.data);\n      } catch (error) {\n        console.error(error);\n      }\n    }; // 组件挂载时调用getEquipmentList方法获取数据\n\n\n    onMounted(getEquipmentList);\n    return {\n      equipmentList,\n      handleBuy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoBE,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,QAAR,EAAkBC,SAAlB,QAAkC,KAAlC;AACA,OAAOC,MAAP,MAAmB,iCAAnB;AAGA,eAAe;AACbC,YAAU,EAAE;AACZD;AADY,GADC;;AAIbE,OAAK,GAAG;AACN,UAAMC,aAAY,GAAIL,QAAQ,CAAC,EAAD,CAA9B;;AAEA,UAAMM,SAAQ,GAAKC,KAAD,IAAW;AAC3BC,WAAK,CAAE,MAAKH,aAAa,CAACE,KAAD,CAAb,CAAqBE,aAAc,EAA1C,CAAL;AACD,KAFD,CAHM,CAON;;;AACA,UAAMC,gBAAe,GAAI,YAAY;AACnC,UAAI;AACF,cAAMC,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAN,CAAU,qCAAV,CAAvB;AACAP,qBAAa,CAACQ,MAAd,CAAqB,CAArB,EAAwBR,aAAa,CAACS,MAAtC,EAA8C,GAAGH,QAAQ,CAACI,IAA1D;AACF,OAHA,CAGE,OAAOC,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAcA,KAAd;AACF;AACD,KAPD,CARM,CAiBN;;;AACAf,aAAS,CAACS,gBAAD,CAAT;AAEA,WAAO;AACLL,mBADK;AAELC;AAFK,KAAP;AAIF;;AA5Ba,CAAf","names":["axios","reactive","onMounted","Header","components","setup","equipmentList","handleBuy","index","alert","equipmentname","getEquipmentList","response","get","splice","length","data","error","console"],"sourceRoot":"","sources":["D:\\Dev\\Front\\demo01\\src\\views\\User\\userIndex.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        \r\n    </div>\r\n     <Header></Header>\r\n    <div class=\"equipment-list\">\r\n      <div class=\"equipment-item\" v-for=\"(item, index) in equipmentList\" :key=\"index\">\r\n        <img :src=\"item.picture\" alt=\"\">\r\n        <div class=\"item-info\">\r\n          <div class=\"title\">{{ item.equipmentname }}</div>\r\n          <div class=\"price\">￥{{ item.price }}</div>\r\n          <div class=\"feature\">{{ item.feature }}</div>\r\n          <div class=\"material\">材质：{{ item.material }}</div>\r\n          <button class=\"buy-button\" @click=\"handleBuy(index)\">立即购买</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  import {reactive, onMounted} from 'vue';\r\n  import Header from '../userLayout/Header/header.vue';\r\n \r\n  \r\n  export default {\r\n    components: {\r\n    Header\r\n  },\r\n    setup() {\r\n      const equipmentList = reactive([]);\r\n  \r\n      const handleBuy = (index) => {\r\n        alert(`购买了${equipmentList[index].equipmentname}`);\r\n      };\r\n  \r\n      // 发起请求并赋值给equipmentList\r\n      const getEquipmentList = async () => {\r\n        try {\r\n          const response = await axios.get('http://localhost:8080/equipmentList');\r\n          equipmentList.splice(0, equipmentList.length, ...response.data);\r\n        } catch (error) {\r\n          console.error(error);\r\n        }\r\n      };\r\n  \r\n      // 组件挂载时调用getEquipmentList方法获取数据\r\n      onMounted(getEquipmentList);\r\n  \r\n      return {\r\n        equipmentList,\r\n        handleBuy\r\n      };\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  .equipment-list {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n  }\r\n  \r\n  .equipment-item {\r\n    width: 25%;\r\n    padding: 10px;\r\n  }\r\n  \r\n  .equipment-item img {\r\n    width: 100%;\r\n    height: 180px;\r\n    object-fit: cover;\r\n  }\r\n  \r\n  .item-info {\r\n    margin-top: 10px;\r\n  }\r\n  \r\n  .title {\r\n    font-weight: bold;\r\n    font-size: 18px;\r\n    color: #333;\r\n  }\r\n  \r\n  .price {\r\n    font-size: 16px;\r\n    color: red;\r\n    margin-top: 5px;\r\n  }\r\n  \r\n  .feature {\r\n    margin-top: 5px;\r\n  }\r\n  \r\n  .material {\r\n    margin-top: 5px;\r\n  }\r\n  \r\n  .buy-button {\r\n    margin-top: 10px;\r\n    padding: 5px 10px;\r\n    border: 1px solid #ccc;\r\n    background-color: #fff;\r\n    cursor: pointer;\r\n    outline: none;\r\n  }\r\n  \r\n  .buy-button:hover {\r\n    background-color: #ddd;\r\n  }\r\n  body {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n  </style>\r\n  "]},"metadata":{},"sourceType":"module"}