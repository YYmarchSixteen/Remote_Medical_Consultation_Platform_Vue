{"ast":null,"code":"import axios from 'axios';\nimport Header from '../userLayout/Header/header.vue';\nexport default {\n  components: {\n    Header\n  },\n\n  data() {\n    return {\n      form: {},\n      username: sessionStorage.getItem('username'),\n      userid: sessionStorage.getItem('userid'),\n      isLoading: false,\n      medicineList: [],\n      equipmentTypes: [],\n      selectedType: '',\n      back: []\n    };\n  },\n\n  mounted() {\n    this.getEquipmentList();\n    this.fetchEquipmentTypes();\n  },\n\n  methods: {\n    equipmentSearch() {\n      if (!this.searchTerm) {\n        // 如果搜索框为空，就显示全部\n        this.getEquipmentList();\n        return;\n      }\n\n      const keyword = this.searchTerm.trim();\n      const filteredList = this.equipmentList.filter(item => item.equipmentname.indexOf(keyword) !== -1 || item.feature.indexOf(keyword) !== -1 || item.material.indexOf(keyword) !== -1);\n      this.updateEquipmentList(filteredList);\n    },\n\n    changeSelect() {\n      alert(\"当前显示类型\" + this.selectedType);\n      axios.get('http://localhost:8080/typeSearch?type=' + this.selectedType).then(res => {\n        console.log(res.data);\n        this.equipmentList = res.data;\n        console.log(this.equipmentList);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    async fetchEquipmentTypes() {\n      try {\n        const res = await axios.get('http://localhost:8080/allMedicineType');\n        this.Types.push(...res.data);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async getEquipmentList() {\n      try {\n        const response = await axios.get('http://localhost:8080/allMedicine');\n        this.medicineList = response.data.data;\n      } catch (error) {\n        console.error(error);\n      }\n    },\n\n    updateEquipmentList(newList) {\n      this.equipmentList = newList;\n    },\n\n    detail(item) {\n      this.$router.push({\n        path: '/detail',\n        query: {\n          id: item.id\n        }\n      });\n    },\n\n    async insertCollect(item) {\n      if (this.isLoading) {\n        return;\n      }\n\n      try {\n        this.isLoading = true;\n        const response = await axios.get('http://localhost:8080/collect', {\n          params: {\n            userid: sessionStorage.getItem('userid'),\n            equipmentid: item.id\n          }\n        });\n\n        if (response.status === 200) {\n          if (response.data === '当前商品已收藏') {\n            this.$message.error('当前商品已收藏');\n          } else if (response.data === '收藏成功') {\n            this.$message.success('收藏成功');\n            item.isCollected = true;\n          } else if (response.data === '收藏失败，请稍后再试') {\n            this.$message.error('收藏失败，请稍后再试');\n          }\n        }\n      } catch (error) {\n        console.error(error);\n      } finally {\n        this.isLoading = false;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iCAAnB;AAEA,eAAe;AACXC,YAAU,EAAE;AACRD;AADQ,GADD;;AAIXE,MAAI,GAAG;AACH,WAAO;AACHC,UAAI,EAAE,EADH;AAEHC,cAAQ,EAAEC,cAAc,CAACC,OAAf,CAAuB,UAAvB,CAFP;AAGHC,YAAM,EAAEF,cAAc,CAACC,OAAf,CAAuB,QAAvB,CAHL;AAIHE,eAAS,EAAE,KAJR;AAKHC,kBAAY,EAAE,EALX;AAMHC,oBAAc,EAAE,EANb;AAOHC,kBAAY,EAAE,EAPX;AAQHC,UAAI,EAAE;AARH,KAAP;AAUH,GAfU;;AAiBXC,SAAO,GAAG;AACN,SAAKC,gBAAL;AACA,SAAKC,mBAAL;AAEH,GArBU;;AAuBXC,SAAO,EAAE;AACLC,mBAAe,GAAG;AACd,UAAI,CAAC,KAAKC,UAAV,EAAsB;AAAE;AACpB,aAAKJ,gBAAL;AACA;AACJ;;AACA,YAAMK,OAAM,GAAI,KAAKD,UAAL,CAAgBE,IAAhB,EAAhB;AACA,YAAMC,YAAW,GAAI,KAAKC,aAAL,CAAmBC,MAAnB,CAA2BC,IAAD,IAC3CA,IAAI,CAACC,aAAL,CAAmBC,OAAnB,CAA2BP,OAA3B,MAAwC,CAAC,CAAzC,IACAK,IAAI,CAACG,OAAL,CAAaD,OAAb,CAAqBP,OAArB,MAAkC,CAAC,CADnC,IAEAK,IAAI,CAACI,QAAL,CAAcF,OAAd,CAAsBP,OAAtB,MAAmC,CAAC,CAHnB,CAArB;AAKA,WAAKU,mBAAL,CAAyBR,YAAzB;AACH,KAbI;;AAcLS,gBAAY,GAAG;AACXC,WAAK,CAAC,WAAW,KAAKpB,YAAjB,CAAL;AACAZ,WAAK,CAACiC,GAAN,CAAU,2CAA2C,KAAKrB,YAA1D,EACKsB,IADL,CACUC,GAAE,IAAK;AACTC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAAC