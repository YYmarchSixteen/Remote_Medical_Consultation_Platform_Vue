{"ast":null,"code":"import { get } from '@/utils/request';\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 15,\n      //默认一页显示 条\n      dialogVisible: false,\n      dialogVisible1: false,\n      info2: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      list: [],\n      adg: []\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    get('http://localhost:8080/equipmenttypeList').then(res => {\n      console.log(res);\n      this.list = res.data;\n    });\n  },\n\n  methods: {\n    del(data) {\n      console.log(data.row.id);\n      get('http://localhost:8080/deleteType', {\n        id: data.row.id\n      }).then(() => {\n        ElMessage({\n          message: '删除成功！',\n          type: 'success'\n        });\n      });\n      window.location.reload(); //刷新\n    },\n\n    add() {\n      this.dialogVisible1 = !this.dialogVisible1;\n    },\n\n    insert() {\n      this.dialogVisible1 = !this.dialogVisible1;\n      get('http://localhost:8080/insertType', this.info1).then(res => {\n        console.log(res);\n\n        if (res.data == '添加成功') {\n          ElMessage({\n            message: '添加成功',\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '添加失败',\n            type: 'danger'\n          });\n        }\n      });\n    },\n\n    handleClick(data) {\n      this.info2 = data.row;\n      this.dialogVisible = !this.dialogVisible;\n    },\n\n    edit() {\n      get('http://localhost:8080/updateType', this.info2).then(res => {\n        console.log(res);\n\n        if (res.data == '更改成功') {\n          ElMessage({\n            message: '更改成功',\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '更改失败',\n            type: 'danger'\n          });\n        }\n      });\n    },\n\n    handleSizeChange: functi