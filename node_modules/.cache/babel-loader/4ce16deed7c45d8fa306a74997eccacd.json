{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      user: null,\n      uploadUrl: 'http://localhost:8081/doctor/uploadPicture'\n    };\n  },\n\n  mounted() {\n    this.fetchUser();\n  },\n\n  methods: {\n    async fetchUser() {\n      try {\n        const response = await axios.get('http://localhost:8081/doctor/findById?doctorId=1');\n        this.user = response.data.data;\n      } catch (error) {\n        console.error('Error fetching user:', error);\n      }\n    },\n\n    async updateUser() {\n      try {\n        await axios.put('http://localhost:8081/doctor/changeInfo', this.user);\n        alert('用户信息已更新');\n      } catch (error) {\n        console.error('Error updating user:', error);\n        alert('更新用户信息时出错');\n      }\n    },\n\n    re() {\n      window.location.reload();\n    },\n\n    toPatient() {\n      this.$router.push('/doctor/patient');\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6DA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,UAAI,EAAE,IADD;AAELC,eAAS,EAAE;AAFN,KAAP;AAID,GANY;;AAObC,SAAO,GAAG;AACR,SAAKC,SAAL;AACD,GATY;;AAUbC,SAAO,EAAE;AACP,UAAMD,SAAN,GAAkB;AAChB,UAAI;AACF,cAAME,QAAO,GAAI,MAAMP,KAAK,CAACQ,GAAN,CAAU,kDAAV,CAAvB;AACA,aAAKN,IAAL,GAAYK,QAAQ,CAACN,IAAT,CAAcA,IAA1B;AACF,OAHA,CAGE,OAAOQ,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,sBAAd,EAAsCA,KAAtC;AACF;AACD,KARM;;AASP,UAAME,UAAN,GAAmB;AACjB,UAAI;AACF,cAAMX,KAAK,CAACY,GAAN,CAAU,yCAAV,EAAqD,KAAKV,IAA1D,CAAN;AACAW,aAAK,CAAC,SAAD,CAAL;AACF,OAHA,CAGE,OAAOJ,KAAP,EAAc;AACdC,eAAO,CAACD,KAAR,CAAc,sBAAd,EAAsCA,KAAtC;AACAI,aAAK,CAAC,WAAD,CAAL;AACF;AACD,KAjBM;;AAkBPC,MAAE,GAAE;AACFC,YAAM,CAACC,QAAP,CAAgBC,MAAhB;AACD,KApBM;;AAqBPC,aAAS,GAAE;AACT,WAAKC,OAAL,CAAaC,IAAb,CAAkB,iBAAlB;AACD;;AAvBM;AAVI,CAAf","names":["axios","data","user","uploadUrl","mounted","fetchUser","methods","response","get","error","console","updateUser","put","alert","re","window","location","reload","toPatient","$router","push"],"sourceRoot":"","sources":["D:\\