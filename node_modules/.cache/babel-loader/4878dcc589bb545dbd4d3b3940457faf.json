adUrl\"\r\n            :show-file-list=\"false\"\r\n            :data={doctorId:1}\r\n            :on-success=\"re\"\r\n            name=\"picture\"\r\n          >\r\n            <img v-if=\"user.picture\" :src=\"user.picture\" class=\"avatar\">\r\n            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n          </el-upload>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"updateUser\">更新用户信息</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n    <div v-else>\r\n      <el-alert\r\n        title=\"正在加载用户信息...\"\r\n        type=\"info\"\r\n        show-icon\r\n      ></el-alert>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      user: null,\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchUser()\r\n  },\r\n  methods: {\r\n    async fetchUser() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8081/admin/all')\r\n        this.user = response.data.data\r\n      } catch (error) {\r\n        console.error('Error fetching user:', error)\r\n      }\r\n    },\r\n    async updateUser() {\r\n      try {\r\n        await axios.put('http://localhost:8081/doctor/update', this.user)\r\n        alert('用户信息已更新')\r\n      } catch (error) {\r\n        console.error('Error updating user:', error)\r\n        alert('更新用户信息时出错')\r\n      }\r\n    },\r\n    re(){\r\n      window.location.reload();\r\n    },\r\n    toAdmin(){\r\n      this.$router.push('/admin/admin')\r\n    },\r\n    toDoctor(){\r\n      this.$router.push('/admin/doctor')\r\n    },\r\n    toNotice(){\r\n      this.$router.push('/admin/notice')\r\n    },\r\n    toPersonal(){\r\n      this.$router.push('/admin/personal')\r\n    },\r\n    toStore(){\r\n      this.$router.push('/admin/store')\r\n    },\r\n    toUser(){\r\n      this.$router.push('/admin/user')\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  margin: 20px auto;\r\n  max-width: 800px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}