{"ast":null,"code":"import axios from 'axios';\nimport { ref, reactive, onMounted } from 'vue';\nexport default {\n  name: 'Header',\n\n  setup() {\n    const searchTerm = ref('');\n    const equipmentTypes = reactive([]); // 反应式对象用于存储器械分类列表\n\n    const handleSearch = () => {\n      const searchUrl = `http://localhost:8080/search?equipmentname=${searchTerm.value}`;\n      window.location.href = searchUrl;\n    }; // 从后端获取器械分类列表并赋值给equipmentTypes\n\n\n    const getEquipmentTypes = async () => {\n      try {\n        const res = await axios.get('http://localhost:8080/equipmenttypeList');\n\n        if (res.status === 200) {\n          equipmentTypes.splice(0);\n          equipmentTypes.push(...res.data);\n        }\n      } catch (error) {\n        console.log(error);\n      }\n    };\n\n    onMounted(() => {\n      getEquipmentTypes();\n    });\n    return {\n      searchTerm,\n      equipmentTypes,\n      handleSearch\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAyCE,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,GAAT,EAAcC,QAAd,EAAwBC,SAAxB,QAAyC,KAAzC;AAEA,eAAe;AACbC,MAAI,EAAE,QADO;;AAEbC,OAAK,GAAG;AACN,UAAMC,UAAS,GAAIL,GAAG,CAAC,EAAD,CAAtB;AACA,UAAMM,cAAa,GAAIL,QAAQ,CAAC,EAAD,CAA/B,CAFM,CAE+B;;AAErC,UAAMM,YAAW,GAAI,MAAM;AACzB,YAAMC,SAAQ,GAAK,8CAA6CH,UAAU,CAACI,KAAM,EAAjF;AACAC,YAAM,CAACC,QAAP,CAAgBC,IAAhB,GAAuBJ,SAAvB;AACD,KAHD,CAJM,CASN;;;AACA,UAAMK,iBAAgB,GAAI,YAAY;AACpC,UAAI;AACF,cAAMC,GAAE,GAAI,MAAMf,KAAK,CAACgB,GAAN,CAAU,yCAAV,CAAlB;;AACA,YAAID,GAAG,CAACE,MAAJ,KAAe,GAAnB,EAAwB;AACtBV,wBAAc,CAACW,MAAf,CAAsB,CAAtB;AACAX,wBAAc,CAACY,IAAf,CAAoB,GAAGJ,GAAG,CAACK,IAA3B;AACF;AACF,OANA,CAME,OAAOC,KAAP,EAAc;AACdC,eAAO,CAACC,GAAR,CAAYF,KAAZ;AACF;AACD,KAVD;;AAYAlB,aAAS,CAAC,MAAM;AACdW,uBAAiB;AAClB,KAFQ,CAAT;AAIA,WAAO;AACLR,gBADK;AAELC,oBAFK;AAGLC;AAHK,KAAP;AAKD;;AAjCY,CAAf","names":["axios","ref","reactive","onMounted","name","setup","searchTerm","equipmentTypes","handleSearch","searchUrl","value","window","location","href","getEquipmentTypes","res","get","status","splice","push","data","error","console","log"],"sourceRoot":"","sources":["D:\\Dev\\Front\\demo01\\src\\views\\userLayout\\Search\\search.vue"],"sourcesContent":["<template>\r\n    <div class=\"header\">\r\n      <el-header class=\"header-wrapper\" height=\"70px\">\r\n        <div class=\"header-logo\">\r\n          <img src=\"../../../assets/OIP-C.jpg\" alt=\"logo\" class=\"logo-img\">\r\n          <span class=\"logo-text\">健康守护者</span>\r\n        </div>\r\n        <div class=\"header-nav\">\r\n          <el-menu :default-active=\"$route.path\" mode=\"horizontal\" router>\r\n            <el-menu-item index=\"/\">首页</el-menu-item>\r\n            <el-submenu index=\"/products\">\r\n              <template #title>\r\n                <div>\r\n                  我的收藏<i class=\"el-icon-arrow-down el-icon--right\"></i>\r\n                </div>\r\n              </template>\r\n              <el-menu-item index=\"/\">个人中心</el-menu-item>\r\n              <el-submenu>\r\n                <template #title>\r\n                  <div>\r\n                    全部器械分类<i class=\"el-icon-arrow-down el-icon--right\"></i>\r\n                  </div>\r\n                </template>\r\n                <el-menu-item v-for=\"(item, index) in equipmentTypes\" :key=\"index\" :index=\"item.id\">\r\n                  {{ item.name }}\r\n                </el-menu-item>\r\n              </el-submenu>\r\n            </el-submenu>\r\n            <el-menu-item index=\"/\">关于我们</el-menu-item>\r\n            <el-menu-item index=\"/\">联系我们</el-menu-item>\r\n          </el-menu>\r\n        </div>\r\n        <div class=\"header-search\">\r\n          <input type=\"text\" v-model=\"searchTerm\" class=\"search-input\" placeholder=\"请输入搜索内容\">\r\n          <button class=\"search-btn\" @click=\"handleSearch\">搜索</button>\r\n        </div>\r\n      </el-header>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  import { ref, reactive, onMounted } from 'vue';\r\n  \r\n  export default {\r\n    name: 'Header',\r\n    setup() {\r\n      const searchTerm = ref('');\r\n      const equipmentTypes = reactive([]); // 反应式对象用于存储器械分类列表\r\n  \r\n      const handleSearch = () => {\r\n        const searchUrl = `http://localhost:8080/search?equipmentname=${searchTerm.value}`;\r\n        window.location.href = searchUrl;\r\n      };\r\n  \r\n      // 从后端获取器械分类列表并赋值给equipmentTypes\r\n      const getEquipmentTypes = async () => {\r\n        try {\r\n          const res = await axios.get('http://localhost:8080/equipmenttypeList');\r\n          if (res.status === 200) {\r\n            equipmentTypes.splice(0);\r\n            equipmentTypes.push(...res.data);\r\n          }\r\n        } catch (error) {\r\n          console.log(error);\r\n        }\r\n      };\r\n  \r\n      onMounted(() => {\r\n        getEquipmentTypes();\r\n      });\r\n  \r\n      return {\r\n        searchTerm,\r\n        equipmentTypes,\r\n        handleSearch,\r\n      };\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .header {\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    right: 0;\r\n    z-index: 999;\r\n  }\r\n  \r\n  .header-wrapper {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    background-color: #fff;\r\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n  }\r\n  \r\n  .header-logo {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-left: 20px;\r\n  }\r\n  \r\n  .logo-img {\r\n    height: 40px;\r\n  }\r\n  \r\n  .logo-text {\r\n    font-size: 20px;\r\n    font-weight: bold;\r\n    margin-left: 10px;\r\n  }\r\n  \r\n  .header-nav {\r\n    margin-right: 120px;\r\n  }\r\n  \r\n  .header-search {\r\n    margin-right: 20px;\r\n  }\r\n  \r\n  .search-input {\r\n    border: 2px solid #7ec8e3;\r\n    border-radius: 20px;\r\n    padding: 6px 12px;\r\n    font-size: 14px;\r\n    color: #333;\r\n  }\r\n  \r\n  .search-btn {\r\n    border: none;\r\n    border-radius: 20px;\r\n    padding: 6px 16px;\r\n    font-size: 14px;\r\n    color: #fff;\r\n    background-color: #b9d76b;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  body {\r\n    font-family: 'Microsoft YaHei';\r\n    color: #666;\r\n    font-size: 12px;\r\n  }\r\n  </style>\r\n  "]},"metadata":{},"sourceType":"module"}