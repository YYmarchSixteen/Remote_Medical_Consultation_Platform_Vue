{"ast":null,"code":"import { get } from '@/utils/request';\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 15,\n      //默认一页显示 条\n      dialogVisible: false,\n      dialogVisible1: false,\n      info2: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      list: [],\n      adg: [],\n      uploadImgUrl: '' // 上传图片的地址\n\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    get('http://localhost:8080/equipmentList').then(res => {\n      console.log(res);\n      this.list = res.data;\n    });\n  },\n\n  methods: {\n    // 上传之前的钩子函数\n    beforeUpload(file) {\n      if (file.type !== 'image/jpeg' && file.type !== 'image/png') {\n        this.$message.error('文件格式错误!');\n        return false;\n      }\n\n      if (file.size / 1024 / 1024 > 2) {\n        this.$message.error('图片大小不能超过 2MB!');\n        return false;\n      }\n\n      this.uploadImgUrl = `http://localhost:8080/uploadEquipmentImg?name=${file.name}`;\n    },\n\n    // 上传成功后的钩子函数\n    handleSuccess(res) {\n      this.info1.picture = res.data.url;\n    },\n\n    del(data) {\n      console.log(data.row.id);\n      get('http://localhost:8080/deleteEquipment', {\n        id: data.row.id\n      }).then(() => {\n        ElMessage({\n          message: '删除成功！',\n          type: 'success'\n        });\n      });\n      window.location.reload(); //刷新\n    },\n\n    add() {\n      this.dialogVisible1 = !this.dialogVisible1;\n    },\n\n    insert() {\n      this.dialogVisible1 = !this.dialogVisible1;\n      get('http://localhost:8080/insertEquipment', this.info1).then(res => {\n        console.log(res);\n\n        if (res.data == '添加成功') {\n          ElMessage({\n            message: '添加成功',\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '添加失败',\n            type: 'danger'\n          });\n        }\n      });\n    },\n\n    handleClick(data) {\n      this.info2 = data.row;\n      this.dialogVisible = !this.dialogVisible;\n    },\n\n    edit() {\n      get('http://localhost:8080/updateEquipment', this.info2).then(res => {\n        console.log(res);\n\n        if (res.data == '修改成功') {\n          ElMessage({\n            message: '修改成功',\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '修改失败',\n            type: 'danger'\n          });\n        }\n      });\n    },\n\n    handleSizeChange: function (size) {\n      //一页显示多少条\n      this.pagesize = size;\n    },\n    handleCurrentChange: function (currentPage) {\n      //页码更改方法\n      this.currentPage = currentPage;\n    }\n  }\n};","map":{"version":3,"mappings":"AAkIA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,cAA1B;AACA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,CADR;AACW;AAChBC,cAAQ,EAAE,EAFL;AAES;AACdC,mBAAa,EAAE,KAHV;AAILC,oBAAc,EAAE,KAJX;AAKLC,WAAK,EAAE,EALF;AAKM;AACXC,WAAK,EAAE,EANF;AAMM;AACXC,UAAI,EAAE,EAPD;AAQLC,SAAG,EAAE,EARA;AASLC,kBAAY,EAAE,EATT,CASa;;AATb,KAAP;AAWD,GAbY;;AAcb;AACAC,SAAO,GAAG;AACRZ,OAAG,CAAC,qCAAD,CAAH,CAA2Ca,IAA3C,CAAiDC,GAAD,IAAS;AACvDC,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,WAAKL,IAAL,GAAYK,GAAG,CAACZ,IAAhB;AACD,KAHD;AAID,GApBY;;AAqBbe,SAAO,EAAE;AACP;AACAC,gBAAY,CAACC,IAAD,EAAO;AACjB,UAAIA,IAAI,CAACC,IAAL,KAAc,YAAd,IAA8BD,IAAI,CAACC,IAAL,KAAc,WAAhD,EAA6D;AAC3D,aAAKC,QAAL,CAAcC,KAAd,CAAoB,SAApB;AACA,eAAO,KAAP;AACF;;AACA,UAAIH,IAAI,CAACI,IAAL,GAAY,IAAZ,GAAmB,IAAnB,GAA0B,CAA9B,EAAiC;AAC/B,aAAKF,QAAL,CAAcC,KAAd,CAAoB,eAApB;AACA,eAAO,KAAP;AACF;;AACA,WAAKX,YAAL,GAAqB,iDAAgDQ,IAAI,CAACK,IAAK,EAA/E;AACD,KAZM;;AAcP;AACAC,iBAAa,CAACX,GAAD,EAAM;AACjB,WAAKN,KAAL,CAAWkB,OAAX,GAAqBZ,GAAG,CAACZ,IAAJ,CAASyB,GAA9B;AACD,KAjBM;;AAkBPC,OAAG,CAAC1B,IAAD,EAAO;AACRa,aAAO,CAACC,GAAR,CAAYd,IAAI,CAAC2B,GAAL,CAASC,EAArB;AACA9B,SAAG,CAAC,uCAAD,EAA0C;AAAE8B,UAAE,EAAE5B,IAAI,CAAC2B,GAAL,CAASC;AAAf,OAA1C,CAAH,CAAkEjB,IAAlE,CAAuE,MAAM;AAC3EZ,iBAAS,CAAC;AACR8B,iB