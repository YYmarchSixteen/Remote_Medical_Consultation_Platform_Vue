/el-button>\r\n        <!-- 删除确认框 -->\r\n        <el-popconfirm title=\"确定要删除吗?\" @confirm=\"del(row)\">\r\n          <template #reference>\r\n            <el-button type=\"danger\">删除</el-button>\r\n          </template>\r\n        </el-popconfirm>\r\n      </template>\r\n    </el-table-column>\r\n  </el-table>\r\n  <el-dialog v-model=\"dialogVisible1\" title=\"增加\" width=\"30%\">\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"用户名\">\r\n        <el-input v-model=\"info1.userName\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"电话\">\r\n        <el-input v-model=\"info1.telephone\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"密码\">\r\n        <el-input v-model=\"info1.password\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"姓名\">\r\n        <el-input v-model=\"info1.realName\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"用户权限\">\r\n        <el-input v-model=\"info1.userRole\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"最后登录日期\">\r\n        <el-input v-model=\"info1.lastLogin\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"登录次数\">\r\n        <el-input v-model=\"info1.loginCount\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"日期\">\r\n        <el-input v-model=\"info1.Data\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible1 = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"insert()\" v-model=\"dialogVisible1\"\r\n          >确认</el-button\r\n        >\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n  <el-dialog v-model=\"dialogVisible\" title=\"编辑\" width=\"30%\">\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"用户名\">\r\n        <el-input v-model=\"info2.userName\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"电话\">\r\n        <el-input v-model=\"info2.telephone\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"密码\">\r\n        <el-input v-model=\"info2.password\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"姓名\">\r\n        <el-input v-model=\"info2.realName\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"用户权限\">\r\n        <el-input v-model=\"info2.userRole\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"最后登录日期\">\r\n        <el-input v-model=\"info2.lastLogin\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"登录次数\">\r\n        <el-input v-model=\"info2.loginCount\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"日期\">\r\n        <el-input v-model=\"info2.Data\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"edit()\" v-model=\"dialogVisible\"\r\n          >确认</el-button\r\n        >\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n  <el-pagination\r\n    currentPage=\"currentPage4\"\r\n    :page-sizes=\"[5, 10, 15, 20]\"\r\n    page-size=\"15\"\r\n    :background=\"background\"\r\n    layout=\"total, sizes, prev, pager, next, jumper\"\r\n    :total=\"tbData.length\"\r\n    @size-change=\"handleSizeChange\"\r\n    @current-change=\"handleCurrentChange\"\r\n  />\r\n</template>\r\n<script>\r\nimport { get } from '@/utils/request'\r\nimport { ElMessage } from 'element-plus'\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentPage: 1, //默认页码为1\r\n      pagesize: 10, //默认一页显示10条\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      info: {}, //修改数据\r\n      info1: {}, //新增数据\r\n      tbData: []\r\n    }\r\n  },\r\n  //初始化工作\r\n  mounted() {\r\n    get('/getUser').then((res) => {\r\n      console.log(res)\r\n      this.tbData = res.data\r\n    })\r\n  },\r\n  methods: {\r\n    handleSizeChange: function (size) {\r\n      //一页显示多少条\r\n\r\n      this.pagesize = size\r\n    },\r\n\r\n    handleCurrentChange: function (currentPage) {\r\n      //页码更改方法\r\n\r\n      this.currentPage = currentPage\r\n    },\r\n    handleClick(data) {\r\n      this.info = data.row\r\n      this.dialogVisible = !this.dialogVisible\r\n    },\r\n    del(data) {\r\n      console.log(data.row.departmentId)\r\n      get('/deleteUser', { departmentId: data.row.departmentId }).then(\r\n        (res) => {\r\n          ElMessage({\r\n            message: '删除成功！',\r\n            type: 'success'\r\n          })\r\n        }\r\n      )\r\n      window.location.reload() //刷新\r\n    },\r\n    edit() {\r\n      this.dialogVisible = !this.dialogVisible //马上消失\r\n      //作用是调整下createTime的格式\r\n      var dt = new Date(this.info.createTime)\r\n      let year = dt.getFullYear()\r\n      let month = dt.getMonth() + 1\r\n      let date = dt.getDate()\r\n      this.info.createTime = year + '/' + month + '/' + date\r\n      get('/updateUser', this.info).then((res) => {\r\n        ElMessage({\r\n          message: '修改成功！',\r\n          type: 'success'\r\n        })\r\n        window.location.reload()\r\n      })\r\n    },\r\n    add() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n    },\r\n    insert() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n      get('/addUser', this.info1).then((res) => {\r\n        console.log(res)\r\n        if (res.data == 1) {\r\n          ElMessage({\r\n            message: '添加成功',\r\n            type: 'success'\r\n          })\r\n          // 刷新\r\n          window.location.reload()\r\n        } else {\r\n          ElMessage({\r\n            message: '添加失败',\r\n            type: 'danger'\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}