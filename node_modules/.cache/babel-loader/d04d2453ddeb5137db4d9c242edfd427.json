{"ast":null,"code":"import { get } from '@/utils/request';\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 10,\n      //默认一页显示10条\n      dialogVisible: false,\n      dialogVisible1: false,\n      info: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      tbData: []\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    get('/getUser').then(res => {\n      console.log(res);\n      this.tbData = res.data;\n    });\n  },\n\n  methods: {\n    handleSizeChange: function (size) {\n      //一页显示多少条\n      this.pagesize = size;\n    },\n    handleCurrentChange: function (currentPage) {\n      //页码更改方法\n      this.currentPage = currentPage;\n    },\n\n    handleClick(data) {\n      this.info = data.row;\n      this.dialogVisible = !this.dialogVisible;\n    },\n\n    del(data) {\n      console.log(data.row.departmentId);\n      get('/deleteUser', {\n        departmentId: data.row.departmentId\n      }).then(res => {\n        ElMessage({\n          message: '删除成功！',\n          type: 'success'\n        });\n      });\n      window.location.reload(); //刷新\n    },\n\n    edit() {\n      this.dialogVisible = !this.dialogVisible; //马上消失\n      //作用是调整下createTime的格式\n\n      var dt = new Date(this.info.createTime);\n      let year = dt.getFullYear();\n      let month = dt.getMonth() + 1;\n      let date = dt.getDate();\n      this.info.createTime = year + '/' + month + '/' + date;\n      get('/updateUser', this.info).then(res => {\n        ElMessage({\n          message: '修改成功！',\n          type: 'success'\n        });\n        window.location.reload();\n      });\n    },\n\n    add() {\n      this.dialogVisible1 = !this.dialogVisible1;\n    },\n\n    insert() {\n      this.dialogVisible1 = !this.dialogVisible1;\n      get('/addUser', this.info1).then(res => {\n        console.log(res);\n\n        if (res.data == 1) {\n          ElMessage({\n            message: '添加成功',\n            type: 'success'\n          }); // 刷新\n\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '添加失败',\n            type: 'danger'\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2IA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,cAA1B;AACA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,CADR;AACW;AAChBC,cAAQ,EAAE,EAFL;AAES;AACdC,mBAAa,EAAE,KAHV;AAILC,oBAAc,EAAE,KAJX;AAKLC,UAAI,EAAE,EALD;AAKK;AACVC,WAAK,EAAE,EANF;AAMM;AACXC,YAAM,EAAE;AAPH,KAAP;AASD,GAXY;;AAYb;AACAC,SAAO,GAAG;AACRV,OAAG,CAAC,UAAD,CAAH,CAAgBW,IAAhB,CAAsBC,GAAD,IAAS;AAC5BC,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,WAAKH,MAAL,GAAcG,GAAG,CAACV,IAAlB;AACD,KAHD;AAID,GAlBY;;AAmBba,SAAO,EAAE;AACPC,oBAAgB,EAAE,UAAUC,IAAV,EAAgB;AAChC;AAEA,WAAKb,QAAL,GAAgBa,IAAhB;AACD,KALM;AAOPC,uBAAmB,EAAE,UAAUf,WAAV,EAAuB;AAC1C;AAEA,WAAKA,WAAL,GAAmBA,WAAnB;AACD,KAXM;;AAYPgB,eAAW,CAACjB,IAAD,EAAO;AAChB,WAAKK,IAAL,GAAYL,IAAI,CAACkB,GAAjB;AACA,WAAKf,aAAL,GAAqB,CAAC,KAAKA,aAA3B;AACD,KAfM;;AAgBPgB,OAAG,CAACnB,IAAD,EAAO;AACRW,aAAO,CAACC,GAAR,CAAYZ,IAAI,CAACkB,GAAL,CAASE,YAArB;AACAtB,SAAG,CAAC,aAAD,EAAgB;AAAEsB,oBAAY,EAAEpB,IAAI,CAACkB,GAAL,CAASE;AAAzB,OAAhB,CAAH,CAA4DX,IAA5D,CACGC,GAAD,IAAS;AACPX,iBAAS,CAAC;AACRsB,iBAAO,EAAE,OADD;AAERC,cAAI,EAAE;AAFE,SAAD,CAAT;AAIF,OANF;AAQAC,YAAM,CAACC,QAAP,CAAgBC,MAAhB,GAVQ,CAUiB;AAC1B,KA3BM;;AA4BPC,QAAI,GAAG;AACL,WAAKvB,aAAL,GAAqB,CAAC,KAAKA,aAA3B,CADK,CACoC;AACzC;;AACA,UAAIwB,EAAC,GAAI,IAAIC,IAAJ,CAAS,KAAKvB,IAAL,CAAUwB,UAAnB,CAAT;AACA,UAAIC,IAAG,GAAIH,EAAE,CAACI,WAAH,EAAX;AACA,UAAIC,KAAI,GAAIL,EAAE,CAACM,QAAH,KAAgB,CAA5B;AACA,UAAIC,IAAG,GAAIP,EAAE,CAACQ,OAAH,EAAX;AACA,WAAK9B,IAAL,CAAUwB,UAAV,GAAuBC,IAAG,GAAI,GAAP,GAAaE,KAAb,GAAqB,GAArB,GAA2BE,IAAlD;AACApC,SAAG,CAAC,aAAD,EAAgB,KAAKO,IAArB,CAAH,CAA8BI,IAA9B,CAAoCC,GAAD,IAAS;AAC1CX,iBAAS,CAAC;AACRsB,iBAAO,EAAE,OADD;AAERC,cAAI,EAAE;AAFE,SAAD,CAAT;AAIAC,cAAM,CAACC,QAAP,CAAgBC,MAAhB;AACD,OAND;AAOD,KA3CM;;AA4CPW,OAAG,GAAG;AACJ,WAAKhC,cAAL,GAAsB,CAAC,KAAKA,cAA5B;AACD,KA9CM;;AA+CPiC,UAAM,GA