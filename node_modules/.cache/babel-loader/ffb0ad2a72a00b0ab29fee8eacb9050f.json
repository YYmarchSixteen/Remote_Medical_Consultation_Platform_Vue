{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      userList: [],\n      currentUserId: null,\n      currentUsername: '',\n      currentChat: [],\n      newMessage: ''\n    };\n  },\n\n  mounted() {\n    this.fetchUserList();\n  },\n\n  methods: {\n    fetchUserList() {\n      axios.get(`http://localhost:8081/chat/doctorChatList?doctorId=13`).then(response => {\n        this.userList = response.data.data;\n      }).catch(error => {\n        console.error('Error fetching user list:', error);\n      });\n    },\n\n    handleUserSelect(userId) {\n      this.currentUserId = userId;\n      this.fetchChatContent(userId);\n      console.log(this.currentUserId);\n      console.log(this.currentUserId);\n      console.log(this.currentUserId);\n      console.log(this.currentUserId);\n    },\n\n    fetchChatContent(userId) {\n      axios.get(`http://localhost:8081/chat/all?doctorId=13&userId=${userId}`).then(response => {\n        this.currentUsername = response.data.data.username;\n        this.currentChat = response.data.data.chat;\n      }).catch(error => {\n        console.error('Error fetching chat content:', error);\n      });\n    },\n\n    sendMessage() {\n      if (this.newMessage.trim() !== '') {\n        // 发送消息逻辑\n        this.newMessage = '';\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6BA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,cAAQ,EAAE,EADL;AAELC,mBAAa,EAAE,IAFV;AAGLC,qBAAe,EAAE,EAHZ;AAILC,iBAAW,EAAE,EAJR;AAKLC,gBAAU,EAAE;AALP,KAAP;AAOD,GATY;;AAUbC,SAAO,GAAG;AACR,SAAKC,aAAL;AACD,GAZY;;AAabC,SAAO,EAAE;AACPD,iBAAa,GAAG;AACdR,WAAK,CAACU,GAAN,CAAW,uDAAX,EACGC,IADH,CACQC,QAAO,IAAK;AAChB,aAAKV,QAAL,GAAgBU,QAAQ,CAACX,IAAT,CAAcA,IAA9B;AACD,OAHH,EAIGY,KAJH,CAISC,KAAI,IAAK;AACdC,eAAO,CAACD,KAAR,CAAc,2BAAd,EAA2CA,KAA3C;AACD,OANH;AAOD,KATM;;AAUPE,oBAAgB,CAACC,MAAD,EAAS;AACvB,WAAKd,aAAL,GAAqBc,MAArB;AACA,WAAKC,gBAAL,CAAsBD,MAAtB;AACAF,aAAO,CAACI,GAAR,CAAY,KAAKhB,aAAjB;AACAY,aAAO,CAACI,GAA