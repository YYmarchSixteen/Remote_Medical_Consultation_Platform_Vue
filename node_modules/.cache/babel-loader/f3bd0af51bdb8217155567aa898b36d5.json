{"ast":null,"code":"import { get } from '@/utils/request';\nimport { ElMessage } from 'element-plus';\nimport { ref, reactive, watch } from 'vue';\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 7,\n      //默认一页显示 条\n      dialogVisible: false,\n      dialogVisible1: false,\n      info2: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      list: [],\n      adg: [],\n      dialogVisibleupdate: false,\n      updateid: 0,\n      medicineTypes: [],\n      medicineName: ''\n    };\n  },\n\n  setup() {\n    const currentPage = ref(1);\n    const pagesize = ref(7);\n    const medicineName = ref('');\n    const filteredData = ref([]);\n    const medicineTypes = ref([]);\n    const uploadRef = ref(null);\n\n    const handleSubmitUpload = () => {\n      uploadRef.value.submit();\n    };\n\n    const handleSuccess = () => {\n      setTimeout(() => {\n        window.location.reload();\n      }, 1000); // 延迟1秒刷新页面\n    };\n\n    const handleUploadAndRefresh = () => {\n      handleSubmitUpload();\n      handleSuccess();\n    };\n\n    return {\n      uploadRef,\n      handleUploadAndRefresh\n    }; // 获取药品类型列表\n\n    get('http://localhost:8080/allMedicineType').then(res => {\n      console.log(res);\n      medicineTypes.value = res.data.data;\n      console.log(medicineTypes.value);\n    });\n\n    const searchMedicine = () => {\n      get(`http://localhost:8080/searchMedicine?medicineName=${medicineName.value}`).then(res => {\n        console.log(res);\n        filteredData.value = res.data.data;\n      });\n    };\n\n    watch([currentPage, pagesize, filteredData], () => {\n      // 数据变化时重新渲染\n      // 可以在这里执行一些其他操作，如更新分页等\n      console.log('Data changed');\n    });\n\n    const handleSizeChange = size => {\n      // 一页显示多少条\n      pagesize.value = size;\n    };\n\n    const handleCurrentChange = page => {\n      // 页码更改方法\n      currentPage.value = page;\n    };\n\n    return {\n      medicineName,\n      filteredData,\n      medicineTypes,\n      searchMedicine,\n      handleSizeChange,\n      handleCurrentChange,\n      currentPage,\n      pagesize\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    // 获取药品类型列表\n    get('http://localhost:8080/allMedicineType').then(res => {\n      console.log(res);\n      this.medicineTypes = res.data.data;\n      console.log(this.medicineTypes);\n    });\n    get('http://localhost:8080/allMedicine').then(res => {\n      console.log(res);\n      this.list = res.data.data;\n    });\n  },\n\n  methods: {\n    submitUpload() {\n      this.$refs.upload.submit();\n    },\n\n    formatText(text, limit) {\n      // 自定义函数，接收文本和限制长度作为参数\n      if (text && text.length > limit) {\n        // 判断文本是否需要截取\n        return text.slice(0, limit) + '...'; // 截取并添加省略号\n      }\n\n      return text; // 不需要截取则直接返回原文本\n    },\n\n    del(data) {\n      console.log(data.row.medicineId);\n      get('http://localhost:8080/deleteMedicine', {\n        medicineId: data.row.medicineId\n      }).then(() => {\n        ElMessage({\n          message: '删除成功！',\n          type: 'success'\n        });\n      });\n      window.location.reload(); //刷新\n    },\n\n    add() {\n      this.dialogVisible1 = !this.dialogVisible1;\n    },\n\n    handleClick(data) {\n      this.info2 = data.row;\n      this.dialogVisible = !this.dialogVisible;\n    },\n\n    searchMedicine() {\n      get(`http://localhost:8080/searchMedicine?medicineName=${this.medicineName}`).then(res => {\n        console.log(res);\n        this.list = res.data.data;\n      });\n    },\n\n    handleSizeChange: function (size) {\n      //一页显示多少条\n      this.pagesize = size;\n    },\n    handleCurrentChange: function (currentPage) {\n      //页码更改方法\n      this.currentPage = currentPage;\n    }\n  }\n};","map":{"version":3,"mappings":"AAuIA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,cAA1B;AACA,SAASC,GAAT,EAAcC,QAAd,EAAwBC,KAAxB,QAAqC,KAArC;AACA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,CADR;AACW;AAChBC,cAAQ,EAAE,CAFL;AAEQ;