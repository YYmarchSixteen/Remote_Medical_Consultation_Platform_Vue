01\\src\\views\\Admin\\equipment.vue"],"sourcesContent":["<template>\r\n  <el-card class=\"box-card\">\r\n    <el-button type=\"primary\" @click=\"add(props)\" class=\"box-card1\">添加器械</el-button>\r\n    <el-table :data=\"list.slice((currentPage - 1) * pagesize, currentPage * pagesize)\" style=\"width: 100%\">\r\n    <el-table-column prop=\"id\" label=\"ID\" width=\"200\" />\r\n    <el-table-column prop=\"equipmentname\" label=\"器械名\" width=\"150\" />\r\n    <el-table-column prop=\"feature\" label=\"特点\" width=\"150\" />\r\n    <el-table-column prop=\"material\" label=\"材质成分\" width=\"150\" />\r\n    <el-table-column prop=\"price\" label=\"价格\" width=\"150\" />\r\n    <el-table-column prop=\"type\" label=\"所属类型\" width=\"150\" />\r\n    <el-table-column prop=\"picture\" label=\"图片\" width=\"300\" >\r\n    <template #default=\"scope\">\r\n       <el-image  style=\"width: 70px; height: 70px\" :src=\"scope.row.picture\" alt=\"\" :fit=\"fill\" ></el-image>\r\n    </template>\r\n  </el-table-column>\r\n    <el-table>\r\n</el-table>\r\n    <el-table-column fixed=\"right\" label=\"操作\" width=\"240\">\r\n      <template #default=\"row\">\r\n        <el-button type=\"success\" @click=\"handleClick(row)\">编辑</el-button>\r\n        <el-button type=\"danger\" @click=\"del(row)\">删除</el-button>\r\n      </template>\r\n    </el-table-column>\r\n  </el-table>\r\n\r\n<!-- 添加框 -->\r\n  <el-dialog v-model=\"dialogVisible1\" title=\"增加\" width=\"30%\">\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"器械名\">\r\n        <el-input v-model=\"info1.equipmentname\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"特点\">\r\n        <el-input v-model=\"info1.feature\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"材质成分\">\r\n        <el-input v-model=\"info1.material\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"价格\">\r\n        <el-input v-model=\"info1.price\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"所属类型\">\r\n        <el-input v-model=\"info1.type\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"图片\">\r\n        <el-input v-model=\"info1.picture\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible1 = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"insert()\" v-model=\"dialogVisible1\"\r\n          >确认</el-button\r\n        >\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n\r\n\r\n  <!-- 修改框 -->\r\n  <el-dialog v-model=\"dialogVisible\" title=\"编辑\" width=\"30%\">\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"器械名\">\r\n        <el-input v-model=\"info2.equipmentname\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"特点\">\r\n        <el-input v-model=\"info2.feature\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"材质成分\">\r\n        <el-input v-model=\"info2.material\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"价格\">\r\n        <el-input v-model=\"info2.price\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"所属类型\">\r\n        <el-input v-model=\"info2.type\" />\r\n      </el-form-item>\r\n    </el-form>\r\n<el-form-item label=\"图片\">\r\n  <el-upload\r\n    action=\"http://localhost:8080/upload\"\r\n    :headers=\"{ 'X-CSRF-Token': csrfToken }\"\r\n    :data=\"{ authenticity_token: csrfToken }\"\r\n    :show-file-list=\"true\"\r\n    :on-change=\"handleUpload\"\r\n    :before-upload=\"beforeUpload\">\r\n    <el-button>点击上传</el-button>\r\n    <div slot=\"tip\" class=\"el-upload__tip\">只能上传jpg/png文件，且不超过2MB</div>\r\n  </el-upload>\r\n  <el-image\r\n    v-if=\"info2.picture\"\r\n    style=\"width: 70px; height: 70px\"\r\n    :src=\"info2.picture\"\r\n    alt=\"\"\r\n    :fit=\"fill\">\r\n  </el-image>\r\n</el-form-item>\r\n\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"edit()\" v-model=\"dialogVisible\">确认</el-button>\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n\r\n\r\n  <el-pagination\r\n    currentPage=\"currentPage\"\r\n    :page-sizes=\"[10]\"\r\n    page-size=\"10\"\r\n    :background=\"background\"\r\n    layout=\"total, sizes, prev, pager, next, jumper\"\r\n    :total=\"list.length\"\r\n    @size-change=\"handleSizeChange\"\r\n    @current-change=\"handleCurrentChange\"\r\n  />\r\n  </el-card>\r\n \r\n</template>\r\n<script>\r\nimport { get } from '@/utils/request'\r\nimport { ElMessage } from 'element-plus'\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentPage: 1, //默认页码为1\r\n      pagesize: 15, //默认一页显示 条\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      info2: {}, //修改数据\r\n      info1: {}, //新增数据\r\n      list: [],\r\n      adg: [],\r\n    }\r\n  },\r\n  //初始化工作\r\n  mounted() {\r\n    get('http://localhost:8080/equipmentList').then((res) => {\r\n      console.log(res)\r\n      this.list = res.data\r\n    })\r\n  },\r\n  methods: {\r\n    beforeUpload() {\r\n    // 配置上传参数\r\n    const csrfToken = document.querySelector('meta[name=csrf-token]').getAttribute('content')\r\n    this.$refs.upload.headers['X-CSRF-Token'] = csrfToken\r\n    this.$refs.upload.data = {\r\n      authenticity_token: csrfToken // 如果您使用的是 Rails 等后端框架，默认情况下需要传递一个名为 authenticity_token 的参数来防御 CSRF 攻击\r\n    }\r\n  },\r\n  handleUpload(res) {\r\n  // 获取上传成功后的文件地址\r\n  const { data } = res.response\r\n  if (data.code === 200) {\r\n    // 上传成功\r\n    this.info2.picture = data.data\r\n  } else {\r\n    // 上传失败，提示用户\r\n    this.$message.error('上传失败，请重试')\r\n  }\r\n},\r\n    del(data) {\r\n      console.log(data.row.id)\r\n      get('http://localhost:8080/deleteEquipment', { id: data.row.id }).then(() => {\r\n        ElMessage({\r\n          message: '删除成功！',\r\n          type: 'success'\r\n        })\r\n      })\r\n      window.location.reload() //刷新\r\n    },\r\n    add() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n    },\r\n    insert() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n      get('http://localhost:8080/insertEquipment', this.info1).then((res) => {\r\n        console.log(res)\r\n        if (res.data == '添加成功') {\r\n          ElMessage({\r\n            message: '添加成功',\r\n            type: 'success'\r\n          })\r\n          window.location.reload()\r\n        } else {\r\n          ElMessage({\r\n            message: '添加失败',\r\n            type: 'danger'\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleClick(data) {\r\n      this.info2 = data.row\r\n      this.dialogVisible = !this.dialogVisible\r\n    },\r\n    edit() {\r\n      \r\n      get('http://localhost:8080/updateEquipment', this.info2).then((res) => {\r\n        console.log(res)\r\n        if (res.data == '修改成功') {\r\n          ElMessage({\r\n            message: '修改成功',\r\n            type: 'success'\r\n          })\r\n          window.location.reload()\r\n        } else {\r\n          ElMessage({\r\n            message: '修改失败',\r\n            type: 'danger'\r\n          })\r\n        }\r\n      })\r\n    },\r\n\r\n    handleSizeChange: function (size) {\r\n      //一页显示多少条\r\n      this.pagesize = size\r\n    },\r\n    handleCurrentChange: function (currentPage) {\r\n      //页码更改方法\r\n      this.currentPage = currentPage\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.box-card {\r\n  margin-top: 5px;\r\n  margin-left: 15px;\r\n  margin-right: 15px;\r\n  height: 95%;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}