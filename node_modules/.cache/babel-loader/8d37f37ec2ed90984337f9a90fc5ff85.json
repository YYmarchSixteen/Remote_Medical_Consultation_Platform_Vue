{"ast":null,"code":"import { get } from '@/utils/request';\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 15,\n      //默认一页显示 条\n      dialogVisible: false,\n      dialogVisible1: false,\n      info2: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      list: [],\n      adg: []\n    };\n  },\n\n  //初始化工作\n  mounted() {\n    get('http://localhost:8080/equipmentList').then(res => {\n      console.log(res);\n      this.list = res.data;\n    });\n  },\n\n  methods: {\n    beforeUpload() {\n      // 配置上传参数\n      const csrfToken = document.querySelector('meta[name=csrf-token]').getAttribute('content');\n      this.$refs.upload.headers['X-CSRF-Token'] = csrfToken;\n      this.$refs.upload.data = {\n        authenticity_token: csrfToken // 如果您使用的是 Rails 等后端框架，默认情况下需要传递一个名为 authenticity_token 的参数来防御 CSRF 攻击\n\n      };\n    },\n\n    handleUpload(res) {\n      // 获取上传成功后的文件地址\n      const {\n        data\n      } = res.response;\n\n      if (data.code === 200) {\n        // 上传成功\n        this.info2.picture = data.data;\n      } else {\n        // 上传失败，提示用户\n        this.$message.error('上传失败，请重试');\n      }\n    },\n\n    del(data) {\n      console.log(data.row.id);\n      get('http://localhost:8080/deleteEquipment', {\n        id: data.row.id\n      }).then(() => {\n        ElMessage({\n          message: '删除成功！',\n          type: 'success'\n        });\n      });\n      window.location.reload(); //刷新\n    },\n\n    add() {\n      this.dialogVisible1 = !this.dialogVisible1;\n    },\n\n    insert() {\n      this.dialogVisible1 = !this.dialogVisible1;\n      get('http://localhost:8080/insertEquipment', this.info1).then(res => {\n        console.log(res);\n\n        if (res.data == '添加成功') {\n          ElMessage({\n            message: '添加成功',\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '添加失败',\n            type: 'danger'\n          });\n        }\n      });\n    },\n\n    handleClick(data) {\n      this.info2 = data.row;\n      this.dialogVisible = !this.dialogVisible;\n    },\n\n    edit() {\n      get('http://localhost:8080/updateEquipment', this.info2).then(res => {\n        console.log(res);\n\n        if (res.data == '修改成功') {\n          ElMessage({\n            message: '修改成功',\n            type: 'success'\n          });\n          window.location.reload();\n        } else {\n          ElMessage({\n            message: '修改失败',\n            type: 'danger'\n          });\n        }\n      });\n    },\n\n    handleSizeChange: function (size) {\n      //一页显示多少条\n      this.pagesize = size;\n    },\n    handleCurrentChange: function (currentPage) {\n      //页码更改方法\n      this.currentPage = currentPage;\n    }\n  }\n};","map":{"version":3,"mappings":"AA0IA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,cAA1B;AACA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,CADR;AACW;AAChBC,cAAQ,EAAE,EAFL;AAES;AACdC,mBAAa,EAAE,KAHV;AAILC,oBAAc,EAAE,KAJX;AAKLC,WAAK,EAAE,EALF;AAKM;AACXC,WAAK,EAAE,EANF;AAMM;AACXC,UAAI,EAAE,EAPD;AAQLC,SAAG,EAAE;AARA,KAAP;AAUD,GAZY;;AAab;AACAC,SAAO,GAAG;AACRX,OAAG,CAAC,qCAAD,CAAH,CAA2CY,IAA3C,CAAiDC,GAAD,IAAS;AACvDC,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,WAAKJ,IAAL,GAAYI,GAAG,CAACX,IAAhB;AACD,KAHD;AAID,GAnBY;;AAoBbc,SAAO,EAAE;AACPC,gBAAY,GAAG;AACf;AACA,YAAMC,SAAQ,GAAIC,QAAQ,CAACC,aAAT,CAAuB,uBAAvB,EAAgDC,YAAhD,CAA6D,SAA7D,CAAlB;AACA,WAAKC,KAAL,CAAWC,MAAX,CAAkBC,OAAlB,CAA0B,cAA1B,IAA4CN,SAA5C;AACA,WAAKI,KAAL,CAAWC,MAAX,CAAkBrB,IAAlB,GAAyB;AACvBuB,0BAAkB,EAAEP,SADG,CACO;;AADP,OAAzB;AAGD,KARQ;;AASTQ,gBAAY,CAACb,GAAD,EAAM;AAClB;AACA,YAAM;AAAEX;AAAF,UAAWW,GAAG,CAACc,QAArB;;AACA,UAAIzB,IAAI,CAAC0B,IAAL,KAAc,GAAlB,EAAuB;AACrB;AACA,aAAKrB,KAAL,CAAWsB,OAAX,GAAqB3B,IAAI,CAACA,IAA1B;AACF,OAHA,MAGO;AACL;AACA,aAAK4B,QAAL,CAAcC,KAAd,CAAoB,UAApB;AACF;AACD,KAnBU;;AAoBPC,OAAG,CAAC9B,IAAD,EAAO;AACRY,aAAO,CAACC,GAAR,CAAYb,IAAI,CAAC+B,GAAL,CAASC,EAArB;A