ist","mounted","then","res","console","log","methods","handleSizeChange","size","handleCurrentChange","handleClick","row","del","departmentId","message","type","window","location","reload","edit","dt","Date","createTime","year","getFullYear","month","getMonth","date","getDate","add","insert","departmentName","level","add2","insert2"],"sourceRoot":"","sources":["D:\\小学期vscode\\demo01\\src\\views\\Department\\demo05.vue"],"sourcesContent":["<template>\r\n  <el-button type=\"primary\" @click=\"add()\">添加</el-button>\r\n  <el-table\r\n    :data=\"tbData.slice((currentPage - 1) * pagesize, currentPage * pagesize)\"\r\n    style=\"width: 100%; font-size: 1.3em\"\r\n    :row-style=\"{ height: '55px' }\"\r\n    show-label=\"false\"\r\n  >\r\n    <el-table-column type=\"expand\" style=\"left: 20px\">\r\n      <template #default=\"row\">\r\n        <!-- <template > -->\r\n        <el-button\r\n          type=\"primary\"\r\n          @click=\"add2()\"\r\n          style=\"margin-left: 47px\"\r\n          round\r\n          >添加</el-button\r\n        >\r\n        <el-table\r\n          :data=\"row.row.children\"\r\n          style=\"width: 100%; margin-left: 40px\"\r\n        >\r\n          <el-table-column prop=\"departmentName\" label=\"科室\" width=\"500\" />\r\n          <!-- <el-table-column prop=\"createTime\" label=\"日期\" width=\"300\" /> -->\r\n          <el-table-column fixed=\"right\" label=\"操作\" width=\"200\">\r\n            <template #default=\"row\">\r\n              <el-button type=\"success\" @click=\"handleClick(row)\" round>\r\n                编辑\r\n              </el-button>\r\n              <!-- 删除确认框 -->\r\n              <el-popconfirm title=\"确定要删除吗?\" @confirm=\"del(row)\">\r\n                <template #reference>\r\n                  <el-button type=\"danger\" round>删除</el-button>\r\n                </template>\r\n              </el-popconfirm>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </template>\r\n    </el-table-column>\r\n    <el-table-column prop=\"departmentName\" label=\"科室\"></el-table-column>\r\n  </el-table>\r\n  <el-dialog v-model=\"dialogVisible1\" title=\"增加一级科室\" width=\"30%\">\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"科室\">\r\n        <el-input v-model=\"info1.departmentName\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <!-- <el-form label-width=\"120px\">\r\n      <el-form-item label=\"级别\">\r\n        <el-input v-model=\"info1.level\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"上级科室\">\r\n        <el-input v-model=\"info1.parentId\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"日期\">\r\n        <el-input v-model=\"info1.createTime\" />\r\n      </el-form-item>\r\n    </el-form> -->\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible1 = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"insert()\" v-model=\"dialogVisible1\"\r\n          >确认</el-button\r\n        >\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n  <el-dialog v-model=\"dialogVisible\" title=\"编辑\" width=\"30%\">\r\n    <!-- <el-form label-width=\"120px\">\r\n      <el-form-item label=\"科室ID\">\r\n        <el-input v-model=\"info.departmentId\" />\r\n      </el-form-item>\r\n    </el-form> -->\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"科室\">\r\n        <el-input v-model=\"info.departmentName\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"级别\">\r\n        <el-input v-model=\"info.level\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"地址\">\r\n        <el-input v-model=\"info.adress\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"上级科室\">\r\n        <el-input v-model=\"info.parentId\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"日期\">\r\n        <el-input v-model=\"info.createTime\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"edit()\" v-model=\"dialogVisible\"\r\n          >确认</el-button\r\n        >\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n  <el-dialog v-model=\"dialogVisible2\" title=\"新增二级科室\" width=\"30%\">\r\n    <el-form label-width=\"120px\">\r\n      <el-form-item label=\"科室\">\r\n        <el-input v-model=\"form.departmentName\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible2 = false\">取消</el-button>\r\n        <el-button\r\n          type=\"primary\"\r\n          @click=\"insert2(row)\"\r\n          v-model=\"dialogVisible2\"\r\n          >确认</el-button\r\n        >\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n  <el-pagination\r\n    currentPage=\"currentPage4\"\r\n    :page-sizes=\"[5, 8, 10, 12]\"\r\n    page-size=\"8\"\r\n    :background=\"background\"\r\n    layout=\"total, sizes, prev, pager, next, jumper\"\r\n    :total=\"tbData.length\"\r\n    @size-change=\"handleSizeChange\"\r\n    @current-change=\"handleCurrentChange\"\r\n  />\r\n</template>\r\n\r\n\r\n\r\n<script>\r\nimport { get } from '@/utils/request'\r\nimport { ElMessage } from 'element-plus'\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentPage: 1, //默认页码为1\r\n      pagesize: 8, //默认一页显示10条\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      dialogVisible2: false,\r\n      info: {}, //修改数据\r\n      info1: {}, //新增数据\r\n      tbData: [],\r\n      form: [],\r\n      list:[]\r\n    }\r\n  },\r\n  mounted() {\r\n    get('/getAllDepartments').then((res) => {\r\n      console.log(res)\r\n      this.tbData = res.data\r\n    })\r\n  },\r\n  methods: {\r\n    handleSizeChange: function (size) {\r\n      //一页显示多少条\r\n\r\n      this.pagesize = size\r\n    },\r\n\r\n    handleCurrentChange: function (currentPage) {\r\n      //页码更改方法\r\n\r\n      this.currentPage = currentPage\r\n    },\r\n    handleClick(data) {\r\n      this.info = data.row\r\n      this.dialogVisible = !this.dialogVisible\r\n    },\r\n    del(data) {\r\n      console.log(data.row.departmentId)\r\n      get('/deleteDepartment', { departmentId: data.row.departmentId }).then(\r\n        (res) => {\r\n          ElMessage({\r\n            message: '删除成功！',\r\n            type: 'success'\r\n          })\r\n        }\r\n      )\r\n      window.location.reload() //刷新\r\n    },\r\n    edit() {\r\n      this.dialogVisible = !this.dialogVisible //马上消失\r\n      //作用是调整下createTime的格式\r\n      var dt = new Date(this.info.createTime)\r\n      let year = dt.getFullYear()\r\n      let month = dt.getMonth() + 1\r\n      let date = dt.getDate()\r\n      this.info.createTime = year + '/' + month + '/' + date\r\n      get('/updateDepartment', this.info).then((res) => {\r\n        ElMessage({\r\n          message: '修改成功！',\r\n          type: 'success'\r\n        })\r\n        window.location.reload()\r\n      })\r\n    },\r\n    add() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n    },\r\n    // insert() {\r\n    //   this.dialogVisible1 = !this.dialogVisible1\r\n    //   get('/addDepartment', {\r\n    //     departmentName: this.props.departmentName,\r\n    //     level: '1',\r\n    //   }).then((res) => {\r\n    //     console.log('==============================')\r\n    //     ElMessage({\r\n    //       message: '添加成功',\r\n    //       type: 'success'\r\n    //     })\r\n    //     // 刷新\r\n    //     window.location.reload()\r\n    //     ElMessage({\r\n    //       message: '添加失败',\r\n    //       type: 'danger'\r\n    //     })\r\n    //   })\r\n    // }\r\n    insert() {\r\n        this.dialogVisible1 = !this.dialogVisible1\r\n      get('/addDepartment', {\r\n        departmentName: this.info1.departmentName,\r\n        level: '1'\r\n      }).then((res) => {\r\n        ElMessage({\r\n          message: '添加成功！',\r\n          type: 'success'\r\n        })\r\n        window.location.reload()\r\n      })\r\n    },\r\n    add2() {\r\n      this.dialogVisible2 = !this.dialogVisible2\r\n    },\r\n    insert2(row) {\r\n       console.log('========================')\r\n       console.log(row)\r\n      get('/addDepartment', {\r\n        departmentName: this.form.departmentName,\r\n        level: '2',\r\n        //parentId: row.row.departmentId\r\n      }).then((res) => {\r\n        ElMessage({\r\n          message: '添加成功！',\r\n          type: 'success'\r\n        })\r\n        //window.location.reload()\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang='scss' scoped>\r\n.demo-table-expand {\r\n  font-size: 0;\r\n}\r\n.demo-table-expand label {\r\n  width: 0px;\r\n  color: #99a9bf;\r\n}\r\n.demo-table-expand .el-form-item {\r\n  margin-right: 0;\r\n  margin-bottom: 0;\r\n  width: 50%;\r\n}\r\n.el-table\r\n  .el-table--fit\r\n  .el-table--enable-row-hover\r\n  .el-table--enable-row-transition {\r\n  margin-left: 47px !important;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}