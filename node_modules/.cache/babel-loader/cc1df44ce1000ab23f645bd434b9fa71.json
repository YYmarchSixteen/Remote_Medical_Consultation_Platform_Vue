{"ast":null,"code":"import { get } from '@/utils/request';\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      //默认页码为1\n      pagesize: 8,\n      //默认一页显示10条\n      dialogVisible: false,\n      dialogVisible1: false,\n      dialogVisible2: false,\n      info: {},\n      //修改数据\n      info1: {},\n      //新增数据\n      tbData: [],\n      form: [],\n      list: []\n    };\n  },\n\n  mounted() {\n    get('/getAllDepartments').then(res => {\n      console.log(res);\n      this.tbData = res.data;\n    });\n  },\n\n  methods: {\n    handleSizeChange: function (size) {\n      //一页显示多少条\n      this.pagesize = size;\n    },\n    handleCurrentChange: function (currentPage) {\n      //页码更改方法\n      this.currentPage = currentPage;\n    },\n\n    handleClick(data) {\n      this.info = data.row;\n      this.dialogVisible = !this.dialogVisible;\n    },\n\n    del(data) {\n      console.log(data.row.departmentId);\n      get('/deleteDepartment', {\n        departmentId: data.row.departmentId\n      }).then(res => {\n        ElMessage({\n          message: '删除成功！',\n          type: 'success'\n        });\n      });\n      window.location.reload(); //刷新\n    },\n\n    edit() {\n      this.dialogVisible = !this.dialogVisible; //马上消失\n      //作用是调整下createTime的格式\n\n      var dt = new Date(this.info.createTime);\n      let year = dt.getFullYear();\n      let month = dt.getMonth() + 1;\n      let date = dt.getDate();\n      this.info.createTime = year + '/' + month + '/' + date;\n      get('/updateDepartment', this.info).then(res => {\n        ElMessage({\n          message: '修改成功！',\n          type: 'success'\n        });\n        window.location.reload();\n      });\n    },\n\n    add() {\n      this.dialogVisible1 = !this.dialogVisible1;\n    },\n\n    // insert() {\n    //   this.dialogVisible1 = !this.dialogVisible1\n    //   get('/addDepartment', {\n    //     departmentName: this.props.departmentName,\n    //     level: '1',\n    //   }).then((res) => {\n    //     console.log('==============================')\n    //     ElMessage({\n    //       message: '添加成功',\n    //       type: 'success'\n    //     })\n    //     // 刷新\n    //     window.location.reload()\n    //     ElMessage({\n    //       message: '添加失败',\n    //       type: 'danger'\n    //     })\n    //   })\n    // }\n    insert() {\n      this.dialogVisible1 = !this.dialogVisible1;\n      get('/addDepartment', {\n        departmentName: this.info1.departmentName,\n        level: '1'\n      }).then(res => {\n        ElMessage({\n          message: '添加成功！',\n          type: 'success'\n        });\n        window.location.reload();\n      });\n    },\n\n    add2() {\n      this.dialogVisible2 = !this.dialogVisible2;\n    },\n\n    insert2(row) {\n      console.log('========================');\n      console.log(row);\n      get('/addDepartment', {\n        departmentName: this.form.departmentName,\n        level: '2' //parentId: row.row.departmentId\n\n      }).then(res => {\n        ElMessage({\n          message: '添加成功！',\n          type: 'success'\n        }); //window.location.reload()\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiJA,SAASA,GAAT,QAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,cAA1B;AACA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,CADR;AACW;AAChBC,cAAQ,EAAE,CAFL;AAEQ;AACbC,mBAAa,EAAE,KAHV;AAILC,oBAAc,EAAE,KAJX;AAKLC,oBAAc,EAAE,KALX;AAMLC,UAAI,EAAE,EAND;AAMK;AACVC,WAAK,EAAE,EAPF;AAOM;AACXC,YAAM,EAAE,EARH;AASLC,UAAI,EAAE,EATD;AAULC,UAAI,EAAC;AAVA,KAAP;AAYD,GAdY;;AAebC,SAAO,GAAG;AACRb,OAAG,CAAC,oBAAD,CAAH,CAA0Bc,IAA1B,CAAgCC,GAAD,IAAS;AACtCC,aAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,WAAKL,MAAL,GAAcK,GAAG,CAACb,IAAlB;AACD,KAHD;AAID,GApBY;;AAqBbgB,SAAO,EAAE;AACPC,oBAAgB,EAAE,UAAUC,IAAV,EAAgB;AAChC;AAEA,WAAKhB,QAAL,GAAgBgB,IAAhB;AACD,KALM;AAOPC,uBAAmB,EAAE,UAAUlB,WAAV,EAAuB;AAC1C;AAEA,WAAKA,WAAL,GAAmBA,WAAnB;AACD,KAXM;;AAYPmB,eAAW,CAACpB,IAAD,EAAO;AAChB,WAAKM,IAAL,GAAYN,IAAI,CAACqB,GAAjB;AACA,WAAKlB,aAAL,