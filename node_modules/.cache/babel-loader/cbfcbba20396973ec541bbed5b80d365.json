admin/update', params)\r\n        alert('用户信息已更新')\r\n      } catch (error) {\r\n        console.error('Error updating user:', error)\r\n        alert('更新用户信息时出错')\r\n      }\r\n    },\r\n    re(){\r\n      window.location.reload();\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  margin: 20px auto;\r\n  max-width: 800px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}