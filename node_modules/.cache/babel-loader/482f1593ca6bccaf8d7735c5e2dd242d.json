{"ast":null,"code":"import axios from 'axios';\nimport Header from '@/views/userLayout/Header/header.vue';\nimport { ElButton } from 'element-plus';\nexport default {\n  components: {\n    Header,\n    ElButton\n  },\n\n  data() {\n    return {\n      medicines: []\n    };\n  },\n\n  mounted() {\n    this.fetchMedicines();\n  },\n\n  computed: {\n    filteredMedicines() {\n      // 根据药品类型过滤药品\n      return this.medicines.filter(medicine => {\n        // 假设你有一个类型过滤器，例如 filterByType\n        return filterByType(medicine.type); // 使用你自己的过滤器函数\n      });\n    }\n\n  },\n  methods: {\n    fetchMedicines() {\n      axios.get('http://localhost:8081/medicine/all').then(response => {\n        this.medicines = response.data.data;\n      }).catch(error => {\n        console.error('Error fetching medicines:', error);\n      });\n    },\n\n    addToCart(medicine) {\n      console.log('Added to cart:', medicine);\n    },\n\n    // eslint-disable-next-line no-unused-vars\n    detail(medicine) {// Implement the detail method\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,sCAAnB;AACA,SAASC,QAAT,QAAyB,cAAzB;AAEA,eAAe;AACbC,YAAU,EAAE;AAAEF,UAAF;AAAUC;AAAV,GADC;;AAEbE,MAAI,GAAG;AACL,WAAO;AACLC,eAAS,EAAE;AADN,KAAP;AAGD,GANY;;AAObC,SAAO,GAAG;AACR,SAAKC,cAAL;AACD,GATY;;AAUbC,UAAQ,EAAE;AACRC,qBAAiB,GAAG;AAClB;AACA,aAAO,KAAKJ,SAAL,CAAeK,MAAf,CAAsBC,QAAO,IAAK;AACvC;AACA,eAAOC,YAAY,CAACD,QAAQ,CAACE,IAAV,CAAnB,CAFuC,CAEH;AACrC,OAHM,CAAP;AAIF;;AAPQ,GAVG;AAmBbC,SAAO,EAAE;AACPP,kBAAc,GAAG;AACfP,WAAK,CAACe,GAAN,CAAU,oCAAV,EACGC,IADH,CACQC,QAAO,IAAK;AAChB,aAAKZ,SAAL,GAAiBY,QAAQ,CAACb,IAAT,CAAcA,IAA/B;AACD,OAHH,EAIGc,KAJH,CAISC,KAAI,IAAK;AACdC,eAAO,CAACD,KAAR,CAAc,2BAAd,EAA2CA,KAA3C;AACD,OANH;AAOD,KATM;;AAUPE,aAAS,CAACV,QAAD,EAAW;AAClBS,aAAO,CAACE,GAAR,CAAY,gBAAZ,EAA8BX,QAA9B;AACD,KAZM;;AAaP;AACAY,UAAM,CAACZ,QAAD,EAAW,CACf;AACF;;AAhBO;AAnBI,CAAf","names":["axios","Header","ElButton","components","data","medicines","mounted","fetchMedicines","comp