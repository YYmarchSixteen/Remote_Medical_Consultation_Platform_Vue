s.medicines = response.data.data;\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching medicines:', error);\r\n        });\r\n    },\r\n    addToCart(medicine) {\r\n      console.log('Added to cart:', medicine);\r\n    },\r\n    // eslint-disable-next-line no-unused-vars\r\n    detail(medicine) {\r\n      // Implement the detail method\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.all-medicines {\r\n  max-width: 80%;\r\n  margin: 5% auto;\r\n  padding: 20px;\r\n}\r\n\r\n.medicine {\r\n  display: inline-block;\r\n  margin-bottom: 20px;\r\n  width: 19%;\r\n  margin-right: 1%;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n\r\n.medicine:last-child {\r\n  margin-right: 0;\r\n}\r\n\r\n.last-in-row {\r\n  margin-right: 0;\r\n}\r\n\r\n.medicine-image {\r\n  width: 50%;\r\n  height: auto;\r\n}\r\n\r\n.medicine-info {\r\n  padding: 10px;\r\n}\r\n\r\n.medicine-name {\r\n  margin-top: 0;\r\n}\r\n\r\n.medicine-price {\r\n  margin-bottom: 10px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}