.dialogVisible\r\n    },\r\n    del(row) {\r\n      ElMessage({\r\n        message: '删除成功！',\r\n        type: 'success'\r\n      })\r\n    },\r\n    edit() {\r\n      ElMessage({\r\n        message: '修改成功！',\r\n        type: 'success'\r\n      })\r\n      this.dialogVisible = !this.dialogVisible\r\n    },\r\n    add() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n    },\r\n    insert() {\r\n      this.dialogVisible1 = !this.dialogVisible1\r\n      console.log(this.info1)\r\n    }\r\n  }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}